			$(function () {

				var iFrames = $('iframe');

				function iResize(currentIframe) {
					//for (var i = 0, j = iFrames.length; i < j; i++) {
					var a = document.createElement('a');
					a.href = currentIframe.src; // iFrames[i].src;
					if (/*a.href != currentIframe.baseURI && */a.hostname === window.location.hostname)
						currentIframe.style.height = currentIframe.contentWindow.document.body.offsetHeight + 'px';
					//iFrames[i].style.height = iFrames[i].contentWindow.document.body.offsetHeight + 'px';
					//}
				}

				if ($.browser.safari || $.browser.opera) {

					iFrames.load(function () {
						setTimeout(iResize(this), 0);
					});

					for (var i = 0, j = iFrames.length; i < j; i++) {
						if (iFrames[i].src != '') {
							var iSource = iFrames[i].src;
							iFrames[i].src = '';
							iFrames[i].src = iSource;
						}
					}
				} else {
					iFrames.load(function () {
						var a = document.createElement('a');
						a.href = this.src;
						if (a.hostname === window.location.hostname)
							this.style.height = (this.contentWindow.document.body.offsetHeight) + '100px';
					});
				}
			});


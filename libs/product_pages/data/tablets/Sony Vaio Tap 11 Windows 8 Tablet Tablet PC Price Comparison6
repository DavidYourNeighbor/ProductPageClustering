<script type="text/javascript">
//<![CDATA[
	$(document).ready(function () {

		var zoomToolTip = $('<span id="zoom-message">Click to zoom</span>').appendTo("body");

		//, .cloud-zoom-gallery
		$(".cloud-zoom").click(function (e) {
			w = $(this)//,anc=$(this).is(".ancillary");
			e.preventDefault();
			if (zoomToolTip.stayhidden = false) { return }
			//if(anc&&$("#cloud-zoom-big").length){return}
			if (w.attr('href') == "") { return }
			w.CloudZoom();
			if ($(this).is("#zoom1")) {
				zoomToolTip.active = true
			} else {
				if ($(this).is("#zoom2")) {
					zoomToolTip.active = false
				}
			}
			w.unbind("mouseover")
		}).mouseover(function () {
			if ($(this).is("#zoom2")) {
				zoomToolTip.hide()
			} else {
				if (!zoomToolTip.active && !zoomToolTip.stayhidden && $(this).attr('href') != "") {//added last condition to circumvent unavailable super images
					zoomToolTip.show()
					$("body > div#wrapper > div#contentwrapper > div#contentcolumn > form > table.versionPageTable > tbody > tr > td > div.db_images > div.imageBox > div > div.VPSaving-vp").hide();
				} else {
					$(this).css({ cursor: 'default' });
				}
			}
		}).mousemove(function (z) {
			if (!zoomToolTip.active) {
				var x = z.pageY + 10;
				var y = z.pageX + 15;
				zoomToolTip.css({ left: y, top: x })
			}
		}).mouseout(function () {
			zoomToolTip.hide();
			if (zoomToolTip.active) {
				zoomToolTip.active = false;
				$(this).mouseover(function () {
					if (!zoomToolTip.active && !zoomToolTip.stayhidden && $(this).attr('href') != "") {//added last condition to circumvent unavailable super images
						zoomToolTip.show()
					} else {
						$(this).css({ cursor: 'default' });
					}
				})
			}
		})

		//Gallery
		$("div.db_images > div.thumbnails > a > img, div.sc_menu > ul.sc_menu > li > a >img").each(function (index) {
			var thisImage = $(this).click(function () {
				//var thisImageId=thisImage.attr("id"),suffix=thisImageId.split("_").pop();
				//$(".thumbnail-images a").removeClass("zoom-select").addClass("carousel-border carousel-over-border");
				//var anch=thisImage.parent();
				//anch.removeClass();
				//anch.addClass("zoom-select");
				$("#zoom1 img").attr("src", thisImage.attr("standardimagesrc"));
				//$("#zoom1 img").attr("width",thisImage.attr("standardimagewidth"));
				//$("#zoom1 img").attr("height",thisImage.attr("standardimageheight"));
				var bigImgSrc = thisImage.attr("zoomimagesrc");
				if (!bigImgSrc) {
					$("#zoom1").attr("href", "javascript: void(0);")
				} else {
					$("#zoom1").attr("href", bigImgSrc);
				}
				showHideZoomIndicator(bigImgSrc);
				$("#zoom1").attr("href", bigImgSrc);
				//check_big_exists(bigImgSrc)
			})
		});
	});
//]]>
</script>

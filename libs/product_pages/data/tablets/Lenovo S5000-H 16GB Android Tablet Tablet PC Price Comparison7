																function SwitchFrame(FrameID) {

																	var FrameIndex = 1;

																	for (FrameIndex=1;FrameIndex<=4;FrameIndex=FrameIndex+1)
																	{
																		if (FrameIndex != FrameID) {
																			document.getElementById("TF-0" + FrameIndex).style.display='none';
																			document.getElementById("Tab-0" + FrameIndex).className="Tab-Small";
																		} else {
																			document.getElementById("TF-0" + FrameIndex).style.display='block';
																			document.getElementById("Tab-0" + FrameIndex).className="Tab-Small Tab-Selected";
																		}
																	}

																}

																SwitchFrame(1);

															</script></div>
            </div>

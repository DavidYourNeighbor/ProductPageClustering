            <div style="display: none" id="Image-Array" class="Carousel-Container"><script type="text/javascript">

																			// Create the array object

																			var ItemImagesArray=new Array("http://images.esellerpro.com/2451/I/161/477/8/5000.jpg","","","","","","","","","","","","","","");

																			// Create the indexing variable

																			var ItemImagesArrayIndex = 1;

																			// We make sure the first image exists before we execute the displaying of the images

																			if (ItemImagesArray[1] != "") {

																				for (ItemImagesArrayIndex in ItemImagesArray) {

																					if (ItemImagesArray[ItemImagesArrayIndex] != "") {

																						// If we do have additional images, create the image object here

																						if (ItemImagesArrayIndex == 1) {

																							document.getElementById("Image-Array").style.display='block';

																							// document.getElementById("imageIndex1").style.display='none';
																							// document.getElementById("MainEnlargeImage").src=ItemImagesArray[ItemImagesArrayIndex];

																						}

																						document.write("<a href='#ebayphotohostinglisting' onclick='Javascript:SwitchPreviewImage(" + ItemImagesArrayIndex + ")' title='Preview " + ItemImagesArrayIndex + "'><img src='" + ItemImagesArray[ItemImagesArrayIndex] + "' alt='Preview " + ItemImagesArrayIndex + "' width='96' class='Thumbnail-Image' id='imageIndex" + ItemImagesArrayIndex + "' /></a>");

																						// Ok we write the dividers here

																						if (ItemImagesArrayIndex == 5) { document.write("<div class='Image-Gallery-Divider'><!-- No Content --></div>"); }
																						if (ItemImagesArrayIndex == 10) { document.write("<div class='Image-Gallery-Divider'><!-- No Content --></div>"); }

																					}


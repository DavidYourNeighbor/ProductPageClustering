    <script type="text/javascript">

        // PRODUCT/DEFAULT.ASPX

        //var addedFrom = 'ViewProduct';

        dataLayer.push({
            'contentCategory': 'Product View'
        });

        $(document).ready(function() {
            dataLayer.push({
                'productSKU': _MainProductSKU,
                'productTitle': $('.producttitle').text().trim(),
                'manufacturerNumber': $('#codes b').next().text().trim(),
                'brand': _MainProductBrand,
                'numberProductImages': $('#smallimages img').length,
                'freeText': $('#cashbackwrapper').text().trim(),
                'averageCustomerRating': $('.pr-snippet-rating-decimal').text(),
                'productPrice': _MainProductPrice.substring(0, _MainProductPrice.length - 2)
            });

            // More Link Product Detail Page
            $(".pshort a[href='#details']:nth(0)").live('click', function() {
                dataLayer.push({ 'event': 'moreLink' });
            });

            // Product Tab Label
            $('#prod_nav a').live('click', function() {
                dataLayer.push({ 'productTabLabel': $(this).attr('title'), 'event': 'ProductTabLabel' });
            });

            // Add to order checkbox ACCESSORIES TAB
            $("input[id^='ACCchkAddtoOrder']").live('click', function() {
                if ($(this).is(':checked')) {
                    dataLayer.push({ 'addAccessory': 'true' });
                }
            });

            // Add to order checkbox PROMOTIONS TAB
            $("input[id^='chkAddtoOrder']").live('click', function() {
                if ($(this).is(':checked') == true) {
                    dataLayer.push({ 'addPromotion': 'true' });
                }
            });

            // Add to Basket Used Product
            $('.used-product-btn a').live('click', function() {
                dataLayer.push({ 'usedItemClick': $(this).parent().parent().parent().find('.used-product-title a').text() });
            });

            // Read Review
            $("a[id^='pr-snippet-read-link']").live('click', function() {
                dataLayer.push({ 'viewReview': true });
            });

            // Write Review
            $('.pr-snippet-write-review a').live('click', function() {
                dataLayer.push({ 'event': 'writeReview' });
            });
        });
    </script>
    


<script type="text/javascript">
 var currencyDecimalPlaces = '2';
 var currencySymbolLeft = '£';
 var currencySymbolRight = '';
 var baseProductPrice = '199';
 var attributePriceArray = {};function calculateProductPriceWithAttributes() {
    var newPrice = parseFloat(baseProductPrice);
    $('select[id^="attributeId_"]').each(function(index, element) {
        var attributePrice = 0;
        var attributeId = '';
        var optionId = '';
        if (typeof(element.id) != 'undefined') {
            attributeId = element.id;
            if (typeof(element.value) != 'undefined') {
                 optionId = 'optionId_' + element.value;
            }
        }
        if (typeof(attributePriceArray[attributeId][optionId]) != 'undefined') {
            newPrice = parseFloat(newPrice) + parseFloat(attributePriceArray[attributeId][optionId]);
        }
    });
    $('#productPriceCurrent').text(currencySymbolLeft + newPrice.toFixed(currencyDecimalPlaces) + currencySymbolRight);
}
$(document).ready(function() {
    calculateProductPriceWithAttributes();
});
</script>


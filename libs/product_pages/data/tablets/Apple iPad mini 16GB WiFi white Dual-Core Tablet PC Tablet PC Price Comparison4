<script type="text/javascript" language="javascript">

	function llReadCookie(name) {
var nameEQ = name + "=";
var ca = document.cookie.split(';');
for(var i=0;i < ca.length;i++) {
var c = ca[i];
while (c.charAt(0)==' ') c = c.substring(1,c.length);
if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
}
return null;
}

function ExtractUsernameFromCookie(usernameCookie) {
	if(usernameCookie)
	{
		var usernameStart = usernameCookie.indexOf("Name=");
		if(usernameStart > -1)
		{
		    usernameStart = usernameStart + 5;
			usernameEnd = usernameCookie.indexOf("&CID");
			if(usernameEnd > -1)
			{
				return usernameCookie.substring(usernameStart,usernameEnd);
			}
			return usernameCookie.substring(usernameStart);
		}
	}
	return null;
}

function llDeleteCookie(name) {
	document.cookie = name + '=; expires=Thu, 01-Jan-70 00:00:01 GMT;';
}

function llSignOut() {
	llDeleteCookie("PlayUsername");	
	window.location = "https://www.play.com/SignOut.aspx?returnUrl=/OrderHistory.aspx";
}

function llSignUp() {
	llDeleteCookie("PlayUsername");	
	window.location = "https://www.play.com/CreateAccount.aspx?returnUrl=http%3a%2f%2fwww.play.com%2fPC%2fPCs%2f4-%2f38259534%2f-%2fProduct.html%3f_%24ja%3dtsid%253a48045%257ckw%253a2DHapw*g2kI%257ccgn%253a2DHapw.g2kI-02WjrzWFaeDSIoJ2t4gLUQ%26cur%3d258";
}

	function SetLoginLite() {
		//check to see if we have the users cookie
		var username = llReadCookie("PlayUsername");
		username = ExtractUsernameFromCookie(username);
		if(username)
		{	
			username = username.toLowerCase();
			var someHtml = jQuery('#llloggedinhellomessage').html();
			jQuery('#llloggedinhellomessage').replaceWith(someHtml);
			someHtml = jQuery('#llbeforelinkloggedin').html();
			jQuery('#llbeforelinkloggedin').replaceWith(someHtml);
			someHtml = jQuery('#lllinkmsgloggedin').html();
			jQuery('#lllink').html(someHtml);
			someHtml = jQuery('#llafterlinkloggedin').html();
			jQuery('#llafterlinkloggedin').replaceWith(someHtml);
			
			jQuery('#loginlitename').html(username);
			
			jQuery('#lllinkmsgloggedin').remove();
			jQuery('#llbeforelinkloggedout').remove();
			jQuery('#llloggedouthellomessage').remove();
			jQuery('#lllinkmsgloggedout').remove();
			jQuery('#llafterlinkloggedout').remove();
			jQuery('#lllink2').remove();
			jQuery('#llafterlink2loggedout').remove();
			jQuery('#lllink2msgloggedout').remove();
			
			jQuery('#lllink').click(llSignOut);
		}
		else
		{
			var someHtml = jQuery('#llloggedouthellomessage').html();
			jQuery('#llloggedouthellomessage').replaceWith(someHtml);
			someHtml = jQuery('#llbeforelinkloggedout').html();
			jQuery('#llbeforelinkloggedout').replaceWith(someHtml);
			someHtml = jQuery('#lllinkmsgloggedout').html();
			jQuery('#lllink').html(someHtml);
			someHtml = jQuery('#llafterlinkloggedout').html();
			jQuery('#llafterlinkloggedout').replaceWith(someHtml);
			someHtml = jQuery('#lllink2msgloggedout').html();
			jQuery('#lllink2').html(someHtml);
			someHtml = jQuery('#llafterlink2loggedout').html();
			jQuery('#llafterlink2loggedout').replaceWith(someHtml);
			
			jQuery('#lllinkmsgloggedout').remove();
			jQuery('#llbeforelinkloggedin').remove();
			jQuery('#llloggedinhellomessage').remove();
			jQuery('#lllinkmsgloggedin').remove();
			jQuery('#llafterlinkloggedin').remove();
			jQuery('#lllink2msgloggedout').remove();
			jQuery('#llafterlink2loggedout').remove();
			
			jQuery('#lllink').click(llSignOut);
			jQuery('#lllink2').click(llSignUp);
		}
		
	}

	SetLoginLite();

</script><script type="text/javascript" language="javascript">

	function ExtractSuperpointsFromCookie(usernameCookie) {
		if(usernameCookie)
		{
			var superPointStart = usernameCookie.indexOf("SP=");
			var superPointEnd = usernameCookie.indexOf("SPend");
			if(superPointStart > -1  &&  superPointEnd > -1)
			{
				return usernameCookie.substring(superPointStart + 3,superPointEnd);
			}
		}
		return null;
	}

	function SetSuperPoints() {
		//check to see if we have the users cookie
		var username = $mw.GetCookie("PlayUsername");
		superPoints = ExtractSuperpointsFromCookie(username);
		if(superPoints)
		{	
			// replace the loggedin text with just the string value - removes the var tag
			var someHtml = jQuery('#sploggedintext').html();
            jQuery('#sploggedintext').replaceWith(someHtml);
			
			// set the balance text		
			someHtml = jQuery('#spbalance').html();
			jQuery('#spbalance').replaceWith(superPoints);
			
			// hide the logged out text
			jQuery('#sploggedouttext').remove();
		}
		else
		{
			// hide the balance
			jQuery('#spbalance').remove();
			// hide the logged in text
			jQuery('#sploggedintext').remove();
			
			// replace the loggedout text with just the string value - removes the var tag
			var someHtml = jQuery('#sploggedouttext').html();
            jQuery('#sploggedouttext').replaceWith(someHtml);
		}		
	}

	SetSuperPoints();


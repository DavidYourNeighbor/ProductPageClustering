		<script type="text/javascript">
			$(document).ready(function(){
				cookieRawString = unescape(boots.common.cookie.getCookie("_bdccookie_bsktSumm"));
				checkVatCookieValues();
			});
			var cookieKeys = {vd: "vd", ve: "ve"};
			var cookie = {};
			var cookieRawString;
			function checkVatCookieValues(){
				cookie = {};
				cookieRawString = cookieRawString.replace("{", "");
				cookieRawString = cookieRawString.replace("}", "");
				var cookieArray = cookieRawString.split(",");
				for (var i=0; i<cookieArray.length; i++) {
					var s = cookieArray[i];
					var keyAndValue = s.split(":");
					if (keyAndValue.length>1) {
						cookie[keyAndValue[0]]=keyAndValue[1];
					}
				}
			}
		</script>
		
		<script type="text/javascript">
			
			var busy = false;
			$(".Add2ShopCart").click(function() {
				if(boots.cookies.isEnabled()){
					if ($(this).hasClass("freeGift" || "freeGiftColour")) {
						return false;
					}
					if (!busy) {
	
						busy = true;
						$("#OrderItemAddForm").attr({action:window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/OrderItemAdd"});
						$("#OrderItemAddForm")[0].URL.value = "ProductDisplay";
						
						/*
						// TOOK THIS OUT AS IT BREAKS ADDING TO BASKET!
						var url = window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/OrderItemAdd";
						var pageformat = $("input#viewType").val();	
						var category = $("input#cmCategory").val();	
						url += "?pageFormat=" + pageformat + "&cmCategory=" + category);
						*/						
	
						$("#OrderItemAddForm").submit();
						
					}				
				}
				return false;
			});
		
			function Add2ShopCartVAT(catEntryId) {
				if(boots.cookies.isEnabled()){
					if ($(this).hasClass("freeGift" || "freeGiftColour")) {
						return false;
					}
					if (!busy) {
						busy = true;
						checkVatCookieValues();
						if (!cookie['vd'] || !cookie['ve'] || (cookie['vd']=='false' && cookie['ve']=='false')) {
							//ask if VAT exemption should be applied
							var html = [
								'<div id="confirmVATRelief">',
								'<span class="tl"></span>','<span class="tr"></span>','<span class="bl"></span>','<span class="br"></span>',
									'<h2>Will you be claiming the VAT back?</h2>',
									'<div class="actions">',
										'<img id="confirmYes" src="/wcsstore/ConsumerDirectStorefrontAssetStore/en_US/i/checkout/v2/btn_yes.gif" alt="Yes" />',
										'<img id="confirmNo" src="/wcsstore/ConsumerDirectStorefrontAssetStore/en_US/i/checkout/v2/btn_no.gif" alt="No" />',
									'</div>',
									'<p><a href="#" class="vatHelp">Help with VAT relief</a></p>',
								'</div>'
							].join("");
			
							$(".page_productDetails").append(html);
							$(".dialogContent").clone().appendTo("#confirmVATRelief p");
							boots.DialogManager.addDialog({
								opener: "vatHelp",
								id: "help"
							});
			
							var x = $(".pl_addToBasket").position().left + (($(".pl_addToBasket").width() - $("#confirmVATRelief").width())/2);
							var y = $(".pl_addToBasket").position().top + (($(".pl_addToBasket").height() - $("#confirmVATRelief").height())/2);
							$("#confirmVATRelief").css("left",x+"px");
							$("#confirmVATRelief").css("top",y+"px");
							
							$("#confirmVATRelief .actions img").click(function(){				
								if($(this).attr("id")=="confirmYes"){
									$("#OrderItemAddForm")[0].reqVATRelief.value = "1";
									$("#OrderItemAddForm").attr({action:window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/VATOrderItemAdd"});
									reportPageview("Checkout_v2_VAT_pop_up_yes");
								} else if($(this).attr("id")=="confirmNo"){
									$("#OrderItemAddForm")[0].reqVATRelief.value = "0";
									$("#OrderItemAddForm").attr({action:window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/OrderItemAdd"});
									reportPageview("Checkout_v2_VAT_pop_up_no");
								}
								$("#OrderItemAddForm")[0].URL.value = "ProductDisplay";
								$("#OrderItemAddForm").submit();
							});
						} else if (cookie['vd']=='true') {
							$("#OrderItemAddForm")[0].reqVATRelief.value = "1";
							$("#OrderItemAddForm").attr({action:window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/VATOrderItemAdd"});
							$("#OrderItemAddForm")[0].URL.value = "ProductDisplay";
							$("#OrderItemAddForm").submit();
						} else {
							$("#OrderItemAddForm")[0].reqVATRelief.value = "0";
							$("#OrderItemAddForm").attr({action:window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/OrderItemAdd"});
							$("#OrderItemAddForm")[0].URL.value = "ProductDisplay";
							$("#OrderItemAddForm").submit();
						}
					}
				}
			}
		
			function Add2ShopCartVATAuto(catEntryId) {
				if(boots.cookies.isEnabled()){
					if ($(this).hasClass("freeGift" || "freeGiftColour")) {
						return false;
					}
					if (!busy) {
						busy = true;
						$("#OrderItemAddForm")[0].reqVATRelief.value = "1";
						$("#OrderItemAddForm").attr({action:window.location.protocol + "//" + window.location.host + "/webapp/wcs/stores/servlet/VATOrderItemAdd"});
						$("#OrderItemAddForm")[0].URL.value = "ProductDisplay";
						$("#OrderItemAddForm").submit();
					}
				}
			}
				function reportPageview(pagename){
		
			try {
				cmCreatePageviewTag(pagename, "", null, null, "10052");
			} catch (err) {
				//do nothing
			}
		
	}


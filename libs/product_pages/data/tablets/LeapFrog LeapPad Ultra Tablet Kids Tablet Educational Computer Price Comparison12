	function flashRedirectedClickEvent() {

		var href="ProductDisplay?productId=1357189&storeId=10052";		
		var trackName="hasMultipleImages";
		var pageName = "Product Detail page";
		
		// alert("Hello from FlashObject - added for tesing Multiple image click. \n Once tested Pls remove this alert");
		//alert("PDIaV-cmCreateManualLinkClickTag("+href+", "+trackName+","+pageName+" ); ");

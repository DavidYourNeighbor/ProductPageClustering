			function myLogFunction(event){
				var event_array=event.split(";");
				var splitter = event_array[0].split(",");
				if(splitter[0] == "PAGE"){
					boots.product.details.currentZoomImage = splitter[1];
					flashRedirectedClickEvent();
				}
				return;
			}




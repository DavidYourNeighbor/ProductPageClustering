	if (typeof salmon != "object") { var salmon = {}; }
	
	salmon.namespace = {
	        addNamespace: function(namespace) {
	                var parts = namespace.split(".");
	                var root = window;
	                for(var i=0;i<parts.length;i++) {
	                        if(typeof root[parts[i]] != "object") {
	                                root[parts[i]] = {};
	                        }
	                        root = root[parts[i]];
	                }
	        }
	};
	
	salmon.namespace.addNamespace("boots.common");

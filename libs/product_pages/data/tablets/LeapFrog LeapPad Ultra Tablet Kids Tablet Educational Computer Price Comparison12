<script language="javascript">
	function focusTool (url) {
		var w=886; 
		var h=660;
		if (navigator.appName == "Microsoft Internet Explorer") {
			var w=w-4; 
		}
		var winName="3for2app";
		var LeftPosition = (screen.width) ? (screen.width-w)/2 : 0;
		var TopPosition = (screen.height) ? (screen.height-h)/2 : 0;
		var settings ='height='+h+'px,width='+w+'px,top='+TopPosition+'px,left='+LeftPosition+'px,status=0,toolbar=0,location=0,menubar=0,directories=0,resizable=0,scrollbars=0,';
		var oldWin = window.open(url,winName,settings);
		oldWin.focus();
	}
</script>


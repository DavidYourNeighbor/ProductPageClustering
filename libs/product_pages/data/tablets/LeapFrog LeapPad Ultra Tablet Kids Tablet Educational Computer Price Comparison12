<script type="text/javascript">	
/* <![CDATA[ */
function getProductCatFromCookieCache(prodId, defaultCat) {

	// retrieve cookie	
	var coreProdCatCache = {};
	var coreProdCatCacheRawString = unescape(cmcommon.cookie.getCookie("_coreProdCatCache"));
	coreProdCatCacheRawString = coreProdCatCacheRawString.replace("{", "");
	coreProdCatCacheRawString = coreProdCatCacheRawString.replace("}", "");
	var coreProdCatCacheArray = coreProdCatCacheRawString.split(",");
	for (var i=0; i<coreProdCatCacheArray.length; i++) {
		var s = coreProdCatCacheArray[i];
		var keyAndValue = s.split(":");
		if (keyAndValue.length>1) {
			coreProdCatCache[keyAndValue[0]]=keyAndValue[1];
		}
	}
	
	// find product in cache?
	if(coreProdCatCache[prodId]) {
		// return value from cache
		return coreProdCatCache[prodId];	
	}
	else {
		// else save default in cache and return the default
		coreProdCatCache[prodId] = defaultCat;
		
		// save the cookie back
		var coreProdCatCacheString = "{";
		var first = true;
		for (var i in coreProdCatCache) {
			if(!first) {
				coreProdCatCacheString += ",";
			}
			first = false;
			coreProdCatCacheString += i;	
			coreProdCatCacheString += ":";
			coreProdCatCacheString += coreProdCatCache[i];	
		}
		
		coreProdCatCacheString += "}";
		
		cmcommon.cookie.setCookie("_coreProdCatCache", escape(coreProdCatCacheString));		
		
		return defaultCat;
	}

}
cmcommon = {
        cookie: {
                Collection: null,
                init: function() {
                        this.Collection = {};
                        var cookies = document.cookie.split(";");
                        var cookie;
                        for( var i = cookies.length-1; i >= 0 ;i--) {
                                cookie = cookies[i].split("=");
                                var cookieName = cookie[0].replace(/ /g,"");
                                if (cookie.length >= 2) {
                                        this.Collection[cookieName] = cookie[1];
                                }
                        }
                        this.init = function(){ };
                },
                setCookie: function(name,value,days) {
                        this.init();
                        var expires = "";
                        if (days) {
                                var date = new Date();
                                date.setDate(date.getDate() + days);
                                expires = "expires=" + date.toGMTString();
                        }
                        var cookie = name + "=" + value + ";" + expires + ";path=/";
                        document.cookie = cookie;
                        this.Collection[name] = value;
                },
                getCookie: function(name) {
                        this.init();
                        return this.Collection[name];
                },
                removeCookie: function(name) {
                        this.init();
                        this.set(name,"",-1);
                        delete this.Collection[name];
                }
        }
}
/* ]]> */

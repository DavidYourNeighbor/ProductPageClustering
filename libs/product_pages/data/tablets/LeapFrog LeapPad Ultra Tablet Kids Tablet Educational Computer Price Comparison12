		<script type="text/javascript">
			function removeUnwantedPdHtml(){
				
				//this removes it from the related products
				$(".pd_relatedItems .pl_productName a").each(function(){
	                if ($(this).find("span.rvwBrandName").length > 0) {
	                    var brandText = $(this).find("span.rvwBrandName").text();
						var prodText = $(this).find("span.rvwProdName").text();
						prodText = prodText.replace(/\u00a0/g, " ");
						var newRelProdText = "<span class=\"rvwBrandName\">" + brandText + "</span><br />" + "<span class=\"rvwProdName\">" + prodText + "</span>";
						$(this).html(newRelProdText);
                        }
                        else {
                            txt = $(this).text();
                            txt = txt.replace(/\u00a0/g, " ");
                            $(this).empty().append(txt);
                        }                                  
                });
			}
			$(document).ready(function() {
				removeUnwantedPdHtml();
			});
		</script>
	

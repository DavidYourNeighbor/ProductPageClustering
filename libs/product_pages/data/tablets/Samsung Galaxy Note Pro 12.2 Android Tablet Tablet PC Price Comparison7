var curImage=-1;







function swapPicture() {







if (document.images) {







var nextImage=curImage+1;







if (nextImage>=numImages)nextImage=0;







if (dimages[nextImage] && dimages[nextImage].complete) {







var target=0;







if (document.images.myImage)target=document.images.myImage;







if (document.all && document.getElementById("myImage"))target=document.getElementById("myImage");







if (target) {target.src=dimages[nextImage].src;curImage=nextImage;}







setTimeout("swapPicture()", 3000);} else {setTimeout("swapPicture()", 500);}}}







setTimeout("swapPicture()", 3000);







</SCRIPT>

function toggle_bottom_tabs(num) {







for(i = 1; i < 7; i++){







if (i == num) {







document.getElementById('elcell_bmain'+i+'-off').style.display = 'none';







document.getElementById('elcell_bmain'+i+'-on').style.display = '';







document.getElementById('elcell_btn'+i).style.display = '';







} else {







document.getElementById('elcell_bmain'+i+'-off').style.display = '';







document.getElementById('elcell_bmain'+i+'-on').style.display = 'none';







document.getElementById('elcell_btn'+i).style.display = 'none';







}







}







}







</SCRIPT>


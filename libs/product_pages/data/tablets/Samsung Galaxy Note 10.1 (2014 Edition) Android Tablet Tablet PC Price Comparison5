// Reevoo Review case
$(document).ready(function(){
	// Done with inteval

		//$('.review-box').css({'display':'none'});

		var interval = setInterval(function(){

			$('.rightbox').each(function(){

				if($(this).find('div').hasClass('reevoomark_badge'))
				{
					//alert($(this).attr('class') + " == >> " + $(this).find('div').attr('id'));
					if($(this).find('div .reevoomark_badge a:last').hasClass('ask_an_owner'))
					{
						//$(this).find('div .reevoomark_badge').addClass('ask');
						if(document.getElementById('askanowner')) {
							$("#askanowner").css({'display':''});
						}
					}
					//$(this).css({'display':'block'});
					//$(".rightbox").css({'display':'block'});

					if(document.getElementById('review')) {
						$("#review").css({'display':''});
					}
				}
				else {

					$(this).attr({'class':''});
					$(this).find('span').remove();
				}
			});

			clearInterval(interval);

		},2000);


	});

</script>


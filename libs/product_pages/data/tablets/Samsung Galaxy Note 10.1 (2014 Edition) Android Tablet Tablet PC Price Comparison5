				//Toggle Odd/Even Value For Row Display In Multi Color Format
				function isOdd(num) {
					return !( num & 1 );
				}
				//Replace/Clean String As Proper Format
				function preg_quote( str ) {
					return (str+'').replace(/([\\\.\+\*\?\[\^\]\$\(\)\{\}\=\!\<\>\|\:])/g, "\\$1");
				}

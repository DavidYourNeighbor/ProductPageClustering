				// Function for ProductXLoad
				var mycounter = 0; //Counter Set/Initiate With 0 Value
				function loadAutoCompleteProdx(strCnt) {
					if(document.getElementById('txtSearch1').value == 'Enter Search Keyword') {
						document.getElementById('txtSearch1').value = '';
					}

					var strUrl = 'http://www.hispek.com/search.php?doAction=ajaxSearch'; //Set AJAX URL To Call Search Function
					$("#txtSearch1").autocomplete({
						source: function( request, response ) {
							//document.getElementById('search-loader').style.display = "";
							$.ajax({
								url: strUrl,
								data: request,
								dataType: "json",
								type: "POST",
								success: function(data) {
									response(data);
									//document.getElementById('search-loader').style.display = "none";
								}
							});
						},
						minLength: 3,
						select: function (event, ui) {
							event.preventDefault();
							//$(this).val($('<div/>').html(ui.item.value).text());
							//document.location.href = ui.item.url;

							$(this).val($('<div/>').html(ui.item.label).text());
							document.location.href = ui.item.url; //Redirect To PD Case After Click From Ajax List Product Information Dropdown
						}
					});
				}


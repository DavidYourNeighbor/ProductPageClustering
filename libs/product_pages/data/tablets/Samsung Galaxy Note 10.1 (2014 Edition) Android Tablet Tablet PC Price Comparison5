				function submitFrmTopSearch(srcType) {
					var searchValue = '';
					// search
					if (document.getElementById('txtSearch1')) {
						searchValue = document.getElementById('txtSearch1').value;
						if (searchValue == 'Enter Search Keyword') {
							searchValue = '';
						}
						if (searchValue == '') {
							alert("Please enter search keyword.");
							document.getElementById('txtSearch1').focus();
							return;
						}
					}
					objForm = document.frmTopSearch;
					objForm.srcType.value = srcType;
					objForm.submit();
				}

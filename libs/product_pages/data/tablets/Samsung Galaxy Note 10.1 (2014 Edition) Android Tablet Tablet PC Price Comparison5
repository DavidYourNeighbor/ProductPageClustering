				function submitLevel1Form(param,reviewid) {
					objForm = document.frmReview12;


											location.href = "https://www.hispek.com/login.php?revId=" + reviewid + "&helpId=" + param;
					
				}

				function submitLevel0FormAbuse(objForm) {

					if(objForm.txtcomment.value == "") {
						document.getElementById('spnReport').style.display = "";
						objForm.txtcomment.focus();
						return false;
					}
					else {
						document.getElementById('spnReport').style.display = "none";
					}

					objForm.doAction.value = "AddReviewAbuse";
					objForm.submit();
				}

				function submitLevel0Form(objForm) {

					if(objForm.txtTitle.value == "") {
						document.getElementById('titleErr').style.display = "";
						objForm.txtTitle.focus();
						return false;
					}
					else {
						document.getElementById('titleErr').style.display = "none";
					}

					if(objForm.tacomment.value == "") {
						document.getElementById('commentErr').style.display = "";
						objForm.tacomment.focus();
						return false;
					}
					else {
						document.getElementById('commentErr').style.display = "none";
					}

					objForm.submit();
				}

				function submitFrmCart(cnt) {

					subProdId = document.frmCart.subProId.value;
					if(subProdId == 0) {
						alert("Selected combination is not available");
						return false;
					}

					objForm = document.frmCart;

					var regQuantity = /^([0-9]+)$/;

					if(!regQuantity.test(objForm.txtQuantity.value) || objForm.txtQuantity.value == 0) {
						alert("Please enter valid Quantity");
						objForm.txtQuantity.value = 1;
						return false;
					}
					
					objForm.target = "_self";
					if(cnt == '1') {
						objForm.doAction.value = "Cart";
					}
					else if(cnt == '2') {
						objForm.doAction.value = "WishList";
					}
					//This Case For Handle Reserve Item Page Link
					else if(cnt == '3'){
						objForm.doAction.value = "reserveItem";
						objForm.target = "_blank";
						objForm.submit();
					}
					else {
						objForm.doAction.value = "BuyNow";
					}

					objForm.submit();
				}

				function submitFrmCart1(productId,subId,price,qty) {
					objForm = document.frmCart1;
					var regQuantity = /^([0-9]+)$/;
						objForm.doAction.value = "Cart";
						objForm.prodId.value = productId;
						objForm.subProId.value = subId;
						objForm.lastPrice.value = price;
						objForm.submit();
				}

				var mainImgNo = '1';

				// function for swapping images
				function swapImages(objImg, imgNo, height) {

					var rszSubImagePath = objImg.src;
					rszSubImagePath = rszSubImagePath.replace("rszleftrightimages", "rszsubimages");
					rszSubImagePath = rszSubImagePath.replace("/images/design-images/mainimg_img2_th.jpg", "/images/noimages/noimg_detail390x305.jpg");

					// Fetch the image field id
					var objRszImg = document.getElementById("productImage");
					objRszImg.src = rszSubImagePath;
					objRszImg.style.marginTop = height + "px";
					mainImgNo = imgNo;
					var objTemp= document.getElementById("temp");
					objTemp.value = "P";
				}


				function MM_openBrWindow(theURL,winName,features) { //v2.0
				  window.open(theURL,winName,features);
				}

				function openpopup(objImg,prdId,width, height) {

					width = Number(width) +  200;
					height = Number(height) + 350;

					var objSub= document.getElementById("subProId");
					var subProdId = objSub.value;
					var objTemp= document.getElementById("temp");
					if(objTemp.value == "P") {
						imgPath = "http://www.hispek.com/productdetail.php?doAction=imgPopup&prodId=" + prdId + "&" + "imgNo=image" + mainImgNo;
					}
					else {
						imgPath = "http://www.hispek.com/productdetail.php?doAction=imgPopup&prodId=" + prdId + "&" + "imgNo1=image" + "&" + "subProdId=" + subProdId;
					}

					popupSize = "scrollbars=yes,width=" + width + ",height= "  + height + ",left=0,top=0,resizable=yes";

					MM_openBrWindow(imgPath,'enlarge',popupSize);
				}

				function setImage(objImage) {
					mainImgNo = objImage.id;
				}

				 function showCriticReview(prdId,extId,param) {

					if(param == 'A') {
						document.getElementById("expert"+ extId).style.display = "";
						document.getElementById("lable"+ extId).style.display = "none";
						document.getElementById("lable1"+ extId).style.display = "";
						document.getElementById("reviewString"+ extId).style.display = "none";

					}
					else {
						document.getElementById("expert"+ extId).style.display = "none";
						document.getElementById("lable"+ extId).style.display = "";
						document.getElementById("lable1"+ extId).style.display = "none";
						document.getElementById("reviewString"+ extId).style.display = "";
					}
				 }

				function checkAttribute(count) {
					for(key in arrSubId) {
						subId = arrSubId[key];
						var flagReturn = true;
						for(var i = 0; i < count; i++ ) {
							var rowWidth = document.getElementById("lstAttrVal" + i);
							var flag = false;
							for(var i1 = 0; i1 < count; i1++) {
								if(arrAttFinal[subId][i1] == rowWidth.value) {
									flag = true;
								}
							}
							if(flag == false) {
								flagReturn = false;
							}
						}
						if(flagReturn == true) {
							return subId;
						}
					}
					return 0;
				}


				function showhideProd(objname) {
					var obj1 = document.getElementById(objname)
					if (obj1.style.display=="inline" || obj1.style.display=="" )
					{
						obj1.style.display="none"
					}
					else
					{
						obj1.style.display=""
					}
				}

				var objSeparator = '^~_/';

				function changePrice(objScr,count) {
					subId = checkAttribute(count);

					document.frmCart.subProId.value = subId;
					flSubKey = false;

											for (key in arrFinal) {
							if(subId == key) {

								flSubKey = true;

								valueFinal = arrFinal[key];

								arrAttPrice = valueFinal.split(objSeparator);
								var ourPrice = 0;
								var rrpPrice = 0;
								var savePrice = 0;
								var flagVat = arrAttPrice[3];
								var flagAddCart = arrAttPrice[6];

								// product status
								objSpanStockText = document.getElementById("spnStock");
								objAddToCart = document.getElementById("addToCart");
								var wishlist = "";
								if(arrAttPrice[5] > 0 && flagAddCart == 'Y') {
																			objSpanStockText.innerHTML = "In Stock";
										objAddToCart.innerHTML = "<input class=\"btn1 addtocartbtn\" type=\"button\" onclick=\"javascript:submitFrmCart(1);\"/>" + wishlist;
									
								}
								else {
									objSpanStockText.innerHTML = "Out Of Stock";
									objAddToCart.innerHTML = "<input class=\"btn1 addtocartbtn-disable\" type=\"button\"/>" + wishlist;
								}

								// rrp Price
																	rrpPrice = arrAttPrice[1];
									objSpanRrp = document.getElementById("spnrrpprice");
									objDDRrp = document.getElementById("rrpDD");

									if(rrpPrice != 0 && rrpPrice != 0.00) {
										if(objDDRrp) {
											objDDRrp.style.display = "";
										}
										if(objSpanRrp) {
											objSpanRrp.innerHTML = rrpPrice;
										}
									}
									else {
										if(objDDRrp) {
											objDDRrp.style.display = "none";
										}
									}
								
								// ourPrice
								objSpanFix = document.getElementById("spnFixprice");
								ourPrice = arrAttPrice[0];

								// inclusive VAT
								if(flagVat == 'Y') {
									priceWithoutVat = ourPrice;
									objSpanIncVat = document.getElementById("spnInsFixprice");
									if(objSpanIncVat) {
										priceVat = arrAttPrice[2];
										objSpanIncVat.innerHTML = priceVat;
										ourPrice = priceVat;
									}
								}
								else {
									priceWithoutVat = ourPrice;
								}

								// Save Price
																	if(document.getElementById("spnSave")) {
										objSpanSave = document.getElementById("spnSave");
										objTDSave = document.getElementById("saveTD");
										savePrice = arrAttPrice[4];
										if(savePrice != 0 && savePrice != 0.00) {
											if(objTDSave) {
												objTDSave.style.display = "";
											}
											objSpanSave.innerHTML = savePrice;
										}
										else {
											if(objTDSave) {
												objTDSave.style.display = "none";
											}
										}
									}
								
								// lastPrice hidden variable
								document.frmCart.lastPrice.value = ourPrice;

								// ourPrice
								objSpanFix.innerHTML = priceWithoutVat;

								objSpanImage = document.getElementById("productImage");
								imageVal = arrAttPrice[7];

								if(imageVal != "images/noimages/noimg_detail390x305.jpg") {
									objSpanImage.src = 'http://www.hispek.com/' + imageVal;

									objSpanImage.style.marginTop = (246 - (objSpanImage.height))/2 + "px";

									var objTemp= document.getElementById("temp");
									objTemp.value = "S";
								}
							}
						}
									}


				// For images
			var arrImg = new Array();

			arrImg[1] = 'SMP6000ZKABTU-pr17727_pr17727_1.jpg';arrImg[2] = 'SMP6000ZKABTU-2_pr17727_2.jpg';arrImg[3] = 'SMP6000ZKABTU-3_pr17727_3.jpg';arrImg[4] = 'SMP6000ZKABTU-4_pr17727_4.jpg';arrImg[5] = 'SMP6000ZKABTU-pr17727_pr17727_1.jpg';
			function changeRelatedPro(prevNext) {
				Imgcnt = arrImg.length - 1;

				var cnt1 = new Array();
				var objName = new Array();
				var objCnt = new Array();
				var objImg = new Array();

				for(i = 1; i <= 3; i++) {
					objImg[i] = document.getElementById("image" + i);
					objCnt[i] = document.getElementById("hidVarcount" + i);
					cnt1[i] = objCnt[i].value;
				}

				if(prevNext == 'Next') {
					for(i = 1; i <= 3; i++) {
						cnt1[i] = parseInt(cnt1[i], 10) + 1;
						if(cnt1[i] > Imgcnt) cnt1[i] = 1;
					}
				}
				else {
					for(i = 1; i <= 3 ; i++) {
						cnt1[i] = parseInt(cnt1[i], 10) - 1;
						if(cnt1[i] < 1) cnt1[i] = Imgcnt;
					}
				}

				for(i = 1; i <= 3 ; i++) {
					objCnt[i].value = cnt1[i];
					objImg[i].src = "http://www.hispek.com/sysimages/rszleftrightimages/" + arrImg[cnt1[i]];
				}

			}

            //<![CDATA[
            //FOR CHECKING HEIGHTS OF PROMOTIONAL BANNERS IN PRODUCT DETAILS CASE
            $(document).ready(function(){
                if($(".pro-banner-boxbd").find("div").hasClass("pro-bannerbox") && !$(".pro-banner-boxbd").find("div").hasClass("promo-bd"))
                {
                    $(".pro-banner-boxbd").height(85)
					$(".proinfo .probox-bd").css('min-height', '399px')
                }
                if(!$(".pro-banner-boxbd").find("div").hasClass("pro-bannerbox") && $(".pro-banner-boxbd").find("div").hasClass("promo-bd"))
                {
                    $(".pro-banner-boxbd").height(70)
					$(".proinfo .probox-bd").css('min-height', '385px')
                }
				if($(".pro-banner-boxbd").find("div").hasClass("pro-bannerbox") && $(".pro-banner-boxbd").find("div").hasClass("promo-bd"))
                {
					$(".proinfo .probox-bd").css('min-height', '473px')
                }
				if($(".voucher-imagebox").find("div").hasClass("pro-bannerbox") && $(".voucher-imagebox").find("div").hasClass("promo-bd"))
                {
					$(".proinfo .probox-bd").css('min-height', '385px')
                }
            });

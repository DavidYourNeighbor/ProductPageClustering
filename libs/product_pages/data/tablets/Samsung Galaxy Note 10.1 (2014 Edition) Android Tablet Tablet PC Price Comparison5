		<script type="text/javascript" language="javascript" >

			$(document).ready(function() {/*this js is used at sidebar to show hide verticle scroller for shop by producer*/
				$(".shopbyproducer-js").each(function(index, domEle){

					var domHeight=$(domEle).height();
					if(domHeight>600){
					$(".shopbyproducer-js").addClass('sb-vr-scroller-js1');
					$(".vr-scroller2 .scroll-pane").height(600);
					$('.sb-vr-scroller-js1 .scroll-pane').jScrollPane({scrollbarWidth:5, showArrows:false});
					$('.sb-vr-scroller-js1 .scroll-pane').wrap($("<div></div>").attr({'className':'scroll-pane-overflow'}) );
					$(".vr-scroller2 .scroll-pane-overflow").height(600);

					}
					else{
						$(".vr-scroller2 .scroll-pane, .vr-scroller2 .scroll-pane-overflow ").css({height: "auto"});
						}

				});
			});

			$(document).ready(function(){

				$("#slider1 .item-box").each(function(){

					if($(this).find("span").hasClass("openvideopopup"))
					{
						var newsrc = $(this).find("span.openvideopopup iframe.video-object").attr('src') + "?wmode=opaque";
						$(this).find("span.openvideopopup iframe.video-object").attr('src',newsrc);

						$(this).hover(function(){
							$(this).find("span.enlargebtn").css({'display':'block'});
						},function(){
							$(this).find("span.enlargebtn").css({'display':'none'});
						})
					}
				});

				//Examples of how to assign the ColorBox event to elements
				$("#slider1 .item-box .openvideopopup").colorbox({
					inline:true,
					href:"#detail-popup",
					onOpen: function(){

						if($(".pager-active").index() == 0)
						{
							$("#detail-popup").html($('#slider1 .item-box').eq($(".pager-active").index()).html());
						}
						else
						{
							$("#detail-popup").html($('#slider1 .item-box').eq($(".pager-active").index() - 1).html());
						}

						$("#detail-popup .video-object").unwrap();
						$("#detail-popup").find('span').remove();

					}
				});

				$("#slider1 .item-box .openimgpopup").colorbox();

			});
		</script>

<script type="text/javascript">
	function get_XmlHttp() {
		var xmlHttp = null;

		if(window.XMLHttpRequest) {		// Firefox, Chrome, IE7+, Opera, Safari, ...
		  xmlHttp = new XMLHttpRequest();
		}
		else if(window.ActiveXObject) {	//  IE6, IE5
		  xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
		}

		return xmlHttp;
	}

	function load_OpenTagScript()
	{
		var headObject = document.getElementsByTagName('head')[0];
		var refNode = document.getElementsByName('ots')[0].nextSibling;
		var script = document.createElement('script');
		
		script.type = 'text/javascript';
		script.src = '//d3c3cq33003psk.cloudfront.net/opentag-75667-654305.js';
		
		headObject.insertBefore(script,refNode);
	}
	
	function get_UniversalVariableByAjax()
	{
		var xmlhttp =  get_XmlHttp();

		xmlhttp.onreadystatechange=function() {
			var headObject = document.getElementsByTagName('head')[0];
			if (xmlhttp.readyState==4 && xmlhttp.status==200) {
				window.universal_variable=JSON.parse(xmlhttp.responseText);
				load_OpenTagScript();
			}
		}

		// get universal variable json
		xmlhttp.open("GET","http://www.currys.co.uk/gb/uk/opentagscript/ajaxContext/%7B%22sFuseaction%22%3A%22item_details%22%2C%22sCountry%22%3A%22gb%22%2C%22sLanguage%22%3A%22uk%22%2C%22iUniverseId%22%3A33%2C%22iCategoryId%22%3A149%2C%22iMarketId%22%3A3402%2C%22iSegmentId%22%3A32003%2C%22iBrandId%22%3A267%2C%22iThemeId%22%3Anull%2C%22sProductId%22%3A%2221633721%22%2C%22iEquivalenceId%22%3Anull%2C%22bSecure%22%3Afalse%2C%22bProductCanBeHomeDelivered%22%3Atrue%2C%22bProductCanBeReservedAndCollected%22%3Afalse%2C%22bProductIsOutOfStock%22%3Afalse%2C%22bProductIsHomeAndRC%22%3Afalse%2C%22bProductIsDownload%22%3Afalse%7D/ajax.html",true);
		xmlhttp.send();
	}
</script>
<script name="ots" type="text/javascript">
	get_UniversalVariableByAjax();
</script>

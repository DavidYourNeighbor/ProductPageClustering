<script type='text/javascript'>
 amznJQ.onCompletion('amznJQ.criticalFeature', function() {
     amznJQ.available('bbopCheckBoxJS', function() {
         var bbopJS = new jQuery.fn.bbopCheckBox();
         bbopJS.initialize(1,0,"To get FREE One-Day Delivery on this item, proceed to checkout using &quot;Add to Basket&quot;");
     });
 });  

amznJQ.onReady('jQuery', function() {
(function(){
   jQuery.fn.bbopCheckBox = function(){
       jQuery.fn.bbopCheckBox.prototype.initialize = function(showBBOP,isSNS,popoverContent) { 
         amznJQ.onReady('popover', function() { 
               (function($) { 
                 $('#primeLearnMoreLink').amazonPopoverTrigger({ 
                   hoverShowDelay: 200, 
                   hoverHideDelay: 200, 
                   width: 460, 
                   showCloseButton: true, 
                   localContent: "#primeLearnMoreDiv", 
                   skin: "default", 
                   title: "&nbsp;", 
                   location: "left",
                   onHide: function() {
				     try {
				       amznJQ.available('ClientEventLogger', function() {
					      ClientEventLogger.logActionCode('ac1');
				       });
			         } catch (exception) {}
		           }
                 }); 
          
                 var originalATCName = $("#addToCartButton, #bb_atc_button").attr("name"); 
          
                 var onBBOPCheckBoxClick = function () {  
                         // Make the prime changes if the check box is visible and it's checked 
                         if ($('#prime-rcx-subs-checkbox-outer').is(':visible') && $("#bbop-check-box").is(':checked')) { 
                           // Attach prime-signup to Add to Cart Button and Pre Order Add to Cart Button 
                           $("#addToCartButton, #bb_atc_button").attr("name","submit.add-to-cart-prime-buy-box"); 
          
                           // Disable One-Click   
                           $("#twisterOneClickOrig, #twisterOneClickOrig input, #oneClickBuyButton").css('cursor', 'not-allowed').amazonPopoverTrigger( { 
                                 location: ['left', 'auto'], 
                                 locationAlign: 'middle', 
                                 locationMargin: -10, 
                                 width: 250, 
                                 hoverHideDelay: 0, 
                                 hoverShowDelay: 0, 
                                 showOnHover: true, 
                                 showCloseButton: false, 
                                 literalContent: popoverContent, 
                                 attached: true,                         skin: "<div class='buying' style='padding: 7px; border: 1px solid rgb(92, 158, 191); background-color: rgb(255, 255, 255); text-transform: none; text-align: center;'><div class='ap_content'></div></div>" 
                           }); 
                         } else { 
                           // Remove prime-signup to Add to Cart Button and Pre Order Add to Cart Button 
                           $("#addToCartButton, #bb_atc_button").attr("name",originalATCName); 
          
                           // Re-enable One-Click  
                           $("#twisterOneClickOrig, #twisterOneClickOrig input, #oneClickBuyButton").css('cursor', '').removeAmazonPopoverTrigger(); 
                         } 
                 }; 
                 
                 $('#prime-rcx-subs-checkbox-outer').show(); 
                 onBBOPCheckBoxClick(); 
          
                 // Bind the onclick function to the checkbox  
                 $("#bbop-check-box").click(function() { onBBOPCheckBoxClick(); }); 
                 // Bind the onclick function to a custom trigger that we can call through twister 
                 $('#bbop-check-box').bind('onHideOrShow', onBBOPCheckBoxClick); 
             })(jQuery); 
         });
       };
   };
   amznJQ.declareAvailable("bbopCheckBoxJS");
 })();
});

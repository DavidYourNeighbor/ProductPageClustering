  <script type="text/javascript">
  <!--

    function DsvTwisterEventHandlerWidget() {
      var aHandlers = new Array();
      var oEventData = new Object();
      var oSelf = this;
      var oDsvDigitalDetails = null;

      this.addHandler = function(f) {
        aHandlers.push(f);
      }

      this.name = "Digital Downloads";
      
      if (typeof amznJQ != 'undefined') {
        amznJQ.available('twister', function() { oSelf.registerForEvents(); });
        amznJQ.available('jQuery', function() { oSelf.getDigitalDetails()});
      }
      
      this.registerForEvents = function() {
        if (typeof DetailPageFramework != 'undefined') {
          DetailPageFramework.registerFeature(this, ['child_select', 'child_deselect'], true);
        }
      }

      this.notify = function(eventType, dpState) { 
        var sAsin = dpState.current_asin;
        
        oEventData['raw'] = dpState;
        oEventData['asin'] = sAsin;
        oEventData['digital'] = null;
        
        if (sAsin != null) {
          DetailPage.Model.fetch(sAsin, "offers", function(sAsin, oData) { oSelf.update(sAsin, oData); } );
        }
        else {
          this.callback();
        }
      }

      this.update = function(sAsin, oData) {
      var oDigitalHtml = new Object();
      if (sAsin) {
        if (oDsvDigitalDetails[sAsin]) {
          var childItem = oDsvDigitalDetails[sAsin];
          if (childItem['dsv_is_digital']) {
            var oDigitalInfo = new Object();
            oDigitalInfo['is_download'] = (childItem['dsv_is_digital'] == 1);
            oDigitalInfo['is_e_card'] = (childItem['dsv_is_digital'] == 2);
            oDigitalInfo['is_online_code'] = (childItem['dsv_is_digital'] == 2);
            oDigitalInfo['is_bundle'] = (childItem['dsv_is_digital'] == 3);
            
            oEventData['digital'] = oDigitalInfo;
          
            oDigitalHtml['availability'] = childItem['dsv-availability'];
            oDigitalHtml['buybox'] = childItem['dsv-buy-box'];
            oDigitalHtml['downloadBar'] = childItem['dsv-download-bar'];
            oDigitalHtml['downloadData'] = childItem['dsv-download-data'];
            oDigitalHtml['bundleData'] = childItem['dsv-bundle-data'];
            oDigitalHtml['bowserIOU'] = childItem['dsv-iou'];
          }
        } 
        else {
            var dpUrl = '/gp/product/ASIN';
            window.location = dpUrl.replace(/ASIN/, sAsin);
        }
      }
      oEventData['html'] = oDigitalHtml ? oDigitalHtml : null;
      this.callback();
    }
    
    this.callback = function() {
      for (i = 0; i < aHandlers.length; i++) {
        aHandlers[i](oEventData);
      }
    }
    
    this.getDigitalDetails = function () {
        jQuery.getJSON("/gp/swvgdtt/util/digital-details.html", 
              {parentAsin : 'B00C8YB7GM', token : Math.random()}, function (data) { oDsvDigitalDetails = data; });
    }
  }

  var goDsvTwisterEventHandler = new DsvTwisterEventHandlerWidget();

  function dsvShowElement(id, visible) {
    var element = document.getElementById(id);
    if (element) {
      element.style.display = visible ? 'block' : 'none';
    }
  }

  function dsvSetElement(id, txt) {
    var element = document.getElementById(id);
    if (element) {
      element.innerHTML = txt ? txt : '';
    }
  }

  function dsvSetLink(id, link) {
    var element = document.getElementById(id);
    if (element) {
      element.href = link;
    }
  }
    
    function dsvGetElement(id) {
    var element = document.getElementById(id);
    return (element ? element.innerHTML : '');
    }
  -->

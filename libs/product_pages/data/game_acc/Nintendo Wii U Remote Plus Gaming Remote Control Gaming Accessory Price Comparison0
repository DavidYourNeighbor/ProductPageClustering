<script>
    function evalJSRegister_cdDetailPageJavascript() {
        try {
            var jsSrc = document.getElementById('cdDetailPageJavascript').innerHTML;
            jsSrc = jsSrc.replace(/&lt;/g, "<").replace(/&gt;/g, ">").replace(/&amp;/g, "&");
            eval(jsSrc);
//          alert('inline javascript eval success');
        } catch(e) {
//          alert('inline javascript eval failure');
          if (typeof console != 'undefined') {
            console.log('inline javascript eval failure: ' + e);
          }
        }
    };
  function onAjaxUpdate_customer_discussions (asin) {
    evalJSRegister_cdDetailPageJavascript();
  }
  function onCacheUpdate_customer_discussions (asin) {
    evalJSRegister_cdDetailPageJavascript();
  }
  function onCacheUpdateReselect_customer_discussions (asin) {
    evalJSRegister_cdDetailPageJavascript();
  }

</script>


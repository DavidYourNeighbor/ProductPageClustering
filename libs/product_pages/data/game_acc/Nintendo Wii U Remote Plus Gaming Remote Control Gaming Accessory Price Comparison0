  var data = { "hidePopover":hidePopover, "unavailablePopOverStringValue":unavailablePopOverStringValue, "showDimSecondUnavailablePopover":showDimSecondUnavailablePopover, "prefetchCount" : prefetchCount, "prioritizeReqPrefetch" : prioritizeReqPrefetch, "smartPrefetchWeblab" : smartPrefetchWeblab, "prefetchFixWeblab" : prefetchFixWeblab, "stateData": stateData , "variationsData" : variationsData, "viewData" : viewData, "useDpx" : useDPX};

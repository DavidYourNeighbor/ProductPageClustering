<script type="text/javascript">

	/* AJAX SEARCH */

	$(function(){
			   
		$('#query').click(showsearch).keyup(showsearch);
		
		function showsearch(){
			
			if($('#query').val() != ''){
		
				//setup before functions
				var typingTimer;                //timer identifier
				var doneTypingInterval = 500;  //time in ms
			
				$('#loading_gif').hide();
				
				$("ul#search_results").empty();
				$('ul#search_results').append('<li>Searching...</li>');
				$('ul#search_results').fadeIn(200);
				$('#loading_gif').show();				
		
				// START TIMER FOR DELAY IN TYPING
				clearTimeout(typingTimer);
				if ($('#myInput').val) {
					typingTimer = setTimeout(doneTyping, doneTypingInterval);
				}			
				
				function doneTyping (){
		
					/* SERIALIZE FORM DATA */
					var post = $('#search_form').serialize(); 
					
					$.post('http://www.simplygames.com/ajax.php?page=live_search&rand=' + Math.random(), post ,function(data) {
						
						$('#loading_gif').hide();
						$("ul#search_results").empty();
						$('ul#search_results').append(data);
					
					});
		
				}
			
			}
			
		}

		/* HIDE LIST ON MOUSE OUT */
		
		$('ul#search_results').hover(function(){
		
			$('ul#search_results').show();
		
		}, function(){
			
			$('ul#search_results').delay(200).fadeOut(200,function(){
																   
				$("ul#search_results").empty();
				$('#loading_gif').hide();
				
			});
			
		});	
	
	});	
    
</script>


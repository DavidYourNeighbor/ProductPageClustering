							if (i == 13) {
								var cmsrb = document.getElementById("C_Main_Sub_Right_Big");
									var a = document.createElement("object");
									a.setAttribute("style", "width: 400px; height: 400px; display: none;");
										
										var b = document.createElement("param");
										b.setAttribute("name", "movie");
										b.setAttribute("value", "http://www.youtube.com/v/" + images[i][0] + "?version=3&rel=0&modestbranding=1");
										
										var c = document.createElement("param");
										c.setAttribute("name", "allowFullScreen");
										c.setAttribute("value", "true");
										
										var d = document.createElement("param");
										d.setAttribute("name", "allowscriptaccess");
										d.setAttribute("value", "always");
										
										var e = document.createElement("embed");
										e.setAttribute("type", "application/x-shockwave-flash");
										e.setAttribute("style", "height: 400px; width: 400px;");
										e.setAttribute("src", "http://www.youtube.com/v/" + images[i][0] + "?version=3&rel=0&modestbranding=1");
										e.setAttribute("allowfullscreen", "true");
										e.setAttribute("allowscriptaccess", "always");
										
											a.appendChild(b);
											a.appendChild(c);
											a.appendChild(d);
											a.appendChild(e);
											cmsrb.appendChild(a);
							}
							num++;
						}
					}
					
					//open zoom window
					var cmsrbi = document.getElementById("C_Main_Sub_Right_Big").getElementsByTagName("img")[0];
					cmsrbi.addEventListener("mouseover", function () {
						document.getElementById("C_Main_Sub_Right_Big_Zoom").style.backgroundImage = "url('" + this.getAttribute("src") + "')";
					}, false);
					
					//move zoomed image
					cmsrbi.addEventListener("mousemove", function (e) {
						var event = e || window.event;
						
						var x = event.pageX - parseInt(this.offsetLeft) - parseInt(document.getElementById("C_All").offsetLeft);
						var y = event.pageY - parseInt(this.offsetTop) - parseInt(document.getElementById("C_All").offsetTop + 80);
						
						var xP = x / 4; // 4px == 1% 
						var yP = y / 4;

						var cmsrbz = document.getElementById("C_Main_Sub_Right_Big_Zoom");
						cmsrbz.style.backgroundPosition = xP + "% " + yP + "%";
					}, false);
					
					if (document.getElementsByClassName("C_Main_Sub_Right_RowN_Thumb").length <= 7) {
						var asd = document.getElementById("C_Main_Sub_Right_Row2");
						asd.parentNode.removeChild(asd);
					}
					setTimeout(function () { window.myInterval = setInterval(function () { AutoScroll(); }, 2000); }, 2000);
					
					var cmsrt = document.getElementsByClassName('C_Main_Sub_Right_RowN_Thumb');
					for (var i = 0; i < cmsrt.length; i++) {
						cmsrt[i].addEventListener("click", function () {
							if (window.myInterval)
								clearInterval(window.myInterval);
							var src = this.childNodes[0].getAttribute('src');
							ChangeMainImage(this);
						}, false);
					}

					var cmsrbl = document.getElementById("C_Main_Sub_Right_big_Left");
					cmsrbl.addEventListener("click", function () {
						if (window.myInterval)
							clearInterval(window.myInterval);
						
						var next = this.nextSibling;
						if (next && isWhitespace(next)) 
							next = next.nextSibling;
							
						var src = next.getElementsByTagName('img')[0].getAttribute('src');
						var a = 0;
						var cmsrrt = document.getElementsByClassName('C_Main_Sub_Right_RowN_Thumb');
						for (var index = 0; index < cmsrrt.length; index++) {
							if (cmsrrt[index].getElementsByTagName('img')[0].getAttribute('src') == src) {
								if (!cmsrrt[index - 1])
									index = 0;
								else 
									index--;
								ChangeMainImage(cmsrrt[index]);
								break;
							}
						}
					}, false);

					var cmsrbr = document.getElementById('C_Main_Sub_Right_big_Right');
					cmsrbr.addEventListener("click", function () {
						if (window.myInterval)
							clearInterval(window.myInterval);
						
						var prev = this.previousSibling;
						if (prev && isWhitespace(prev))
							prev = prev.previousSibling;
							
						var src = prev.getElementsByTagName('img')[0].getAttribute('src');
						var a = 0;
						var cmsrrt = document.getElementsByClassName('C_Main_Sub_Right_RowN_Thumb');
						for (var index = 0; index < cmsrrt.length; index++) {
							if (cmsrrt[index].getElementsByTagName('img')[0].getAttribute('src') == src) {
								if (!cmsrrt[index + 1])
									index = 0;
								else 
									index++;
								ChangeMainImage(cmsrrt[index]);
								break;
							} 
						}
					}, false);

					var h = document.getElementById("C_Main_Sub_Right").clientHeight - 40;
					document.getElementById('C_Main_Sub_Left_Main').style.height = h + "px";

					var h2 = document.getElementById("C_Main_Sub").clientHeight;
					document.getElementById("C_Title").style.marginTop = (h2 + 25) + "px";


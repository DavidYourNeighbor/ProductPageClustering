					function isWhitespace(node) { return node.nodeType == 3 && /^\s*$/.test(node.data); }

					function fadeIn(el) {
						if (el.style.opacity != null && el.style.opacity != '') {
							var opacity = parseFloat(el.style.opacity);
							opacity = opacity + 0.02;
						} else {
							var opacity = 0.02;
						}
							
						el.style.opacity = opacity;
						el.style.filter = "alpha(opacity=" + (opacity * 100) + ")";
						
						if (el.style.opacity < 1)
							setTimeout(function () { fadeIn(el) }, 10);
					}

					function fadeOut(el) {
						if (el.style.opacity != null && el.style.opacity != '' && el.style.opacity < 1) {
							var opacity = parseFloat(el.style.opacity);
							if (opacity > 0.02)
								opacity = opacity - 0.02;
							else
								opacity = 0;
						} else {
							var opacity = 1 - 0.02;
						}
							
						el.style.opacity = opacity;
						el.style.filter = "alpha(opacity=" + (opacity * 100) + ")";
						
						if (el.style.opacity > 0)
							setTimeout(function () { fadeOut(el) }, 10);
						else
							el.style.display = "none";
					}
					
					function AutoScroll() {
						if (typeof(window.abcdef) == 'undefined')
							window.abcdef = 1;
						else
							window.abcdef = window.abcdef + 1;
							
						if (window.abcdef > document.getElementsByClassName("C_Main_Sub_Right_RowN_Thumb").length -1 || document.getElementsByClassName("C_Main_Sub_Right_RowN_Thumb")[window.abcdef].getElementsByTagName("img")[0].getAttribute("data-i") == 13)
							window.abcdef = 0;
						var el = document.getElementsByClassName("C_Main_Sub_Right_RowN_Thumb")[window.abcdef].getElementsByTagName("img")[0];
						var src = el.getAttribute('src');
						
						var big = document.getElementById("C_Main_Sub_Right_Big").getElementsByTagName("img")[0];
						big.setAttribute('src', src);
					}
					
					var desc = document.getElementById("C_Main_Sub_Left_Title");
					desc.addEventListener("click", function () {
						var a = document.getElementById("C_Main_Sub_Left_Main");
						var b = document.getElementById("C_Main_Sub_Right");
						if (a.style.display == 'none') {
							a.style.opacity = 0;
							a.style.display = "block";
							b.style.opacity = 0;
							b.style.display = "block";
							setTimeout(function () { fadeIn(a) }, 10);
							setTimeout(function () { fadeIn(b) }, 10);
						} else {
							a.style.opacity = 1;
							setTimeout(function () { fadeOut(a) }, 10);
							setTimeout(function () { fadeOut(b) }, 10);
						}
					}, false);
					
					function ChangeMainImage(cmsrrt) {
						var el = cmsrrt;
						if (! el.getElementsByTagName('img')[0].getAttribute("src").match(new RegExp(/(youtube)/gi))) {
								
							document.getElementById('C_Main_Sub_Right_Big').getElementsByTagName('img')[0].setAttribute('src', el.getElementsByTagName('img')[0].getAttribute('src'));
							document.getElementById('C_Main_Sub_Right_Big').getElementsByTagName('img')[0].style.display = "block";				
							if (document.getElementById('C_Main_Sub_Right_Big').getElementsByTagName('object')[0])
								document.getElementById('C_Main_Sub_Right_Big').getElementsByTagName('object')[0].style.display = "none";
						} else {
							document.getElementById('C_Main_Sub_Right_Big').getElementsByTagName('img')[0].style.display = "none";
							document.getElementById('C_Main_Sub_Right_Big').getElementsByTagName('object')[0].style.display = "block";
						}
					}
					
					if (document.getElementById("C_Top_Middle_Search").offsetHeight > document.getElementById("C_Top_Middle_Button").offsetHeight)
						document.getElementById("C_Top_Middle_Button").style.height = document.getElementById("C_Top_Middle_Search").offsetHeight + "px";

					var temp = document.getElementById("C_Details").innerHTML;
					if (typeof(temp) == 'undefined' || temp == '' || temp == ' ' ) {
						temp = document.getElementById("C_Title");
						temp.parentNode.removeChild(temp);
					}
				</script>
			</div>
			<div style = 'width: 1000px; height: 300px;'></div>
		</div>
	</body>
</html></div>
		</td>
	</tr>
</table>
</div>

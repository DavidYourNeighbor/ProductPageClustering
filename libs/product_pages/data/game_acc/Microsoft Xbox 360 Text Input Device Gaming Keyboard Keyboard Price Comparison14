	if (asWarrantyCodes[""] != ""){
		if (asWarrantyTypeCodes[""] != ""){
			asWarrantyPromoTexts[""] = "";
		}
		else {
			asWarrantyPromoTexts[""] = "";
		}
	}


	asLowStockInds["option1"] = "Available";
	
	asColoursInStock[""] = "Y";
	asSizesInStock[""] = "Y";
	

	itemDetails[""] =
		{
		wasPriceDisplay : '',
		wasPriceSaveDisplay : '',
		wasPricePublicationCode : ''
		};

	
numSizes    = 1;
numColour2s = 0;



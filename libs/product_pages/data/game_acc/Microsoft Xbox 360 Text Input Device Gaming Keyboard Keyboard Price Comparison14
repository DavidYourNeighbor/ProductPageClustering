  /* <![CDATA[ */
  
	function bindXFOevents() {
		$("ul#brandnav li.curvissaTab").mousedown(function(){
											_gaq.push(['_trackEvent', 'XFO', 'Curvissa Logo', 'Go to Curvissa from ' + window.location.href ]);
										});
		$("ul#brandnav li.swimwearTab").mousedown(function(){
											_gaq.push(['_trackEvent', 'XFO', 'Swimwear365 Logo', 'Go to Swimwear365 from ' + window.location.href ]);
										});
		$("ul#brandnav li.kaleidoscopeTab").mousedown(function(){
											_gaq.push(['_trackEvent', 'XFO', 'Kaleidoscope Logo', 'Go to Kaleidoscope from ' + window.location.href ]);
										});
		$("ul#brandnav li.wittTab").mousedown(function(){
											_gaq.push(['_trackEvent', 'XFO', 'Witt-International Logo', 'Go to Witt-International from ' + window.location.href ]);
										});								
	}
	
	function bindTopNavEvents() {
		$("div#siteWideBanner").mousedown(function(){
			_gaq.push(['_trackEvent', 'Site Wide Banner', 'Click for:' + $(this).text()]);
		});
		$("div#divHeader>ul#topNav>li>a, div#divHeader>ul#topNav>li>span>a").mousedown(function(){
			_gaq.push(['_trackEvent', 'Top Nav', 'Tab Click', $(this).attr("title")]);
		});
		$("div#divHeader>ul#topNav>li>a").each(function(){
			var thisTab = $(this);
			var tabName = thisTab.attr("title");
			thisTab.parent().find("div.topnavFlydown>div>ul.categoryList>li>a").mousedown(function(){
				_gaq.push(['_trackEvent', 'Top Nav', tabName + ' Dropdown', 'Category: ' + $(this).attr('title')]);
			});
			thisTab.parent().find("ul.promotionsList>li>a").mousedown(function(){
				var thisAnchor = $(this);
				_gaq.push(['_trackEvent', 'Top Nav', tabName + ' Dropdown', thisAnchor.parent().siblings(".flydownHeading").text() + ": " + thisAnchor.attr('title')]);
			});
			thisTab.parent().find("ul.plainBoldList>li>a, ul.plainList>li>a").mousedown(function(){
				var thisAnchor = $(this);
				_gaq.push(['_trackEvent', 'Top Nav', tabName + ' Dropdown', thisAnchor.text() + ": " + thisAnchor.attr('href')]);
			});
		});
		
	}
	
	//function that loads the 1.8.2 jquery library if no jquery on page and then calls the dynamicBinding function
    window.onload = function(){
	    if(!window.jQuery)
        {   var jqscript = document.createElement("script"); 
			jqscript.type = "text/javascript"; jqscript.src = "/common/jssource/jquery-1.8.2.min.js";
			jqscript.async = true;
			jqscript.onload = function(){dynamicBinding();};
            jqscript.onreadystatechange = function() {  if (this.readyState == 'complete') { dynamicBinding(); }  }
            var jqs = document.getElementsByTagName('script')[0]; 
            jqs.parentNode.insertBefore(jqscript, jqs);  
		} 
		else 
		{
		    dynamicBinding();
		}	   
	};			
//Functions					
				
	
    //function that binds mousedown events for recently viewed items
	function recentlyViewedItemsBind(page) {
	    $("ul#RecentlyViewed a.imgLink").each(function(){ 
														$(this).mousedown(function(){_gaq.push(['_trackEvent', 'Recommendations', 'Recently Viewed Items - '+page + ' page', 'Item Image - Link Click:' + $(this).attr('href') ]);
																				});
													}); 
		$("ul#RecentlyViewed p.itemDescription a").each(function(){ 
														$(this).mousedown(function(){_gaq.push(['_trackEvent', 'Recommendations', 'Recently Viewed Items - '+page + ' page', 'Item Description - Link Click:' + $(this).attr('href') ]);
																				});
													}); 
		$("ul#RecentlyViewed p.itemPrice a").each(function(){ 
														$(this).mousedown(function(){_gaq.push(['_trackEvent', 'Recommendations', 'Recently Viewed Items - '+page + ' page', 'Item Price - Link Click:' + $(this).attr('href') ]);
																				});
													}); 											
	}
	
	
    //function that binds social events to all social links
	function bindSocial() {
	$("[href*='facebook']").each(function(){ 
	                $(this).attr('target','_blank');
				    $(this).mousedown(function(){
								_gaq.push(['_trackSocial', 'facebook', 'LookAgain Facebook interaction', 'Link Click FOR ' + $(this).attr('href') + ' ON ' + document.location.href ]);
								     });
									    }); 
    $("[href*='twitter']").each(function(){ 
	                $(this).attr('target','_blank');
				    $(this).mousedown(function(){
								_gaq.push(['_trackSocial', 'twitter', 'LookAgain Twitter interaction', 'Link Click FOR ' + $(this).attr('href') + ' ON ' + document.location.href]);
								     });	
                                        }); 
    $("[href*='blog']").not("[title='Blog - Behind The Look']").each(function(){ 
	                $(this).mousedown(function(){
								_gaq.push(['_trackEvent', 'Inspire', 'LookAgain Blog Interaction', 'Link Click FOR ' + $(this).attr('href') + ' ON ' + document.location.href]);
								     });	
                                        }); 	
	$("[href*='pinterest']").each(function(){ 
	                $(this).mousedown(function(){
								_gaq.push(['_trackSocial', 'pinterest', 'LookAgain Pinterest Interaction', 'Link Click FOR ' + $(this).attr('href') + ' ON ' + document.location.href]);
								     });	
                                        }); 										
    }	
	
	
	
	function dynamicBinding() {
		$("div.bddRemove").each(function(){
					var itemInfo = 'Product:' + $(this).siblings(".bddDesc").text() + '_' + $(this).siblings(".bddColour").text() + '_'  + $(this).siblings(".bddSize").text() + '_' + $(this).siblings(".bddQty").text() + '_Price:' + $(this).siblings(".bddPrice").text();
					$(this).mousedown(function(){_gaq.push(['_trackEvent', 'Item Removal', 'Remove item from basket fly-down', itemInfo]);
								     });
		});							 
	    bindSocial();
		bindXFOevents();
		bindTopNavEvents();
		//binding an event to the footer Email Newsletter section
		$("a#emailSignUpText").mousedown(function(){
		                        _gaq.push(['_trackEvent', 'Email newsletter signup', 'Signup Link click','Footer Link for signup.asp' ]);
								     });
		//bind events to footer other shops links
		$("ul#otherShops > li > a").each(function(){ 
		                    $(this).mousedown(function(){
								_gaq.push(['_trackEvent', 'Footer', 'Other Shops', 'Link Click for ' + $(this).attr('href') ]);
								     });
								       }); 	
		$("div.footerColumn:eq(0), div.footerColumn:eq(1)").each(function(){ 
							var thisColumn = $(this);
							var columnName = thisColumn.find("h3").text();
							$(this).find("ul>li>a").mousedown(function(){ 
								_gaq.push(['_trackEvent', 'Footer', columnName, 'Link Click for ' + $(this).text() ]);
													});
															}); 
		
		$("a.emailAFriendLink").mousedown(function(){ 
							    _gaq.push(['_trackEvent', 'Goal', 'Email a friend about a product', 'Link Click for ' + $(this).attr('href') ]);
								     });

		

		//function that binds mousedown events for productdisplay recommendations
		$("div.recommendationsContainer").each(function(){
													var element = $(this);
													if (element.attr("id") != 'recentlyViewedParentContainer') {
														var recType = element.attr("id").replace("ParentContainer","");		
														element.find("a.imgLink").each(function(){
																					$(this).mousedown(function(){
																						_gaq.push(['_trackEvent', 'Recommendations', recType + ' - productdisplay page', 'Item Image - Link Click:'  + $(this).attr('href') ]);
																					});
														});								
														element.find("p.itemDescription a").each(function(){ 
																					$(this).mousedown(function(){
																						_gaq.push(['_trackEvent', 'Recommendations', recType + ' - productdisplay page', 'Item Description - Link Click:'  + $(this).attr('href') ]);
																					});
														});
														element.find("p.itemPrice a").each(function(){ 
																					$(this).mousedown(function(){
																						_gaq.push(['_trackEvent', 'Recommendations', recType + ' - productdisplay page', 'Item Price - Link Click:'  + $(this).attr('href') ]);
																					});
														});											
													}
		}); 
		recentlyViewedItemsBind("productdisplay");	
	  		                                        
	
	_gaq.push(['_trackPageview']);
		
			
	}

  //Google Analytics end of tracking snippet
   (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
  /* ]]> */	
</script>





function outputError(errorText)
{
    $("#add_error_msg").html(errorText);
    $("#add_error_msg").show();
}

function addItemToBagOrHotlist() {

    var o1 = $("#option1 option:selected");
    var o2 = $("#option2 option:selected");
    var extraOption = $("#extraOption option:selected");
    var qty = $("#quantity option:selected");
    var colour2Sel = colourSel = sizeSel = quantitySel = "";
    var products = "", sKey = "", errorMsgSubmit = "", maxBagLines = 30;
    var totalCount;
    if (hotlistSelected != "true")
    {   var trolleyitemscount = 0;
        totalcount = parseInt(trolleyitemscount) + 1;
        if (trolleyitemscount >= maxBagLines) {
            errorMsgSubmit = "The maximum number("+maxBagLines+") of item lines have been added to the order";
            outputError(errorMsgSubmit);
            return;
        }

        if (totalcount > maxBagLines) {
            errorMsgSubmit = "Too many item lines (max " + maxBagLines + ") are being added to the order, decrease quantity required.";
            outputError(errorMsgSubmit);
            return;
        }
    } else {
        var VideoName = "", ProductListAttributes = "ProdListAttr", ProductListText = "ProdListText";
        if (VideoName == ""){
           VideoName = "N/A";
        }
        

        totalcount = parseInt(hotlistitemscount) + 1;
        
            if (hotlistitemscount >= maxBagLines) {
                errorMsgSubmit = "The maximum number(" + maxBagLines+ ") of item lines have been added to the Hot List";
                outputError(errorMsgSubmit);
                return;

/* Function        : option_change                                                                */
/*------------------------------------------------------------------------------------------------*/
/* Description     : Function to handle item option changes.                                      */
/*------------------------------------------------------------------------------------------------*/
function option_change(optionSelected)
{
	var urlCatno = "";
	var warrantyChecked = "";
	var warrantyDeclinedChecked = "";
	
	var radioAccept = document.getElementById("fsRadioAccept");
	var radioDecline = document.getElementById("fsRadioDecline");
	if(radioAccept != null) {
		if (radioAccept.checked){
			warrantyChecked = "CHECKED";
			warrantyDeclinedChecked = "";
		}
		else if (radioDecline.checked){
			warrantyChecked = "";
			warrantyDeclinedChecked = "CHECKED";
		}
		else{
			warrantyChecked = "";
			warrantyDeclinedChecked = "";
		}
	}
	
	var o1 = document.getElementById("option1");
	var o2 = document.getElementById("option2");
	var extraOption = document.getElementById("extraOption");
	var colCount  = "NONE";
	var col2Count = "NONE";
	var sizCount  = "NONE";
	var sKey = "";
	var midKey = "";
	if(o1 != null) {
		if (colCount == "MULTI"){
			sKey = o1.options[o1.selectedIndex].value;
		}
		else {
			sKey = o1.value;
		}
	}
	if(extraOption != null) {
		if (col2Count == "MULTI"){
			sKey += extraOption.options[extraOption.selectedIndex].value;
		}
		else {
			sKey += extraOption.value;
		}
	}
	midKey = sKey;
	if(o2 != null) {
		if (sizCount == "MULTI"){
			sKey += o2.options[o2.selectedIndex].value;
		}
		else {
			sKey += o2.value;
		}
	}
	// If changed a different catalogue item (option 1) then create extra option.
	if (optionSelected && col2Count == "MULTI")
	{
		if (optionSelected.id == "option1")
		{
			if ($("#emailinstockOptions").is(":visible"))
					{
					   $("#emailinstockOptions").slideUp();
					   $("#emailinstockerror").hide();
					}
			createColour2Options(o1.value, "-- Select option --&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
			sKey = o1.value;

		}
	}
	// If changed a different catalogue item (option 1) then rewrite option 2 with relevant low stock ind.
	if (optionSelected && sizCount == "MULTI")
	{
		if (optionSelected.id == "extraOption" || (optionSelected.id == "option1" && col2Count != "MULTI"))
		{
			if(optionSelected.id == "extraOption" && extraOption.selectedIndex == 0){
				midKey = "";
			}
			if ($("#emailinstockOptions").is(":visible"))
			{
			   $("#emailinstockOptions").slideUp();
			   $("#emailinstockerror").hide();
			}
			createSizeOptions(midKey, "-- Select option --&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
			sKey = midKey;
		}
	}
	
	// Back of book - check that an identifier (colour) has been populated
	var Sizenames = asSizenames[sKey];	
	var strIdentifierPopulated = "";
	if(typeof(Sizenames) != "undefined")
	{
		if (Sizenames.length > 0)
		{
			for (var i=0;i<asScene7ImageIdentifier.length;i++)
			{
				if (asScene7ImageIdentifier[i] != "")
				{
					strIdentifierPopulated = true;
					break;
				}
			}
		}	
	}
	
	// If a colour has been populated - check for a different image (back of book)
	if (strIdentifierPopulated == true)
	{
		var strImageForColour = "";
		var strScene7ImageFullName = "";
		var masterItem = "96T894";
		if(typeof(Sizenames) != "undefined")
		{
			if (Sizenames.length > 0)
			{
				Sizenames = Sizenames.toUpperCase().replace(/ /,"_").replace("/","");
				for (var i=0;i<asScene7ImageIdentifier.length;i++)
				{
					if (Sizenames == asScene7ImageIdentifier[i])
					{
						asImageForColour[sKey] = asScene7ImageFullName[i];
						break;
					}
					// Default to master image if no colour match found
					if (asScene7ImageIdentifier[i] == "")
					{
						strScene7ImageFullName = asScene7ImageFullName[i];
						if (strScene7ImageFullName.length == 10)
						{
							if (asScene7ImageCatalogueNumber[i] == masterItem || strImageForColour == "")
							{
							strImageForColour = asScene7ImageFullName[i];
							asImageForColour[sKey] = strImageForColour;
							}
						}
					}
				}
			}
		}
	}
	
	var ImageForColour = asImageForColour[sKey];
	if(typeof(ImageForColour) != "undefined") {
		if(ImageForColour != ""){
			var ImageSeoText;
			var ImageForColourUrl;
			var ImageForColourParam;
			ImageSeoText = "Xbox 360 Chatpad and Headset (3&#43;) by Microsoft";
			ImageSeoText = ImageSeoText.replace(/ /g,"-");
			ImageSeoText = ImageSeoText.replace(/\//g,"-");
			ImageForColourParam = "cloudzoom";
			ImageForColourUrl = "http://images.lookagain.co.uk" + "/is/image/OttoUK/" + ImageForColourParam + "/" + ImageSeoText + "~" + ImageForColour + ".jpg";
			changeZoomLink(ImageForColourUrl);
			changeLargeLink(ImageForColour);
			ImageForColourParam = "355w";
			ImageForColourUrl = "http://images.lookagain.co.uk" + "/is/image/OttoUK/" + ImageForColourParam + "/" + ImageSeoText + "~" + ImageForColour + ".jpg";
			MM_swapImage('product_image','',ImageForColourUrl,1);
		}
	}
	// Set the was price lements now we have a change in option.
	setWasPriceElements('', '', itemDetails, false);

	var deltype = asDeliveryTypes[sKey];
	var o5 = document.getElementById("delivery_container");

	var delcharge = asDeliveryCharges[sKey];

	//Update the delivery charge
	var o9 = document.getElementById("delivery_charge_container");
	o9.innerHTML = "<p class='emph'>Delivery &pound;3.99</p>";
	if (delcharge > 0)
	{
		o9.innerHTML = '<p><span class="emph">Delivery&nbsp;&pound;' + delcharge + '</span></p>';
	}
	var o6          = document.getElementById("fs_desc_price");
	var o7          = document.getElementById("fs_select");
	var warcode     = asWarrantyCodes[sKey];
	var wardesccode = asWarrantyDescCodes[sKey];
	var wartypecode = asWarrantyTypeCodes[sKey];
	var o14         = document.getElementById("quantity");

	if(typeof(wardesccode) != "undefined")
	{
		//Extended Warranty
		if(wardesccode == "EW"){
			var o10 = document.getElementById("ewtab");
			var warimg = "/web/pix/productdisplay/fs_header_extended_warranty.gif";
		   if(wartypecode == "LO"){
				var fsmaintext = '<span class="fs_optiontext"><br />Including:</span><ul><li>Accidental damage cover</li><li>No hidden costs - labour &amp; callout costs included</li></ul>';
			}
			else if(wartypecode == "PL"){
				var fsmaintext = '<span class="fs_optiontext"><br />Including:</span><ul><li>Accidental damage cover</li><li>No hidden costs - parts, labour &amp; callout costs included</li></ul>';
			}
			else if(wartypecode == "AD"){
				var fsmaintext = '<span class="fs_optiontext"><br />Including:</span><ul><li>Accidental damage cover</li></ul>';
			}
			else{
				var fsmaintext = '<span class="fs_optiontext"><br />Including:</span><ul><li>Accidental damage cover</li><li>No hidden costs - parts, labour &amp; callout costs included</li><li>Protection against theft & robbery</li></ul>';
			}
			var fslinks = '<a href="/web/pix/financial-services/ew.pdf" target="_blank" alt="policy details">View full Terms & Conditions &raquo;</a>';
			document.getElementById('warrantyTabContainer').style.display = "block";            
		}

		//Furniture Protection
		if(wardesccode == "FP"){
			var o10 = document.getElementById("fptab");
			var warimg = "/web/pix/productdisplay/fs_header_furniture_protection.gif";
			var fsmaintext = '<span class="fs_optiontext"><br>Including:</span><ul><li>Protect your purchases against accidental damage</li><li>No hidden costs</li><li>One phone call is all it takes to arrange a repair within days</li><li>New for old replacement if it cant be repaired</li></ul>';
			var fslinks = '<a class="landinglink" style="color:#C5168D;" href="/web/pix/financial-services/fp.pdf" target="_blank"><span class="fs_pinktext">View full Terms & Conditions &raquo;</span></a>';
			document.getElementById('warrantyTabContainer').style.display = "block";            
		}

		var o11 = document.getElementById("productdisplaywarrantybox");
	}
	if (urlCatno != "" && asItemcodes[sKey] != urlCatno){
		warrantyChecked = "";
	}

	if (warcode == "") { warcode = undefined; }

	if(typeof(warcode) != "undefined") {
		if(warcode != ""){
			if(asWarrantyDurations[sKey] != ""){
				var fsTabHeaderText = asWarrantyDurations[sKey] + ' years cover for only &pound;' + asWarrantyPrices[sKey]*o14.options[o14.selectedIndex].value;
			}
			else{
				var fsTabHeaderText = 'for only &pound;' + asWarrantyPrices[sKey]*o14.options[o14.selectedIndex].value;
			}
			/*
			If the warranty type is ProductProtect
			*/
			if(asWarrantyDescs[sKey] == "PRC"){
				o6.innerHTML = '<div class="fs_optiontext">' + asWarrantyDescs[sKey].replace("PRC","Product Protect") + ' - ' + fsTabHeaderText + '<br /><a class="landinglink" style="color:#666666;" href="#" onclick="showTab(\'' + asWarrantyDescCodes[sKey].toLowerCase() + '\'); $(\'.info_tabs\').addClass(\'inactive\'); $(\'#warrantyTabContainer a\').removeClass(\'inactive\');return false">more info</a></div>';
			}else{
			o6.innerHTML = '<div class="fs_optiontext">' + asWarrantyDescs[sKey].replace("PRC","Product Protect") + ' - ' + fsTabHeaderText + '<br> <a class="landinglink" style="color:#666666;" id="warranty_moreinfo" href="#" onclick="showTab(\'' + asWarrantyDescCodes[sKey].toLowerCase() + '\'); $(\'.info_tabs\').addClass(\'inactive\'); $(\'#warrantyTabContainer a\').removeClass(\'inactive\');return false">more info</a></div>';
			}

			
			o7.innerHTML = '<div id="fs_multiple_outer">' +
								'<label for="fsRadioAccept">Yes please</label>' +
								'<input name="fs_radio" ' + warrantyChecked + ' type="radio" value="Y" id="fsRadioAccept" onclick="option_change(this)"/>' +
								'<br/>' +
								'<label for="fsRadioDecline">No thanks</label>' +
								'<input name="fs_radio" ' + warrantyDeclinedChecked + ' type="radio" value="N" id="fsRadioDecline" onclick="option_change(this)">'
							'</div>';
			

			if(wardesccode != "PR")
			{
				o10.style.display = "block";
				o11.innerHTML = '<img src="' + warimg + '"><p>' + fsTabHeaderText + '</p>' + fsmaintext + fslinks;
			}
            $("#warrantyWrapper").show();
		}
	}
    else
    {
        document.getElementById("fptab").style.display = "none";
	    document.getElementById("ewtab").style.display = "none";
	    document.getElementById("prtab").style.display = "none";
		document.getElementById("warrantyTabContainer").style.display = "none";
        $("#detailsTab").trigger("click");        
        $("#warrantyWrapper").hide();
    }

	var fPrice       = asPrices[sKey];
	var o3           = document.getElementById("price_container");
	var o12          = document.getElementById("stockLinePrice");
	var o13          = document.getElementById("stockLineCatno");
	var qtyContainer = document.getElementById("qtyContainer");
    var o15 = document.getElementById("stockAndPrice");
	
	if(typeof(fPrice) != "undefined") {

		$('#quantity').removeAttr('disabled');
		o13.innerHTML = "<span class='emph'>Product Code:</span> " + asItemcodes[sKey] + asPubs[sKey];
		o15.style.display = 'block';
		var tmpNumStr = new String(fPrice.toFixed(2));
		var priceTimesQty = (fPrice.toFixed(2))*o14.options[o14.selectedIndex].value;
		var tmpNumStr2 = priceTimesQty.toFixed(2);

		if (fPrice >= 1000) {
			var iStart = tmpNumStr.indexOf(".");
			if (iStart < 0)
			iStart = tmpNumStr.length;

			iStart -= 3;
			while (iStart >= 1) {
			tmpNumStr = tmpNumStr.substring(0,iStart) + "," + tmpNumStr.substring(iStart,tmpNumStr.length)
			iStart -= 3;
			}
		}
		if (priceTimesQty >= 1000) {
			var iStart = tmpNumStr2.indexOf(".");
			if (iStart < 0)
			iStart = tmpNumStr2.length;

			iStart -= 3;
			while (iStart >= 1) {
			tmpNumStr2 = tmpNumStr2.substring(0,iStart) + "," + tmpNumStr2.substring(iStart,tmpNumStr2.length)
			iStart -= 3;
			}
		}

		o3.innerHTML = "&pound;" + tmpNumStr;
		stockLinePriceOnly = "&pound;" + tmpNumStr2;
		if (warrantyChecked == "CHECKED"){
			var warPriceTimesQty = (asWarrantyPrices[sKey])*o14.options[o14.selectedIndex].value;
			var totalPrice = priceTimesQty + warPriceTimesQty;
			warPriceTimesQty = warPriceTimesQty.toFixed(2);
			var tmpNumStr3 = totalPrice.toFixed(2);
			if (totalPrice >= 1000) {
				var iStart = tmpNumStr3.indexOf(".");
				if (iStart < 0)
				iStart = tmpNumStr3.length;

				iStart -= 3;
				while (iStart >= 1) {
				tmpNumStr3 = tmpNumStr3.substring(0,iStart) + "," + tmpNumStr3.substring(iStart,tmpNumStr3.length)
				iStart -= 3;
				}
			}
			stockLinePriceWithWarranty = stockLinePriceOnly + " + &pound;" + warPriceTimesQty + "<br>= <span class='emph'>&pound;" + tmpNumStr3 + "</span>";
			o12.innerHTML = stockLinePriceWithWarranty;
		}
		else {

			o12.innerHTML = "<span class='emph'>" + stockLinePriceOnly + '</span>';
		}

		if (doStockCheck != "SKIP") {
			doStockCheck = "Y";
		}
	}
	else {
		o12.innerHTML = '';
		o15.style.display = 'none';
		$('#quantity').attr('disabled','disabled');
	}

	// If need to, perform the stock check AJAX call.
	if (doStockCheck == "Y")
	{
		checkStock("http://www.lookagain.co.uk/common/functions/ServiceStockBalance.asp?tc=LKA&tt=AC&cics=P&lcg=P&debug=0", "NONE", "NONE", "NONE");
	}
}

/* ]]> */
</script>


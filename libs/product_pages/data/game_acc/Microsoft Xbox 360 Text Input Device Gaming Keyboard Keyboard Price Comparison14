<script type="text/javascript">
/* <![CDATA[ */
var hotlistSelected;
var asPrices             = new Array();
var asPubs               = new Array();
var asItemcodes          = new Array();
var asSizenames          = new Array();
var asStockSizes         = new Array();
var asStockSizenames     = new Array();
var asDefaultSizes       = new Array();
var asDefaultSizenames   = new Array();
var asDeliveryTypes      = new Array();
var asDeliveryCharges    = new Array();
var asWarrantyCodes      = new Array();
var asWarrantyDescCodes  = new Array();
var asWarrantyDescs      = new Array();
var asWarrantyDurations  = new Array();
var asWarrantyItems      = new Array();
var asWarrantyPrices     = new Array();
var asWarrantyTypeCodes  = new Array();
var asWarrantyFullDescs  = new Array();
var asWarrantyPromoTexts = new Array();
var asPremDels           = new Array();
var asScene7AImages      = new Array();
var asExtraOptions       = new Array();
var asLowStockInds       = new Array();
var asColoursInStock     = new Array();
var asColour2sInStock    = new Array();
var asSizesInStock       = new Array();
var asImageForColour     = new Array();
var colour2Count         = new Array();
var sizeCount            = new Array();
var itemDetails = new Array();
var numColour2s = 0;
var numSizes    = 0;
var asScene7ImageCatalogueNumber	= new Array();
var asScene7ImageFullName			= new Array();
var asScene7ImageIdentifier			= new Array();

			asScene7ImageCatalogueNumber[0] = "96T894";
			asScene7ImageFullName[0] = "96T894FRSP";
			asScene7ImageIdentifier[0] = "";
			
			asImageForColour[""] = "96T894FRSP";
			colour2Count[""] = 0;sizeCount[""]   = 1;
	asPrices[""]            = 30;
	asPubs[""]              = "WLJ";
	asItemcodes[""]         = "96T894";
	asSizenames[""]         = "";
	asStockSizenames["option1"]         = "";
	asDefaultSizenames["defaultoption1"]     = "";
	asStockSizes["option1"]             = "";
	asDefaultSizes["defaultoption1"]         = "";
	asExtraOptions["option0"]     = "";
	asDeliveryTypes[""]     = "D";
	asDeliveryCharges[""]   = "3.99";
	asPremDels[""]          = "";
	asScene7AImages[""]     = "96T894FRSP";
	asWarrantyCodes[""]     = "";
	asWarrantyDescCodes[""] = "";

	if (asWarrantyDescCodes[""] != ""){
		asWarrantyDescs[""] = "";
	}
	else {
		asWarrantyDescs[""] = "";
	}

	asWarrantyDurations[""]  = "";
	asWarrantyItems[""]      = "";
	asWarrantyPrices[""]     = 0;
	asWarrantyTypeCodes[""]  = "";
	asWarrantyFullDescs[""]  = "";
	asWarrantyPromoTexts[""] = "";

	if (asWarrantyDurations[""] != ""){
		asWarrantyFullDescs[""] = asWarrantyFullDescs[""] + " years ";
	}
	if (asWarrantyDescCodes[""] != ""){
		asWarrantyFullDescs[""] = asWarrantyFullDescs[""] + "";

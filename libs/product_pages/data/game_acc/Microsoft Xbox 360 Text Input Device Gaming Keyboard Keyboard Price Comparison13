	<script type="text/javascript" charset="UTF-8" src="/wcsstore/argos/en_GB/images/emark.js"></script> 
	<script type="text/javascript">
		// <![CDATA[
			
			AI_CUSTOMER_ID = "38e5c972-174d-11e0-a152-12313b032222";
			
			// product to product - 624a
			// script switch
			argos.pdp.productToProductActive = true;
			
			if(argos.pdp.productToProductActive){
				// add potential container's max product limit to array (containerID,maxlimit value) - CR 624a
				argos.products.components.setContainerProductComponentLimit('#pdpAdditionalItems',20);
				argos.products.components.setContainerProductComponentLimit('#pdpAlternativeProducts',20);
				//set emark object up
				var emark = new Emark();
				// run when page complete
				$(document).ready(function() {	
					var additionalItemsRequest = emark.getRecommendations("33014232_Technology_Headsets_YouMayAlsoLike", ["ProductId:"+argos.pdp.product.number]);
					var alternativeProductsRequest = emark.getRecommendations("ProductPage_Alternatives", ["ProductId:"+argos.pdp.product.number]);
					emark.commit(function() {     
						//Get presentation data for the products in this array and render the result
						// param order - avail request (partnumbers returned), selector, tab title, apply quick link
					argos.pdp.requestProducts(additionalItemsRequest.products,"#pdpAdditionalItems","FetchProductDetailsForP2P","You may also like",true,additionalItemsRequest.trackingcode);
					argos.pdp.requestProducts(alternativeProductsRequest.products,"#pdpAlternativeProducts","FetchProductDetailsForP2P","Alternatives",true,alternativeProductsRequest.trackingcode);
					//FetchProductDetailsForP2PAlt changed to FetchProductDetailsForP2PAlt as double click handling is fixed
					});
			
				});
			}
		// ]]>
	</script>


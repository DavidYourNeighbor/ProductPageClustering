							function picOnLoad(isSetClkId){
								var elem = document.getElementById('icThrImg'); 
								var pic = document.getElementById('icImg'); 
								elem.style.display = 'none'; 
								pic.style.display = ''; 
								if(isSetClkId) {
									pic.setAttribute('clk', elem.getAttribute('imgsel'));	
								} 

							var image = document.createElement('img');
							image.src= 'http://i.ebayimg.com/t/Skylanders-Giants-Light-Core-Giant-New-Character-Figure-s-Wave-2-Brand-New-/00/s/MjE3WDMwMA==/$T2eC16J,!)UE9s3wBohGBQnQ-4yz0Q~~60_35.JPG';

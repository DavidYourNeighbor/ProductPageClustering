                <div class="clear"></div>
            </div>
            <div class="clear"></div>
            <div class="media">
                            
                <!-- below is the original video player stuff - Start -->
                                <div class="pre-roll-trailers"></div>
                <div class="media-viewer">                     

<script type="text/javascript">
    var promo = {
        no : 1,
        protectionDiv : 1,        
        protectionAdd : 1,
        protectionNoAdd : "0",
                
    }
    var VideoPlayer = {
        flashPlayer: '/assets/967dca32/player.swf',
        flashPlayerSkin: "/assets/967dca32/jreid_shopto1.zip",
        logo: "/assets/967dca32/shoptotv_logo1.png",
        logo2: "/assets/967dca32/shoptotv_logo2.png",
        position: 'bottom-right',
        promoVideo: "http://tv.shopto.net/videos/flv/4366.flv",
        source_type: "",
          plugins: {
          /*"ova-jw": {
              ads: {
                skipAd: {
                  enabled: "true"
                },
                companions: {
                  regions: [
                    {
                      id: "companion",
                      width: 80,
                      height: 300
                    }
                  ]
                },
                schedule: [
                  {
                    position: "pre-roll",
                    tag: "http://ad.afy11.net/ad?enc=4&asId=10407&referrer=2134323"
                  }
                ]
              },
              debug: {
                levels: "fatal, config, vast_template, http_calls"
              }
            }*/
      },      
              
        /**
         * Creates video player
         * 
         * element and sources are required, other parameters are optional
         * 
         * @param element Container element for video player
         * @param sources Array of urls linking to video files
         * @param height
         * @param width
         * @param id Unique id of video
         */
        setup: function(element, sources, thumbnail, id, height, width){
            //console.log('setup called');
            if (height == undefined) {
                height = 398;
            }
            if (width == undefined) {
                width = 670;
            }
            if (id == undefined) {
                id = "video_yw0";
            }
            // setup html
            var html = "<video height='" + height + "' width='" + width + "' id='" + id + "'>";
            
            if (sources != undefined) {
                for (key in sources){
                    source = sources[key];
                    //console.log('added source:'+source);
                    html+= "<source src='"+source+"'>";
                }
            }
            
            html+= "</video>";
            //$(element).append(html);
            $(element).html(html);
            
            // let jwplayer take over
            VideoPlayer.init(id, thumbnail);
        },
        
        /**
         * Helper function to init jwplayer, should not be used directly
         */
        init: function(id, thumbnail){
            //console.log('jwplayer acting on this html: '+$('#'+id).html());
            //console.log('LOGO: '+VideoPlayer.logo);
            
            jwplayer(id).setup({
                autostart: false,
                modes: [
                    /*{ type: 'html5' },*/ /* Chrome actually renders html5 videos, but because we do not have a theme I am disabling this */
                    { type: 'flash', src: VideoPlayer.flashPlayer }
                ],
                repeat: 'list', /* needed for playlists to work */
                
                /* proof of concept for playing multiple videos */

                                //uncomment to show playlist
                //'playlist.position': 'right',
                //'playlist.size': '320',
               
               
               events: {
                    onReady: function() {
                        var oldPlaylist = jwplayer(id).getPlaylist();
                        //console.log('onReady start')                
                        //if this is the music sectio0n then start playlist
                        //alert(oldPlaylist.toSource());
                        //alert(VideoPlayer.playlist.toSource());
                        if(VideoPlayer.source_type=='music' && VideoPlayer.playlist!='') 
                        {
                           displayPlaylist(id,jwplayer(id).getPlaylist());
                        }else 
                        {
                            
                                //Otherwise add promo video to playlist
                                addToPlaylist(id, VideoPlayer.promoVideo);
                                
                        }
                        
                    },
                    // hide string Advertisement after trailer.
                    onPlay: function(){
                            if(promo.protectionNoAdd >= 1)
                            {
                                $('.pre-roll-trailers').hide();
                            }    
                            // trailer only once time
                            if( promo.protectionAdd == 1)
                            {    
  
                                // display string Advertisement for trailer. 
                                onBeforePlay();
                                promo.protectionAdd += 1;

                            }
                            else
                            {   
                                // hide string Advertisement for trailer.
                                // when you turn on from play list
                                $('.pre-roll-trailers').hide();
                            }
                        
                    },
                    // hide string Advertisement after trailer.
                    onComplete: function(){
                    if(promo.no >= 1)
                    {
                        $('.pre-roll-trailers').hide();
                    }
                    else
                    {   
                       promo.no += 1; 
                    }
                    }
                    
               },
        
                skin: VideoPlayer.flashPlayerSkin,
                'logo': VideoPlayer.logo,
                'logo.hide': false,
                'logo.position': VideoPlayer.position,
                image: thumbnail,
                controlbar : {position:'over'},
                'controlbar.position': 'bottom'
                //bufferlength: 1

            });
        },
        
        /**
         * This function cleans up previously setup videoplayer, if this is not done properly then loading 2nd, 3rd, etc. videos through js fails
         */
        cleanup: function(id){
        
            try {
                jwplayer(id).remove();  // when we load player from js, we always load it into the same id, so need to remove old player first
            } catch(err){
                // this fires when the player was never setup for id
            }
        
            try{
                jwplayer("video_yw0").remove();
                
            } catch(err) {
                // do nothing, just try to remove php player
            }
            
        
        }
    };
    
    function addslashes(str) {
        str=str.replace(/\\/g,'\\\\');
        str=str.replace(/\'/g,'\\\'');
        str=str.replace(/\"/g,'\\"');
        str=str.replace(/\0/g,'\\0');
        str=str.replace(/\//g,'\\\\/');
        return str;
        }

    /**
    * This is the function for setting cookie. Used to preserve current playlist
    * 
    * @param value of the cookie in json format.
    */
    function setPlaylistCookie(value) {
        value = JSON.stringify(value);
        value = addslashes(value);
        var expires = 4 * 1000 * 60 * 60;
        var today = new Date();
        today.setTime( today.getTime() );
        var expires_date = new Date( today.getTime() + (expires) );
        var name = 'playlist';
        document.cookie = name + "=" + "'"+value +"'" + ((expires==null)?"" : ("; expires=" + expires_date.toGMTString()));
    }

    /**
    * This function expands the playlist from one item to 6 items. Each item is 25px height so in order to show more items just change the 
    * numberOfItemsVisible variable
    * 
    * @param id of the div containing the playlist.
    */
    var status = 'hidden';
    var numberOfItemsVisible = 6;
    function toggle(id,div) {
        newItem = jwplayer(id).getPlaylistItem();
        if(status=='hidden') 
        {
            $('#'+div).animate({
                height: numberOfItemsVisible*25+'px'
              },500, 'linear', function() {
                  $('#'+div).css("overflow-y","scroll");
                  $('#'+div).css("overflow-x","hidden");
                  $('#playlist_button').attr('src','/themes/shopto/images/uparrow.png');
                  
              });
            
            status = 'show';      
        }else 
        {
            $('#'+div).animate({
                height: '25px'
              },1000, 'linear', function() {
                  $('#'+div).css("overflow","hidden");
                  $('#playlist_button').attr('src','/themes/shopto/images/dwnarrow.png');
                  $('#playlist').animate({ scrollTop: $("#playlist_content").scrollTop() + (30*newItem.index) });
              });
            status = 'hidden';
        }
                    
    }
    
    /**
    * This function will clear the whole playlist and start playing one item only.
    * 
    * @param file url of the track to be played
    * @param title title of the track to be played, needed for nice playlist
    * @param file artist of the track to be played, needed for nice playlist
    */
    function playOne(id,file,title,artist) 
    {
       if(title=='' || title==null) title = file; 
       //There always must be at least one file in the playilst
       var newPlaylist = [{ 'file': file, 'title':title, 'artist':artist }];
       jwplayer(id).load(newPlaylist); 
    }
    
     /**
    * This function will add one item to the end of the playlist
    * 
    * @param file url of the track to be played
    * @param title title of the track to be played, needed for nice playlist
    * @param file artist of the track to be played, needed for nice playlist
    * 
    * please remember to call displayPlaylist after changing it's content
    */
    function addToPlaylist(id,file,title,artist) 
    {
        //console.log('addtoplaylist start')           
        //return false;
        if(title=='' || title==null) title = file; 
        // get current playlist
        var oldPlaylist = jwplayer(id).getPlaylist();
        // create new playlist
        var newPlaylist = [{ 'file': file, 'title':title, 'artist':artist }];
        newPlaylist.push(oldPlaylist[0]);
        jwplayer(id).load(newPlaylist);    
        
        // new playlist in position 0, current playlist in position 
    }
    
     /**
    * This function will remove one item from the playlist
    * 
    * @param index number of the item from the playlist to be removed
    * 
    * Remeber to call displayPlaylist afterremoving items
    */
    function removeFromPlaylist(id,index) 
    {
        // get current playlist
        var oldPlaylist = jwplayer(id).getPlaylist();
        var newPlaylist=[];
        for(var key in oldPlaylist) 
        {   
            if(key!=index) {   
                var item = oldPlaylist[key];
                newPlaylist.push(item);
            }
        }
        jwplayer(id).load(newPlaylist); 
        setPlaylistCookie(newPlaylist);
    }
    
    function onBeforePlay()
    {   
        if( promo.protectionDiv === 1 )
        {    

            $('.pre-roll-trailers').append('Advertisement');
            $('.pre-roll-trailers').css('z-index', '10');
            $('.pre-roll-trailers').css('font-weight', 'bold');
            $('.pre-roll-trailers').css('font-size', '20px');
            $('.pre-roll-trailers').css('font-style', 'italic');
            $('.pre-roll-trailers').css('color','#fff');
            
            promo.protectionDiv += 1;
        }
        
    }

    
     /**
    * This function will refresh playlist window. It must be called manually to prevent errors as playlist not always needs to be displayed on the screen.
    * 
    * @param playlist array of objects containing the playlist,"jwplayer().getPlaylist()" function can (and should) be passed 
    * @param target id of the div containg the playlist
    */
    function displayPlaylist(id,playlist,target) 
    {   
        //alert(playlist.toSource());
        if(target=='' || target==null) target='playlist_content';
        //Remove playlist content
        $('#'+target).html('');
        for(var key in playlist) 
        {  
            var item = playlist[key];
            if(item.title.length>15) item.title = item.title.substr(0,15)+'...'; 
            if(item.artist.length>10) item.artist = item.artist.substr(0,10)+'...'; 
            $('#'+target).append('<div class="playlist_item" id="item'+key+'">'+
                '<a href="javascript:void(0);" onclick="playItem(\''+id+'\','+key+');">'+
                    '<font color="black">'+item.title+'</font> '+
                    '<font color="grey">by</font> '+
                    '<font color="red">'+item.artist+'</font>'+
                '</a>'+
                '<a style="float:right;" href="javascript:void(0);" onclick="removeFromPlaylist(\''+id+'\','+key+');displayPlaylist(\''+id+'\',jwplayer('+id+').getPlaylist());">\n\
                    <img src="/themes/shopto/images/playlist_cross.png" height="6" style="padding:2px;"></a></div>')
            //alert(oldPlaylist[i].file);
        }
    }
    /**
    * This function will start playing previous track from the playlist.    jwplayer(id).onBeforePlayHide()
    */           
    function playPreviousTrack(id) {
        var newItem = jwplayer(id).getPlaylistItem();
        playItem(id,newItem.index-1);
    }
    
     /**
    * This function will start playing the next track from the playlist.
    */
    function playNextTrack(id) {
        var newItem = jwplayer(id).getPlaylistItem();
        playItem(id,newItem.index+1);
    }  
    /**
     * This function is used to toggle play/pause. Also it changes icon accordingly to the status
     * 
     *  @param forceStatus if passed true it will start playing now matter what the current status is, false will pause 
     */
    function play(id,forceStatus) {
       jwplayer(id).play(id,forceStatus);
       var state = jwplayer(id).getState();
       if(state=='PAUSED') $('#play_button').attr('src','/themes/shopto/images/playbtn.png');else
       $('#play_button').attr('src','/themes/shopto/images/pausebtn.png');
    }  
    /**
     * This function will stop the player and unload currently playing track.
     */
    function stop(id) {
       jwplayer(id).stop();
       $('#play_button').attr('src','/themes/shopto/images/playbtn.png');
    }  
    
     /**
    * This function will play specific element from the playlist
    * 
    * @param itemNumber index of the playlist element to be played
    */
    function playItem(id,itemNumber)
    {
        var newItem = jwplayer(id).getPlaylistItem(itemNumber);
        //It is possible to resize it if the movie has been chosen from the playlist
        //if(newItem.height) {alert('it is a movie file, I am not bothered to resize the player so have fun watching without video:P')}
        $('div.playlist_item').css('background-color','white');
        $('#item'+ itemNumber).css('background-color','#DDDDDD');
        $('#play_button').attr('src','/themes/shopto/images/pausebtn.png');
//        $('#playlist').animate({ scrollTop: $("#playlist_content").scrollTop() + (30*itemNumber) });
        //$('#currently_playing').html('Currently playing:<br>'+newItem.title);
        jwplayer(id).playlistItem(itemNumber);
     }
</script>


<script type="text/javascript">
function changeBgImage (id, halfPoint, out) 
{
    for(i=1;i<=id;i++)
    {
        if(out!=1)
        {
            image = '/themes/shopto/images/start4.jpg';
        }
        else if(halfPoint >=i)
        {
            image = '/themes/shopto/images/start1.jpg';
        }
        else if(halfPoint == i-0.5)
        {
            image = '/themes/shopto/images/start2.jpg';
        }
        else
        {
            image = '/themes/shopto/images/start3.jpg';
        }
        var rating = "Rating_"+i;
	var element = document.getElementById(rating);
	element.style.backgroundImage = "url("+image+")";
    }
}
</script>
                    </div>

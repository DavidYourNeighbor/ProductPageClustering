<script type="text/javascript">
    
var DialogLoginWidget = {
    
    
    
    registerCallback: function(id, callback)
    {
        
    },
    

   /**
    * Logs in customer via ajax
    * @param username
    * @param password
    * @param successCallback, callback function
    */
    ajaxLoginCustomer: function (username, password, successCallback)
    {
        //console.log('username: '+username+', password: '+password);
        $.ajax({
            type: 'POST',
            url: '/api/login',
            data: {
                'username': username,
                'password': password
            },
            success: function()
            {
                $('#w-customer-login').dialog('close');
                //headerBar.refresh();    // refresh header bar after customer login
                if (successCallback)
                {
                    successCallback();
                }

                window.location.reload();
            }
        });
    },
    // fix me
    open: function()
    {
        $('#w-customer-login').dialog('open');
    }

};

    
    
</script>

<div id="w-customer-login">

<div>
    <div>

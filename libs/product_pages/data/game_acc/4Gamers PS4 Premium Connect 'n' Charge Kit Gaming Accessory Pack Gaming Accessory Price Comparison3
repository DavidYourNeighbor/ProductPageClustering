<script type="text/javascript">
    var PointsBlock = {
        change: function(element){
            el = $('#yw1');
            el.removeClass('w-pointsblock-gold');
            el.removeClass('w-pointsblock-bronze');
            el.removeClass('w-pointsblock-silver');
            
            $('#w-membership-bronze').removeClass('w-pointsblock-bronze');
            $('#w-membership-silver').removeClass('w-pointsblock-silver');
            $('#w-membership-gold').removeClass('w-pointsblock-gold');
            
            switch($(element).attr('id'))
            {
                case 'w-membership-bronze':
                    el.addClass('w-pointsblock-bronze');
                    $(element).addClass('w-pointsblock-bronze');
                    $('div.g-point-half div.label').text('Reward Points');
                    $('div.g-value div.label').text('Points Value');
                    break;
                case 'w-membership-silver':
                    el.addClass('w-pointsblock-silver');
                    $(element).addClass('w-pointsblock-silver');
                    $('div.g-point-half div.label').text('Member Discount');
                    $('#points-block-points').text('£'+parseFloat('0.6').toFixed(2));
                    $('div.g-value div.label').text('Member Price');
                    $('#points-block-points-value').text('£'+parseFloat('19.25').toFixed(2));
                    break;
                case 'w-membership-gold':
                    el.addClass('w-pointsblock-gold');
                    $(element).addClass('w-pointsblock-gold');
                    $('div.g-point-half div.label').text('Member Discount');
                    $('#points-block-points').text('£'+parseFloat('0.99').toFixed(2));
                    $('div.g-value div.label').text('Member Price');
                    $('#points-block-points-value').text('£'+parseFloat('18.86').toFixed(2));
                    break;
            }

            // resize points after changing them
            PointsBlock.fixSize();
        },

        fixSize: function(){
            var points = $('#points-block-points');
            var cash = $('#points-block-points-value');

            var maxWidth = 120;
            
            points.css('font-size', 50);
            fontSize = 50;
            while ( parseInt((points.css('width')) + parseInt(cash.css('width'))) > maxWidth )
            {
                //console.log('cash: '+cash.css('width') + 'points: '+points.css('width'));
                var fontSize = parseInt(points.css('font-size'));
                points.css('font-size', fontSize - 1);
            }
            $('div.g-point-half').css('line-height', fontSize+'px');
            $('div.g-value').css('line-height', '51px');
        }
    }
</script>


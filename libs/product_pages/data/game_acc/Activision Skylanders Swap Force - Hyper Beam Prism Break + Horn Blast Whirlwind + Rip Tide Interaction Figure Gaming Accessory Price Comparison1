<script>
    window.fbAsyncInit = function() {
        FB.init({
            appId      : '390930040978042', // App ID
            channelUrl : '//www.coolshop.co.uk/facebook-channel-file/', // Channel for for X-server communication
            status     : false, // check login status
            cookie     : true, // enable cookies to allow the server to access the session
            xfbml      : true  // parse XFBML
        });

        // Get Login Status and save appropriate data
        FB.getLoginStatus(function(response) {
            if (response.status == 'connected')
            {
                // User has auth'd our App and is logged into Facebook
                var data = {
                    csrfmiddlewaretoken: 'HEvNyqNJNNTUyqgAbwn2SnmRRBE1d6H9',
                    accessToken: response.authResponse['accessToken'],
                    userID: response.authResponse['userID'],
                    expiresIn: response.authResponse['expiresIn']
                }
                $.ajax({
                    type: 'POST',
                    url: '/facebook/check-login/',
                    data: data
                });
            }
            // We do not need to care about the rest -- for now
        });
    };

    function fb_has_permission(permission) {
        FB.api('/me/permissions', function(response) {
            return (permission in response['data'][0]);
        });
    }

    function fb_bought(product)
    {
        FB.api(
            'me/coolshop_co_uk:buy',
            'post',
            {
                product: product
            },
            function(response) {}
        );
    }

    function fb_hook(callback)
    {
        FB.login(function(response) {
            if (response.authResponse)
            {
                var data = {
                    csrfmiddlewaretoken: 'HEvNyqNJNNTUyqgAbwn2SnmRRBE1d6H9',
                    accessToken: response.authResponse['accessToken'],
                    userID: response.authResponse['userID'],
                    expiresIn: response.authResponse['expiresIn']
                }
                $.ajax({
                    type: 'POST',
                    url: '/facebook/hook/',
                    async: false,
                    data: data,
                    success: function(data) {
                        data = $.parseJSON(data);
                        data['response'] = response
                        callback(data);
                    }
                });
            }
        },
        {scope: 'email,user_birthday,user_location,user_relationships,user_interests,publish_actions'});
    }

    function fb_unhook(callback)
    {
        var data = {
            csrfmiddlewaretoken: 'HEvNyqNJNNTUyqgAbwn2SnmRRBE1d6H9'
        }
        $.ajax({
            type: 'POST',
            url: '/facebook/unhook/',
            data: data,
            success: function(data) {
                data = $.parseJSON(data);
                callback(data);
            }
        });
    }

    function fb_login(callback)
    {
        FB.login(function(response) {
            if (response.authResponse)
            {
                var data = {
                    csrfmiddlewaretoken: 'HEvNyqNJNNTUyqgAbwn2SnmRRBE1d6H9',
                    accessToken: response.authResponse['accessToken'],
                    userID: response.authResponse['userID'],
                    expiresIn: response.authResponse['expiresIn']
                }
                $.ajax({
                    type: 'POST',
                    url: '/facebook/login/',
                    data: data,
                    success: function(data) {
                        data = $.parseJSON(data);
                        if (callback)
                            callback();
                        else if ('created' in data)
                            window.location.replace('/my/welcome');
                        else
                            window.location.replace('/my/account/profile');
                    }
                });
            }
        },
        {scope: 'email,user_birthday,user_location,user_relationships,user_interests,publish_actions'});
    }

    // Load the SDK Asynchronously
    (function(d){
        var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
        if (d.getElementById(id)) {return;}
        js = d.createElement('script'); js.id = id; js.async = true;
        js.src = "//connect.facebook.net/en_US/all.js";
        ref.parentNode.insertBefore(js, ref);
    }(document));
</script>

<script src="https://cdn.klarna.com/external/toc/v1.0/js/klarna.terms.min.js"></script>
<script src="https://static.coolshop.com/js/jquery.form.js" type="text/javascript"></script>

<script>
    // Auto-complete start
    $('#term').on('keyup', function (e) {
        if (e.keyCode == 27) { // [Esc]
            $('#suggestions').fadeOut();
            $('.suggestions li').removeClass('selected');
        }
        if ((e.keyCode != 13) && (e.keyCode != 38) && (e.keyCode != 40) && (e.keyCode != 27))
        {
            if($('#term').val().length == 0) {
                $('#suggestions').fadeOut();
            }
            else {
                $.get($('#term').attr('ajax'), {term: $('#term').val() }, function (data) {
                    if(data != '0') {
                        $('#suggestions').fadeIn();
                        $('#suggestions').html(data);
                    }
                    else {
                        $('#suggestions').fadeOut();
                    }
                });
            }
        }
        $(document).on("click", function (e) {
            e.stopPropagation();
            $('#suggestions').fadeOut();
        });
    });
    // Auto-complete end

    // Newsletter signup
    $('#newslettersubscribe').on('click', function(event) {
        event.preventDefault();
        $('#newsletter-optin-checkbox').prop('checked', true);
        var $form = $('#newsletterform'),
            $inputs = $form.find("input, select, button, textarea"),
            serializedData = $form.serialize();
        $.ajax({
            url: '/newsletter/footer',
            type: 'post',
            data: serializedData,
            success: function(data) {
                $('#newsletterconfirm').text(data);
            }
        });
    });
    // Newsletter signup end
</script>


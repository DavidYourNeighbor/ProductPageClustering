<script>
    $('.navigation').on('click', function(event) {
        event.preventDefault();
        window.scrollTo(0, 0);
        $('.mobileNavigation').toggle();
    });
    $('.mobileSearchNav').on('click', function(event) {
        event.preventDefault();
        $('.mobileSearch').toggle();
        $('.mobileSearch input').focus();
    });

    if( Modernizr.mq('(max-width: 480px)') && Modernizr.touch ) {
        $('a').on('mouseover', function(event) {
            window.location = $(this).attr('href');
        });
    }
</script>




<script type="text/javascript">
    

    $(document).ready(function(){
        $("#discsignup-activate").click(
            function(){
                $("#discsignup-container").toggle();
                $(this).toggle();
            });


        $("#discsignup-form").ajaxForm({
        	// target identifies the element(s) to update with the server response
        target: '#discsignup-msg',
        // success identifies the function to invoke when the server response
        // has been received; here we apply a fade-in effect to the new content
        success: function() {
        	$('#discsignup-msg').addClass("text--orange");
            $('#discsignup-msg').fadeIn('slow');
        }
    });


        
    });

</script>

<script type="text/javascript">

    if( Modernizr.mq('(max-width: 480px)') ) {
        $('#product-cover').clone().appendTo('#sku');
        $('#product-cover').remove();

        $('#product-right-column').clone().appendTo('#mobilePrice').css('float', 'none');
        $('#product-right-column').remove();
    }


	$(document).ready(function () {
        
        comments = $('#comments-container');
        $.get(comments.data('url'), function(response) {
            comments.html(response);
            commentsReplyLoad();
        });
        

        

        $("a[href^=\"#\"]").on("click", function(e) {
            var target;
            e.preventDefault();
            target = this.hash;
            console.log(target);

            return $("html, body").animate({
                scrollTop: $(target).offset().top - 100
            }, 1000);
        });


    });

</script>


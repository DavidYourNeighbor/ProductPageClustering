<script type="text/javascript">
(function($) {
  $(function() {
  $('#footerLangList a[href*="'+location.host+'"]').remove();
  var len = document.getElementById('footerLangList').getElementsByTagName('a').length;
  var br=Math.ceil(len/Math.ceil(len/6));
  for (var i=0; i<len;i++) {
    if(i===br) {
      $('#footerLangList a').eq(i).before('<br />');
      break;
    }
  }
  });
})(jQuery);
</script>


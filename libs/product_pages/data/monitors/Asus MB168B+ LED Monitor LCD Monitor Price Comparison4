document.write('<link href="http://www.cclonline.com/content/ebay/css/ccl-listing-styles.css" rel="stylesheet" type="text/css" />'); 
function getFirstImageNode(nodes) {
for (var i=0; i<nodes.length; i++) {
if (nodes[i].nodeType == 1)
return nodes[i];
}
return null;
}
function removeClass(haystack, needle) {
var index = haystack.indexOf(needle)
if (index != -1)
return haystack.substring(0, index - 1) + haystack.substring(index + needle.length, haystack.length);
else
return haystack;
}
function showImage(thumbElement) 
{
var ai = document.getElementById('ActiveImage');
ai.src = 'http://www.cclonline.com' + thumbElement;
}
function showTab(headerLink, bodyID) {
var tabHeader = document.getElementById('TabHeader');
for (var i=0; i<tabHeader.childNodes.length; i++) {
if (tabHeader.childNodes[i].nodeType == 1 && tabHeader.childNodes[i].className.indexOf('tab') != -1)
tabHeader.childNodes[i].className = removeClass(tabHeader.childNodes[i].className, 'selected');
} 
headerLink.className += ' selected';
var tabBody = document.getElementById('TabBody');
for (var i=0; i<tabBody.childNodes.length; i++) {
if (tabBody.childNodes[i].nodeType == 1 && tabBody.childNodes[i].className.indexOf('tab') != -1)
tabBody.childNodes[i].className = removeClass(tabBody.childNodes[i].className, 'selected');
}
var bodyElement = document.getElementById(bodyID);
bodyElement.className += ' selected';
}



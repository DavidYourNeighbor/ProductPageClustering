							function picOnError(){
								var elemThr = document.getElementById('icThrImg');
								var pic = document.getElementById('icImg'); 
								elemThr.src='http://p.ebaystatic.com/aw/pics/cmp/icn/iconImgNA_96x96.gif'; 

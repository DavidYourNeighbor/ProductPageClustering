<script type="text/javascript">
/*<![CDATA[*/

function initPageEvents() {
	$(".modal").colorbox({
		onComplete: function() {
		    ACC.common.refreshScreenReaderBuffer();
		},
		onClosed: function() {
			ACC.common.refreshScreenReaderBuffer();
		}
	});
	$("#carousel_alternate img").click(function() {
		$(this).closest("li").addClass("current").siblings().removeClass("current");
	});
    CloudZoom.quickStart();
	$(".gallery-video").click(function(e) {
		$("img.main-image").hide();
		$("#carousel_alternate li").removeClass("current");
		$(this).parent().addClass("current");		
		$("#primaryImageIframe").attr("src", this.href).show();
		return false;
	});
	$(".cloudzoom-gallery").click(function(e) {
		$("img.main-image").show();
		$("#carousel_alternate li a.gallery-video").parent().removeClass("current");
		$("#primaryImageIframe").hide();
		return true;
	});

	$("#Size").change(function () {
		var url = "";
		var selectedIndex = 0;
		$("#Size option:selected").each(function () {
			url = $(this).attr('value');
			selectedIndex = $(this).attr("index");
		});
		if (selectedIndex != 0) {
			window.location.href=url;
		}
	});

	$("#variant").change(function () {
		var url = "";
		var selectedIndex = 0;
		
		$("#variant option:selected").each(function () {
			url = $(this).attr('value');
			selectedIndex = $(this).attr("index");
		});
		if (selectedIndex != 0) {
			window.location.href=url;
		}
	});

	//get cross sell and up sell
	$.ajax({
		  url: "/thefilter/showCrossSellAndUpSellProducts?productCode=A91LN",
		  success: function(data) {
			onFilterSuccess($("#crossSellUpSell"), data);
		  },
		  cache: false
	});
	
}

$(document).ready(function() {
	
	initPageEvents();

});

/*]]>*/
</script><a class="livechat-tab" title="Click for Live Chat" href="https://ussco.activepoint.com/chatmaplinDec2013/"><i class="icon-comment"></i> <span>Live chat</span></a></body>


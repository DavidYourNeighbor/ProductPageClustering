<script type="text/javascript">
//<![CDATA[
	$(document).ready(function() {
		$.jsonp({
			url: "http://api.buyitdirect.co.uk/WSVersionPage/WSVersionPage.svc/GetBundlesDataInJSON",
			data: { "xmlPayload" : "<API><StockCode>HT231HPB</StockCode><Eligibility><BrandID>6</BrandID><ChannelID>1</ChannelID><BasketValue>0</BasketValue><VATExclusive>1</VATExclusive><DisplayCurrencyCode>GBP</DisplayCurrencyCode><SuppressCashback>0</SuppressCashback></Eligibility></API>" },
			timeout: 100000,
			success: JsonPCallBack,
			callbackParameter: "method",
			error: function (xOptions, textStatus) {
				$("ul#mainnav > li:first").remove();// this; the xOptions object or xOptions.context if provided
				$("div.productTabs > div.tabcontent > div > div#bundleEngine").hide();
				//showTab($("ul#mainnav li:first a")[0]);
				//initTabs();
			}
		});
	});

    

	function JsonPCallBack(result){
		var data = $.parseJSON(result.d);
		
        if (data == undefined) {
			$("ul#mainnav li:nth-child(2)").remove();
			//$("ul#mainnav > li:first").remove();
			$("div.productTabs > div.tabcontent > div > div#bundleEngine").hide();
            return;
        }

        if (data.BundleList == undefined) {
			$("ul#mainnav li:nth-child(2)").remove();
			//$("ul#mainnav > li:first").remove();
			$("div.productTabs > div.tabcontent > div > div#bundleEngine").hide();
            return;
        }

		if(data.BundleList.length > 0) {
			_gaq.push(['_trackEvent','Bundle_Version','Render', 'Quantity of bundles returned for version ' + data.BundleList[0].BundleLines[0].Product.StockCode, data.BundleList.length])
			var rowIndex = 0;
			var bundleListCount = data.BundleList.length;
			var strBundleHTML = collectTabHTML( data.BundleList.shift(), rowIndex, bundleListCount);

			// write bundle html to the necessary DIV
			$("body div#wrapper > div#contentwrapper > div#contentcolumn > form > table.versionPageTable > tbody > tr:eq(2) > td").append(strBundleHTML) 
			/*.children("div.bundleRow").corner({
				tl: { radius: 8 },
				tr: { radius: 8 },
				bl: { radius: 8 },
				br: { radius: 8 }});*/
		}
		
		if(data.BundleList.length == 0) {
			$("ul#mainnav li:nth-child(2)").remove();
			//$("ul#mainnav li:first").remove();
			$("div.productTabs > div.tabcontent > div > div#bundleEngine").hide();
			/*$("ul#mainnav li:first").hide();
			window.location.hash = 'maindesc';*/
		} else {
			var strBundleHTML2;
			var divBundleEngine = $('div#bundleEngine');
			$.each(data.BundleList, function (i, bundleList) {
				divBundleEngine.append(collectTabHTML(this, ++rowIndex, bundleListCount));

				//strBundleHTML += '<br />';
				divBundleEngine.append(document.createElement("br"));

				/*if(i==0)
					strBundleHTML2 = collectTabHTML(this, ++rowIndex, bundleListCount);
				else
					wrapperDiv.insertBefore( document.createElement("br"), bundleDescriptionDiv.nextSibling );
					strBundleHTML2.appendChild(collectTabHTML(this, ++rowIndex, bundleListCount));*/
				//strBundleHTML2 += collectTabHTML(this, ++rowIndex, bundleListCount);
			});			

			// write bundle html to the necessary DIV
			//$('div#bundleEngine').append(strBundleHTML2);
		}
		//initTabs();
	}
	
	function collectTabHTML(data, rowIndex, bundleListCount)
	{
		//var strBundleHTML = '';
		
		//strBundleHTML += '<div class="bundleRow">';
		var wrapperDiv = document.createElement("div");
		wrapperDiv.className = "bundleRow";

		var bundleObjectID = data.BundleObjectID;
		var bundleNumber = data.BundleNumber;
		var bundleHash = data.Hash;

		if(rowIndex==0) {
			// Bundle product names on top left-hand side of row
			//strBundleHTML += '<div style="float:left;" class="bundleTitle"><H3>Buy these items together and save even more!</H3></div>' + (bundleListCount>1?'<div style="float:right;margin-top: 12px;margin-bottom: 10px;" class="bundleTitle"><a style="color:#000C43; font-weight:bold;" href="#bundleEngine" onClick="showTab($(\'ul#mainnavtabbed > li > a[href=#bundleEngine]:first\')[0])">See more packages...</a></div><div style="clear:both;"></div>':'');
			var togetherDiv = document.createElement("div");
			togetherDiv.className = "bundleTitle";
			togetherDiv.style.cssText = "float:left;";
			var togetherH3 = document.createElement("H3");
			var togetherH3Content = document.createTextNode("Buy these items together and save even more!");
			togetherH3.appendChild(togetherH3Content);
			togetherDiv.appendChild(togetherH3);
			wrapperDiv.appendChild(togetherDiv);
			if(bundleListCount>1)
			{
				var morePackagesDiv = document.createElement("div");
				morePackagesDiv.className = "bundleTitle";
				morePackagesDiv.style.cssText = "float:right;margin-top: 12px;margin-bottom: 10px;";
				var morePackagesA = document.createElement("a");
				morePackagesA.style.cssText = "color:#000C43; font-weight:bold;"
				morePackagesA.href = "#bundleEngine";
				morePackagesA.onclick = function() { showTab($('ul#mainnavtabbed > li > a[href=#bundleEngine]:first')[0]) };
				var morePackagesAContent = document.createTextNode("See more packages...");
				morePackagesA.appendChild(morePackagesAContent);
				morePackagesDiv.appendChild(morePackagesA);
				wrapperDiv.appendChild(morePackagesDiv);
				var morePackagesClearDiv = document.createElement("div");
				morePackagesClearDiv.style.cssText = "clear:both;";
				wrapperDiv.appendChild(morePackagesClearDiv);
			}
		} else {
			// Bundle product names on top left-hand side of row
			//strBundleHTML += '<div class="bundleDescription">';
			var bundleDescriptionDiv = document.createElement("div");
			bundleDescriptionDiv.className = "bundleDescription";

			// Loop through each BundleLine
			$.each(data.BundleLines, function (j, bundleLine) {
				if (bundleLine.Visible) {
					// loop through products. If product is Hero, wrap in H3 tag
					if (j == 0) {
						// Assuming the first item is the Hero item
						//strBundleHTML += '<H3>' + bundleLine.Product.Description + '</H3>';
						var bundleDescriptionH3 = document.createElement("H3");
						var bundleDescriptionH3Content = document.createTextNode(bundleLine.Product.Description);
						bundleDescriptionH3.appendChild(bundleDescriptionH3Content);
						bundleDescriptionDiv.appendChild(bundleDescriptionH3);
					}
					else {
						//strBundleHTML += '<B> + </B>';
						//strBundleHTML += bundleLine.Product.Description;
						bundleDescriptionBold = document.createElement("B");
						var bundleDescriptionBoldContent = document.createTextNode(" + ");
						bundleDescriptionBold.appendChild(bundleDescriptionBoldContent);
						bundleDescriptionContent = document.createTextNode(bundleLine.Product.Description);
						bundleDescriptionDiv.appendChild(bundleDescriptionBold);
						bundleDescriptionDiv.appendChild(bundleDescriptionContent);
					}
				}
			});
			//strBundleHTML += '</div><!-- div.bundleDescription -->';
			wrapperDiv.appendChild(bundleDescriptionDiv);
		}
			
		// Bundle product images on lower left-hand side of row
		//strBundleHTML += '<div class="bundleDetails">';
		var bundleDetailsDiv =  document.createElement("div");
		bundleDetailsDiv.className = "bundleDetails";
		if (rowIndex==0) {
			bundleDetailsDiv.className += ' bundleDetailsHero';
		}

		//strBundleHTML += '<div class="bundleImages-prev-horizontal">';
		var bundleImagesPrevHorizontalDiv =  document.createElement("div");
		bundleImagesPrevHorizontalDiv.className = "bundleImages-prev-horizontal";
		//strBundleHTML += '</div><!-- bundleImages-prev-horizontal -->';
		bundleDetailsDiv.appendChild(bundleImagesPrevHorizontalDiv);
		//strBundleHTML += '<div class="bundleImages-next-horizontal">';
		var bundleImagesNextHorizontalDiv =  document.createElement("div");
		bundleImagesNextHorizontalDiv.className = "bundleImages-next-horizontal";
		//strBundleHTML += '</div><!-- bundleImages-next-horizontal -->';
		bundleDetailsDiv.appendChild(bundleImagesNextHorizontalDiv);

		//strBundleHTML += '<div class="bundleImagesClip">';
		var bundleImagesClipDiv =  document.createElement("div");
		bundleImagesClipDiv.className = "bundleImagesClip";
		if (rowIndex==0) {
			bundleImagesClipDiv.className += ' bundleImagesClipHero';
		}
		//strBundleHTML += '<div class="bundleImages">';
		var bundleImagesDiv =  document.createElement("div");
		bundleImagesDiv.className = "bundleImages";

		var addToBasketURL = '/asp/Basket.asp?'; // string used to build up url

		addToBasketURL += 'B=' + bundleObjectID + '_' + bundleNumber;
		addToBasketURL += '&H=' + bundleHash;
		addToBasketURL += '&numLineCount=' + data.BundleLines.length;
		addToBasketURL += '&U=/Hannspree_HannsG_23_INCH_wide__TOUCH__1920_x_1080__5ms__DVI__VGA__HDMI__SPE_HT231HPB/version.asp';
		//addToBasketURL += '&U=' + "/Toshiba_Satellite_Pro_L450D-14Z_Windows_7_Laptop_PSLY6E-00F006EN/version.asp";

		var visibleBundleLinesLength = 0;
		// Loop through each BundleLine
		// Create a picture and a plus sign for each bundle line item
		$.each(data.BundleLines, function (j, bundleLine) {
			if (bundleLine.Visible) {
				visibleBundleLinesLength++;
				if (j != 0) {
					//strBundleHTML += '<span class="plus"> + </span>';
					var plusSpan = document.createElement("span");
					plusSpan.className = "plus";
					var plusSpanContent = document.createTextNode(" + ");
					plusSpan.appendChild(plusSpanContent);
					bundleImagesDiv.appendChild(plusSpan);
				}
				var bundleLineItemClass = 'bundleLineItem';
				if (bundleLine.Product.IsHeroItem == 1) {
					bundleLineItemClass += ' hero';
				}
				var itemURL = "/MVCLightbox"; // TODO: Build up the url to each product within the bundle line
				var linkToProduct = itemURL + "/Version/Details/" + bundleLine.Product.WebProdID; // TODO: Replace with a call to a lightbox
				//strBundleHTML += '<span class="' + bundleLineItemClass + '" >';// onclick="' + linkToProduct + '"
				//strBundleHTML += '<img alt="' + bundleLine.Product.Description + '" src="../imageupload/showimage.aspx?id=' + bundleLine.Product.ImageID + '&width=75&height=75" />';
				//strBundleHTML += '</span>';
				var bundleLineItemSpan = document.createElement("span");
				bundleLineItemSpan.className = bundleLineItemClass;
				//bundleLineItemSpan.href = linkToProduct;
				
				var bundleLineItemSpanJQuery = $(bundleLineItemSpan);
				bundleLineItemSpanJQuery.fancybox({
					'enableEscapeButton' : true,
					'centerOnScroll' : true,
					'href' : linkToProduct,
					'autoScale' : false,
					'autoDimensions' : false,
					'width' : 660,
					'height' : 'auto'
				});
				


				var bundleLineItemImg = document.createElement("img");
				bundleLineItemImg.alt = bundleLine.Product.Description;
				bundleLineItemImg.src = "../imageupload/showimage.aspx?id=" + bundleLine.Product.ImageID + "&width=75&height=75";
				bundleLineItemSpan.appendChild(bundleLineItemImg);
				bundleImagesDiv.appendChild(bundleLineItemSpan);
			}
			var variant = j + 1;
			addToBasketURL += '&V' + variant + '=' + bundleLine.Product.WebProdID;
			addToBasketURL += '&Q' + variant + '=' + bundleLine.Quantity;
			addToBasketURL += '&PI' + variant + '=' + bundleLine.Product.ItemPriceIncVAT;
			addToBasketURL += '&PX' + variant + '=' + bundleLine.Product.ItemPriceExVAT;
		});

		//strBundleHTML += '</div><!-- div.bundleImages -->';
		bundleImagesClipDiv.appendChild(bundleImagesDiv);
		//strBundleHTML += '</div><!-- div.bundleImagesClip -->';
		bundleDetailsDiv.appendChild(bundleImagesClipDiv);
		//strBundleHTML += '</div><!-- div.bundleDetails -->';
		wrapperDiv.appendChild(bundleDetailsDiv);

		if(rowIndex==0 && visibleBundleLinesLength > 4 || visibleBundleLinesLength > 6)
		{
			var bundleImagesClipDivJQuery = $(bundleImagesClipDiv);
			var bundleImagesDivJQuery = bundleImagesClipDivJQuery.children("div.bundleImages:first");
			// unordered list's left margin
			//spanPadding = 0;//15;

			//Get menu width
			//var bundleImagesDivWidth = 555;//bundleDetailsDivJQuery.width();

			//Remove scrollbars
			//bundleImagesClipDivJQuery.css({overflow: 'hidden'});

			//Find last image container
			//var lastSpan = bundleImagesDivJQuery.children('span.bundleLineItem:last-child');

			//When user move mouse over menu
			/*bundleDetailsDivJQuery.mousemove(function(e){
				//As images are loaded bundleImagesDiv width increases,
				//so we recalculate it each time
				var bundleImagesDivWidth = lastSpan[0].offsetLeft + lastSpan.outerWidth() + spanPadding;

				var left = (e.pageX - bundleDetailsDivJQuery.offset().left) * (bundleImagesDivWidth-bundleDetailsDivWidth) / bundleDetailsDivWidth;
				bundleDetailsDivJQuery.scrollLeft(left);
			});*/
			SetNavigationDisplay(bundleImagesDivJQuery, visibleBundleLinesLength);
			bundleImagesPrevHorizontalDiv.className = "bundleImages-prev-horizontal bundleImages-prev-horizontal-active";
			bundleImagesNextHorizontalDiv.className = "bundleImages-next-horizontal bundleImages-next-horizontal-active";
			bundleImagesPrevHorizontalDiv.onclick = function() { PreviousSlide(bundleImagesDivJQuery, visibleBundleLinesLength) };
			bundleImagesNextHorizontalDiv.onclick = function() { NextSlide(bundleImagesDivJQuery, visibleBundleLinesLength) };
		}


		// Pricing on right-hand side of row
		//strBundleHTML += '<div class="bundlePrice">';
		var bundlePriceDiv = document.createElement("div");
		bundlePriceDiv.className = "bundlePrice";
		//strBundleHTML += '<div class="bundleSavingMessage">' + data.SavingMessage + '</div><!-- div.bundleSavingMessage -->';
		var bundleSavingMessageDiv = document.createElement("div");
		bundleSavingMessageDiv.className = "bundleSavingMessage";
		var bundleSavingMessageDivContent = document.createTextNode(data.SavingMessage);
		bundleSavingMessageDiv.appendChild(bundleSavingMessageDivContent);
		bundlePriceDiv.appendChild(bundleSavingMessageDiv);
		//strBundleHTML += '<img id="imgPrice_Bundle' + rowIndex.toString() + '" alt="' + data.DisplayCurrencySymbol + data.BundlePriceDisplay + '" src="../commoncontent/showprice.aspx?p=' + Math.round(data.BundlePriceDisplay*100) + '&amp;s=28&amp;&c=' + ((data.DisplayCurrencySymbol == '\u20AC')?'e':'p') + '&amp;col=red">';
		var imgPrice = document.createElement("img");
		imgPrice.id = "imgPrice_Bundle" + rowIndex.toString();
		imgPrice.alt = data.DisplayCurrencySymbol + data.BundlePriceDisplay;
		imgPrice.src = "../commoncontent/showprice.aspx?p=" + Math.round(data.BundlePriceDisplay*100) + "&s=28&c=" + ((data.DisplayCurrencySymbol == '\u20AC')?'e':'p') + "&col=red";
		bundlePriceDiv.appendChild(imgPrice);
		//strBundleHTML += '<div class="bundleStrikethroughPriceIncVAT">(' + data.DisplayCurrencySymbol + data.BundleStrikethroughPriceDisplay.toFixed(2) + ' if bought separately)</div><!-- div.bundleStrikethroughPriceIncVAT -->';
		var bundleStrikethroughPriceIncVATDiv = document.createElement("div");
		bundleStrikethroughPriceIncVATDiv.className = "bundleStrikethroughPriceIncVAT";
		var bundleStrikethroughPriceIncVATDivContent = document.createTextNode("(" + data.DisplayCurrencySymbol + data.BundleStrikethroughPriceDisplay.toFixed(2) + " if bought separately)");
		bundleStrikethroughPriceIncVATDiv.appendChild(bundleStrikethroughPriceIncVATDivContent);
		bundlePriceDiv.appendChild(bundleStrikethroughPriceIncVATDiv);
		//strBundleHTML += '<div class="addToBasket">';
		//strBundleHTML += '<a class="buyBundleButton" href="' + addToBasketURL + '" ><span>Add these to basket</span></a>';
		//strBundleHTML += '</div><!-- div.addToBasket -->';
		var addToBasketDiv = document.createElement("div");
		addToBasketDiv.className = "addToBasket";
		var addToBasketA = document.createElement("a");
		addToBasketA.className = "buyBundleButton";
		addToBasketA.href = addToBasketURL;
		var addToBasketASpan = document.createElement("span");
		var addToBasketASpanContent = document.createTextNode("Add these to basket");
		addToBasketASpan.appendChild(addToBasketASpanContent);
		addToBasketA.appendChild(addToBasketASpan);
		addToBasketDiv.appendChild(addToBasketA);
		bundlePriceDiv.appendChild(addToBasketDiv);
		//strBundleHTML += '</div><!-- div.bundlePrice -->';
		wrapperDiv.appendChild(bundlePriceDiv);

		if(rowIndex==0) {
			// Bundle product names on top left-hand side of row
			//strBundleHTML += '<div class="bundleDescription">';
			var bundleDescriptionDiv = document.createElement("div");
			bundleDescriptionDiv.className = "bundleDescription bundleDescriptionHero";

			// Loop through each BundleLine
			$.each(data.BundleLines, function (j, bundleLine) {
				if (bundleLine.Visible) {
					if (j != 0) {
						//strBundleHTML += '<B> + </B>';
						bundleDescriptionDivBold = document.createElement("b");
						var bundleDescriptionDivBoldContent = document.createTextNode(" + ");
						bundleDescriptionDivBold.appendChild(bundleDescriptionDivBoldContent);
						bundleDescriptionDiv.appendChild(bundleDescriptionDivBold);

					}
					//strBundleHTML += bundleLine.Product.Description;
					var bundleDescriptionDivContent = document.createTextNode(bundleLine.Product.Description);
					bundleDescriptionDiv.appendChild(bundleDescriptionDivContent);
				}
			});
			//strBundleHTML += '</div><!-- div.bundleDescription -->';
			wrapperDiv.appendChild(bundleDescriptionDiv);
		}


		//strBundleHTML += '</div><!-- div.bundleRow -->';
		
		//if(rowIndex!=0) {
			//strBundleHTML += '<br />';
			//wrapperDiv.insertBefore( document.createElement("br"), wrapperDiv.nextSibling );
			//wrapperDiv.appendChild(document.createElement("br"));
		//}

		//return strBundleHTML;
		return wrapperDiv;
	}

	function NextSlide(bundleImagesDivJQuery, bundleLinesLength) {
		if(!EnableSliding)
			return;
		EnableSliding = false;
		// get the current margin and subtract the slide width
		var newMargin = CurrentMargin(bundleImagesDivJQuery) - SlideWidth;

		// slide the wrapper to the left to show the next panel at the set speed. Then set the nav display on completion of animation.
		bundleImagesDivJQuery.animate({ marginLeft: newMargin }, SlideSpeed, function () { SetNavigationDisplay(bundleImagesDivJQuery, bundleLinesLength); EnableSliding = true; });
	}

	function PreviousSlide(bundleImagesDivJQuery, bundleLinesLength) {
		if(!EnableSliding)
			return;
		EnableSliding = false;
		// get the current margin and subtract the slide width
		var newMargin = CurrentMargin(bundleImagesDivJQuery) + SlideWidth;

		// slide the wrapper to the right to show the previous panel at the set speed. Then set the nav display on completion of animation.
		bundleImagesDivJQuery.animate({ marginLeft: newMargin }, SlideSpeed, function () { SetNavigationDisplay(bundleImagesDivJQuery, bundleLinesLength); EnableSliding = true; });
	}

	var SlideWidth = 115;
    var SlideSpeed = 500;
	var EnableSliding = true;

    /*$(document).ready(function () {
        // set the prev and next buttons display
        SetNavigationDisplay();
    });*/

    function CurrentMargin(bundleImagesDivJQuery) {
        // get current margin of slider
        var currentMargin = /*$("#slider-wrapper")*/bundleImagesDivJQuery.css("margin-left");
		if(currentMargin == "")
			currentMargin = 0;

        // first page load, margin will be auto, we need to change this to 0
        if (currentMargin == "auto") {
            currentMargin = 0;
        }

        // return the current margin to the function as an integer
        return parseInt(currentMargin);
    }

    function SetNavigationDisplay(bundleImagesDivJQuery, bundleLinesLength) {
        // get current margin
        var currentMargin = CurrentMargin(bundleImagesDivJQuery);
		var bundleImagesClipDivJQuery = bundleImagesDivJQuery.parent("div.bundleImagesClip");

        // if current margin is at 0, then we are at the beginning, hide previous
        if (currentMargin == 0) {
			bundleImagesClipDivJQuery.prevAll("div.bundleImages-prev-horizontal").hide();
            //$("#PreviousButton").hide();
        }
        else {
			bundleImagesClipDivJQuery.prevAll("div.bundleImages-prev-horizontal").show();
            //$("#PreviousButton").show();
        }

        // get wrapper width
        //var wrapperWidth = $("#slider-wrapper").width();
		var wrapperWidth = 670;
		// unless hero
		if(bundleImagesClipDivJQuery.hasClass('bundleImagesClipHero'))
			wrapperWidth = 440;

		var plusWidth = 20;
		var ClipWidth = SlideWidth * bundleLinesLength - plusWidth;

        // turn current margin into postive number and calculate if we are at last slide, if so, hide next button
        if ((currentMargin * -1) + wrapperWidth == ClipWidth) {//(wrapperWidth - SlideWidth)) {
			bundleImagesClipDivJQuery.prevAll("div.bundleImages-next-horizontal").hide();
            //$("#NextButton").hide();
        }
        else {
			bundleImagesClipDivJQuery.prevAll("div.bundleImages-next-horizontal").show();
            //$("#NextButton").show();
        }
    }
//]]>
</script>

	

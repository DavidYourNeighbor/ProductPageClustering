        setGeoIPDependentBehavioursSD();
        function setGeoIPDependentBehavioursSD() {
            $(document).ready(function () {

                try {
                    //display local country phone number
                    var searchString = "united kingdom";

                    if (searchString.indexOf(geoip_country_name().toLowerCase()) > -1) {
                        document.getElementById("phone").style.backgroundImage = 'url("/skin_default/template_images/freephone.gif")';
                    }
                    else {
                        //display phone number outside UK
                        document.getElementById("phone").style.backgroundImage = 'url("/skin_default/template_images/headerphone-alt.gif")';
                    }
                }
                catch (err) { }
            });
        }
</script>


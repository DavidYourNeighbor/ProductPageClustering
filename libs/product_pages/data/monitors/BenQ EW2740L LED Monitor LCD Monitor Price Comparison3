<script language="javascript" type="text/javascript">
    var geoipSciptLoadStateChange = false;
    if (getGeoIPCookieContents() == null) {
        var a = document.createElement("script");
        var b = document.getElementsByTagName('script')[0];
        a.src = "http://api.buyitdirect.co.uk/geolocation/GeoIP/jsdetails";
        a.async = true; a.type = "text/javascript"; b.parentNode.insertBefore(a, b);
        // IE
        a.onreadystatechange = function () {
            if ((a.readyState === 'loaded' || a.readyState === 'complete') && geoipSciptLoadStateChange == false) {
                geoipSciptLoadStateChange = true;
                setGeoIPDependentBehavioursLD();
                try {
                    setGeoIPCookieContents(geoip_city(), geoip_region_name(), geoip_country_name(), 2);
                }
                catch (err) { }
            }
        };
        // Mozilla & IE9+
        a.onload = function () {
            if (geoipSciptLoadStateChange == false) {
                geoipSciptLoadStateChange = true;
                setGeoIPDependentBehavioursLD();
                try {
                    setGeoIPCookieContents(geoip_city(), geoip_region_name(), geoip_country_name(), 2);
                }
                catch (err) { }
            }
        };
        // Async Script call has 3 seconds to respond otherwise response will be ignored so we don't block dependencies
        setTimeout(function () {
            if (geoipSciptLoadStateChange == false) {
                geoipSciptLoadStateChange = true;
                setGeoIPDependentBehavioursLD();
                setGeoIPCookieContents("nullCity", "nullRegion", "nullCountry", null); // Set GeopIP cookie values to empty for duration of session
            }
        }, 3000);

    }
    else {
        setGeoIPDependentBehavioursLD();
    }


    function setGeoIPDependentBehavioursLD() {

        $(document).ready(function () {

            try {
                //display local country phone number
                var searchString = "united kingdom";
                var geoipCountry = getGeoipCountry();
                if (searchString.indexOf(geoipCountry.toLowerCase()) > -1 || geoipCountry == 'nullCountry') {
                    checkLocation("phone", "background", "/skin_default/template_images/topphone-local.gif");
                }
                else {
                    //display phone number outside UK
                    document.getElementById("phone").style.backgroundImage = 'url("/skin_default/template_images/topphone-alt.gif")';
                }
            }
            catch (err) { }
        });
    }
    /*checkLocation("local_ads","display","block");
    checkLocation("national_ads","display","none");
    IfNotLocal("local_ads","display","none");
    IfNotLocal("national_ads","display","block");*/
</script>


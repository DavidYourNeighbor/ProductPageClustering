<script type="text/javascript">
resetNavigation();
function resetNavigation() {
for (i=1;i<=7;i++) {
document.getElementById('nav-'+i+'-on').style.display = 'none';
document.getElementById('nav-'+i+'-off').style.display = 'block';
}
}
function hoverNav(jd){
document.getElementById('nav-'+jd+'-off').style.display = 'none'; 
document.getElementById('nav-'+jd+'-on').style.display = 'block'; 
}
</script>

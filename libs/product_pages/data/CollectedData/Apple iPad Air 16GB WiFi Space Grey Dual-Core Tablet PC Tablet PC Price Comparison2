<script type="text/javascript">
var dimages=new Array();var numImages=13;
for (i=0;i<numImages;i++){
dimages[i]=new Image();
dimages[i].src="http://elcellonline.com/template2013/uk/rotatingbanner-"+(i+1)+".gif";
}
var curImage=-1;
function swapPicture() {
if (document.images) {
var nextImage=curImage+1;
if (nextImage>=numImages)
nextImage=0;
if (dimages[nextImage] && dimages[nextImage].complete) {
var target=0;
if (document.images.myImage)
target=document.images.myImage;
if (document.all && document.getElementById("myImage"))
target=document.getElementById("myImage");
if (target) {
target.src=dimages[nextImage].src;
curImage=nextImage;
}
setTimeout("swapPicture()", 3000);
} else {
setTimeout("swapPicture()", 500);
}
}
}
setTimeout("swapPicture()", 3000);
</script>
<img id="myImage" name="myImage" src="http://elcellonline.com/template2013/uk/rotatingbanner-1.gif">
<script type="text/javascript">
function toggle_main_pic(num) {
for(i = 1; i < 9; i++){
if (i == num) {
document.getElementById('elcell_small_pic'+i).className = 'ecell-thumb-on';
document.getElementById('elcell_main_pic'+i).style.display = '';
} else {
document.getElementById('elcell_small_pic'+i).className = '';
document.getElementById('elcell_main_pic'+i).style.display = 'none';
}
}
}
</script>

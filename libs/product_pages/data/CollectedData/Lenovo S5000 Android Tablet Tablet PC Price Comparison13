<script type="text/javascript">
	var cmpr_opts = {
		compareTemplate : "/WW/site/templates/compare/CompareTemplate.html", 
		sliderTemplate : "/WW/site/templates/compare/SliderTemplate.html",
		burl : !!window.long_url ? long_url : "//shop.lenovo.com/SEUILibrary/controller/e/gbweb/LenovoPortal/en_GB",
		furl : "/gb/en", 
		file : ""
	}
	$(function() {
		checkSliderVisible();
		$('#compareSlider').centerSlide({speed:300, fixed:1, comparetemplate:cmpr_opts.compareTemplate, slidertemplate:cmpr_opts.sliderTemplate});
	});
</script>

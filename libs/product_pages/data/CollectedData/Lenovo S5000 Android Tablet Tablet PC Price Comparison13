  function isIE( version, comparison )
  {
    var $div = $('<div style="display:none;"/>').appendTo($('body'));
    $div.html('<!--[if '+(comparison||'')+' IE '+(version||'')+']><a>&nbsp;</a><![endif]-->');
    var ieTest = $div.find('a').length;
    $div.remove();
    return ieTest;
  }
  var ttanchor = document.createElement("A");
  ttanchor.setAttribute("href", location.href);
  var ttqs = {};
  ttanchor.href.replace(new RegExp("([^?=&]+)(=([^&]*))?", "g"), function($0, $1, $2, $3) { ttqs[$1] = $3; });
  var ttID = 'b'; //ttqs['tt'];
  if(!isIE(8,'lte')) // && (ttID == "b" || ttID == "c" || ttID == "d"))

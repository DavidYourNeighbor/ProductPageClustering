<script type="text/javascript">
var lmd={
  actualBoxNum:"",
  bundleID:"",
  businessType:"Public",
  chanprefix:"Public",
  country:"GB",
  customerID:"",
  discountCode:"",
  eCoupon:"",
  eDealCode:"",
  EPPserial:"",

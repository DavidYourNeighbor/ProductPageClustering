			<script type="text/javascript">
			//<![CDATA[
				var othSlideWidth = 150 + 32;
				var othSlideSpeed = 500;
				var othEnableSliding = true;

				$(document).ready(function () {
					//$('ul#AccVariantsTable').jcarousel({vertical: true, scroll: 3, animation: 750});
					var myCarouselDivJQuery = $('div#mycarousel');
					var visibleOtherItemsLength = myCarouselDivJQuery.children().length;
					if (visibleOtherItemsLength > 4) {

						//Remove scrollbars
						//bundleImagesClipDivJQuery.css({ overflow: 'hidden' });

						SetNavigationDisplayOth(myCarouselDivJQuery, visibleOtherItemsLength);
						var otherDetailsPrevHorizontalDiv = $('div#otherDetails-prev-horizontal');
						var otherDetailsNextHorizontalDiv = $('div#otherDetails-next-horizontal');
						//otherDetailsPrevHorizontalDiv.className = "active";
						//otherDetailsNextHorizontalDiv.className = "active";
						otherDetailsPrevHorizontalDiv.click(function () { PreviousSlideOth(myCarouselDivJQuery, visibleOtherItemsLength) });
						otherDetailsNextHorizontalDiv.click(function () { NextSlideOth(myCarouselDivJQuery, visibleOtherItemsLength) });
					}

					function NextSlideOth(myCarouselDivJQuery, visibleOtherItemsLength) {
						if (!othEnableSliding)
							return;
						EnableSliding = false;
						// get the current margin and subtract the slide width
						var newMargin = CurrentMargin(myCarouselDivJQuery) - othSlideWidth;

						// slide the wrapper to the left to show the next panel at the set speed. Then set the nav display on completion of animation.
						myCarouselDivJQuery.animate({ marginLeft: newMargin }, othSlideSpeed, function () { SetNavigationDisplayOth(myCarouselDivJQuery, visibleOtherItemsLength); othEnableSliding = true; });
					}

					function PreviousSlideOth(myCarouselDivJQuery, visibleOtherItemsLength) {
						if (!othEnableSliding)
							return;
						othEnableSliding = false;
						// get the current margin and subtract the slide width
						var newMargin = CurrentMargin(myCarouselDivJQuery) + othSlideWidth;

						// slide the wrapper to the right to show the previous panel at the set speed. Then set the nav display on completion of animation.
						myCarouselDivJQuery.animate({ marginLeft: newMargin }, othSlideSpeed, function () { SetNavigationDisplayOth(myCarouselDivJQuery, visibleOtherItemsLength); othEnableSliding = true; });
					}

					function SetNavigationDisplayOth(myCarouselDivJQuery, visibleOtherItemsLength) {
						// get current margin
						var currentMargin = CurrentMargin(myCarouselDivJQuery);
						var otherItemsClipDivJQuery = myCarouselDivJQuery.parent("div#otherItemsClip");

						// if current margin is at 0, then we are at the beginning, hide previous
						if (currentMargin == 0) {
							otherItemsClipDivJQuery.prevAll("div#otherDetails-prev-horizontal").hide();
							//$("#PreviousButton").hide();
						}
						else {
							otherItemsClipDivJQuery.prevAll("div#otherDetails-prev-horizontal").show();
							//$("#PreviousButton").show();
						}

						// get wrapper width
						//var wrapperWidth = $("#slider-wrapper").width();
						var wrapperWidth = 696;

						var plusWidth = 32;
						var ClipWidth = othSlideWidth * visibleOtherItemsLength - plusWidth;

						// turn current margin into postive number and calculate if we are at last slide, if so, hide next button
						if ((currentMargin * -1) + wrapperWidth == ClipWidth) {//(wrapperWidth - SlideWidth)) {
							otherItemsClipDivJQuery.prevAll("div#otherDetails-next-horizontal").hide();
							//$("#NextButton").hide();
						}
						else {
							otherItemsClipDivJQuery.prevAll("div#otherDetails-next-horizontal").show();
							//$("#NextButton").show();
						}
					}
				});
			//]]>
			</script>

	<script type="text/javascript">
		// we are using a semi-public interface requirejs.s.contexts._.config.paths to iterate over config values
		// which might get removed or changed between versions, so we need to check it's still available to us here
		if (!requirejs.s || !requirejs.s.contexts || !requirejs.s.contexts._ || !requirejs.s.contexts._.config || !requirejs.s.contexts._.config.paths) {
			alert('requireJS interface "requirejs.s.contexts._.config.paths" is no longer available. This interface is used to determine which modules to load per site configuration.\n\nPlease check for updates on this matter and fix code of ALL site JS files accordingly!');
		}

		var
			path = {
				nonsecure : '',

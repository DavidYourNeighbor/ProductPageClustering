    <script type="text/javascript">
        $(document).ready(function () {
            $("a[rel^='prettyPhoto']").prettyPhoto({
                social_tools: '',
                show_title: false,
                allow_resize: false,
                deeplinking: false,
                theme: 'light_rounded',
                callback: function () {
                    $("input[type=text]:first").focus().blur();
                }
            });

            $("a[rel='pp-showdelivery']").click(function (e) {
                e.preventDefault();
                var postcode = $("#delivery-dates-postcode").val();
                var $target = $("#showdelivery");
                var oldLink = $target.attr("href");
                if (postcode.length > 0) {
                    $target.attr("href", oldLink.replace("_PC_", postcode));
                }
                $target.click();
                $target.attr("href", oldLink);
            });

            $("#delivery-dates-postcode").keypress(function (e) {
                if (e.which === 13) {
                    $("a[rel='pp-showdelivery']").click();
                    e.preventDefault();
                }
            });

            $("form.quantity").keypress(function (e) {
                if (e.which === 13) {
                    e.preventDefault();
                }
            });
        });
	</script>
     


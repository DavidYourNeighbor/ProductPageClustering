<script>
    
        $(function() {
            
            var mf = "Microsoft";
            var pn = "J6X-00012";

            if (typeof ccs_cc_args === "undefined") {
                ccs_cc_args = [];
            }

            ccs_cc_args.push(['mf', 'Microsoft']);
            ccs_cc_args.push(['pn', 'J6X-00012']);
            ccs_cc_args.push(['lang', siteUserLanguage]);
            ccs_cc_args.push(['market', siteCountryCode]);
            ccs_cc_args.push(['_SKey', 'a1af4fb1']);
            ccs_cc_args.push(['_ZoneId', 'f37a52790f']);

            $.ajax({
                url: (is_secure ? "https://" : "http://") + "logo.cnetcontentsolutions.com/product?",
                data: { h: "06138bc5", mf: mf, pn: pn, returnMediaType: "logoProductList", locale: siteUserLanguageIso.replace("-", "_"), format: "json", "allLogos": true },
                dataType: "jsonp",
                cache: false,
                success: function(data) {

                    $.each(data.product_data_response.contentResponse.logoProductLists, function(index, value) {

                        
                        switch (value.logo_type)
                        {
                            
                            case "office2013":
                                var program = "of13pri";
                                break;
                            case "windowsClient8":
                                var program = "w8pri";
                                break;
                            case "winServer2012":
                                var program = "wspri";
                                break;
                            default:
                                
                                return;
                        }

                        
                        $.each(value.list, function(index, value) {
                            
                            if($('#' + value.productDetail.productId + '_price').length > 0) {
                                
                                ccs_cc_args.push([program + '_' + value.productDetail.versionId, $('#' + value.productDetail.productId + '_price .linelistprice').html()]);
                            }
                        });
                    });

                    var setupCnetLogo = function() {
                        registerTimeout(setTimeout(function() {
                            if ($("#ccs-logos table").length > 0)
                            {
                                $("#ccs-logos table td").each(function() {
                                    if ($(this).is(':empty')){
                                        $(this).remove();
                                    }
                                });
                            }
                            else
                            {
                                setupCnetLogo();
                            }
                        }, 500));
                    };

                    setupCnetLogo();

                }
            });

            var sc = document.createElement('script');
            sc.type = 'text/javascript'; sc.async = true;
            sc.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'cdn.cnetcontent.com/jsc/h.js';
            var n = document.getElementsByTagName('script')[0];
            n.parentNode.insertBefore(sc, n);

            
            var setupCnetContent = function() {
                registerTimeout(setTimeout(function() {
                    if ($(".ccs-cc-inline").length > 0)
                    {
                        if ($(".ccs-cc-inline-video-section").length > 0 || $(".ccs-cc-inline-gallery").length > 0)
                        {
                            $("#product-tabs").append("<li><a href='#media-gallery-tab' class='tab-anc'>Media Gallery</a></li>");
                            $("#product-tabs-panel").append("<div class='product-tab' id='media-gallery-tab'></div>");

                            if ($(".ccs-cc-inline-video-section").length > 0)
                            {
                                $("#media-gallery-tab").append($(".ccs-cc-inline-video-section"));
                                $(".ccs-cc-inline-video-section .ccs-cc-inline-separator").remove();
                            }

                            if ($(".ccs-cc-inline-gallery").length > 0)
                            {
                                $("#media-gallery-tab").append($(".ccs-cc-inline-gallery"));
                                if ($(".ccs-cc-inline-video-section").length > 0)
                                {
                                    $(".ccs-cc-inline-gallery").before("<div class='ccs-cc-inline-separator'></div>");
                                }
                            }
                        }

                        $("#ccs-inline-content .ccs-cc-inline-separator").remove();
                        $("#product-information").tabs("refresh");
                    }
                    else
                    {
                        setupCnetContent();
                    }
                }, 500));
            };
            setupCnetContent();

        });
    
</script>

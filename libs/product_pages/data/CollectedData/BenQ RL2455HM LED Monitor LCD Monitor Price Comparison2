    function showAnswerForm(comment_id) {
        document.getElementById(comment_id).appendChild(
        document.getElementById('ReplyForm'));



        $('#ReplyForm').show('slow', function () {
            $('#AskQuestionForm').hide('slow');
            $('#btnAskQuestion').attr('disabled', 'disabled');
            $('#hfReplyTo').val(comment_id);
        });


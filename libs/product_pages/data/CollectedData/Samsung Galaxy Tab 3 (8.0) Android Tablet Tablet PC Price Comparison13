function cv_mo20O5KYIF048R8loadSwf_Core(swfLocation, swfWidth, swfHeight, flashVersion, bgColor, swfParams, flashParams, embedAttributes) {

  try {
      cv_mo20O5KYIF048R8so = new SWFObject(swfLocation, "cv_mo20O5KYIF048R8AMPlayerProd", swfWidth, swfHeight, flashVersion, bgColor);	   


      for (var v in flashParams) {
        cv_mo20O5KYIF048R8so.addParam(v, flashParams[v]);
      }
      for (var v in embedAttributes) {
        cv_mo20O5KYIF048R8so.setAttribute(v, embedAttributes[v]);
      }

      cv_mo20O5KYIF048R8so.addVariable("swfEmbedTime", new Date().getTime());

      var agt = navigator.userAgent;
       
      var reFirefox = new RegExp("firefox/", "i");
      var regx = agt.split(reFirefox);
      var ffVersion = 8;
      if (regx[1]) {
          var pts = regx[1].split(/[.]/);
          ffVersion = parseFloat(pts[0] + "." + pts[1]);
      }
      var minorVersion = parseFloat(navigator.ap);
      if ((navigator.appVersion.indexOf("Mac")!=-1) || (ffVersion < 1.5)) {
          cv_mo20O5KYIF048R8so.setAttribute("height", swfHeight);
          cv_mo20O5KYIF048R8so.setAttribute("width", swfWidth);
          cv_mo20O5KYIF048R8so.addVariable("oldFirefox", "1");	     

	var data = new Object();
	
	
	
	var url="http://www.hiwayhifi.co.uk/ajaxcart/cart/add/";
	
	function ACC_ADDCART(prid,prdsku,prdopt)
	{
	//alert(prdopt);
	
	if(prdopt!="0")	
	{
	document.getElementById(prdsku).style.display="block";
	return false;
	
		}
	else
	{
	
				
				
				
				
				
			
	
	data["product"] = prid;
	
	data['form_key'] = "wqAQwZVWPbjB3Aof";
						
			jQuery.post(
				url,
				data,
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);
					if (response.success) {
						getInfoACC();
						document.getElementById(prdsku).style.display="none";
						document.getElementById(prid).innerHTML="Added";
					} else {
					
						alert(response.msg);
						document.getElementById(prdsku).style.display="none";
					}
				}
			);
			
			}
			
			
			
	
	}
	
	
	function ACC_ADDCART_OPT(prid,prdsku,prdname,prdval)
	{
	var ajax_scrid="ACC_user_";
	
	var Acc_user = ajax_scrid.concat(prid);	
	document.getElementById(Acc_user).style.display="block";	
	var prdname;
	data["product"] = prid;
	data[prdname]=prdval;
	

	
	
	
	data['form_key'] = "wqAQwZVWPbjB3Aof";
						
			jQuery.post(
				url,
				data,
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);
					if (response.success) {
						getInfoACC();
						document.getElementById(prdsku).style.display="none";
						document.getElementById(Acc_user).style.display="none";	
						document.getElementById(prid).innerHTML="Added";
					} else {
					
						alert(response.msg);
						document.getElementById(prdsku).style.display="none";
						document.getElementById(Acc_user).style.display="none";	
					}
				}
			);
	
	
	}
	
	
	
	
	
	
	
	
	
	
	
	function getInfoACC() {
			var form_key = "wqAQwZVWPbjB3Aof";
			var url = "http://www.hiwayhifi.co.uk/ajaxcart/cart/getinfo/";
			mask('Updating information ...');
			
			jQuery.post(
				url,
				{
					form_key: form_key
				},
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);
					
					update(response);
				}
			)
		}
		
	
	function mask(msg) {
			jQuery('body').mask(msg);
		}
		
		function unmask() {
			jQuery('body').unmask();	
		}
		
		
		
		function update(response) {
		
			jQuery('.top-link-cart').html(response.topLink);			
			jQuery('#shopping_cont').replaceWith(response.sidebar);	                  

			
			
		}
	
	</script>


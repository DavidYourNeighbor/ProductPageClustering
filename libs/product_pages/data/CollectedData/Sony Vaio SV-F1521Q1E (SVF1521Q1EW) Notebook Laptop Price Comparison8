var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;  
   if(!emailReg.test(emailval)) {  
        alert("Please enter valid email id");
        jQuery('#Ask_email').focus();
return false;
}

if(jQuery('#Ask_Customer').val()=="")
{
alert("Please Select Your Enquiry");
jQuery('#Ask_Customer').focus();
return false;
}

//document.Cart_Edit.submit();  
//setLocation(Prod_Url);
var actionPath="http://www.hiwayhifi.co.uk/checkout/cart/updatePost/";


var dataString ='cart['+ Prod_id +'][qty]='+qs;

     
     jQuery.ajax({
            url: actionPath,
            type: 'POST',
            data: dataString ,
            dataType: 'html',
            error: function(){
                alert ("ERROR");
            },
            success: function(resp,textStatus){
            document.getElementById(ajax_id_S).style.display="none";
            //document.getElementsByTagName('html')[0].innerHTML =resp
           //alert(resp);
           shop_getInfo();
           
          
                
            }
        }); 
//setQty(Prod_id, \'\');" id="add1" class="plus" />');


}
else
{
alert("Requested Quantity is unavailable");
 document.getElementById(ajax_id_S).style.display="none";
 
}
}

}
function SubQty(id,countval,Prod_id,Prod_Url)
{

var Qty_Name_S="Prv_Qty_";
var Qty_id_S=countval.toString();

var P_Qty_S=Qty_Name_S.concat(Qty_id_S);

var ajax_id_S="ajax_Qty_".concat(Qty_id_S);


document.getElementById(ajax_id_S).style.display="block";


var currentVal =document.getElementById(P_Qty_S).value;
if (!currentVal || currentVal=="" || currentVal == "NaN"  || currentVal ==0 )
{
currentVal=0;

document.getElementById(ajax_id_S).style.display="none";



}

else
{
var qs=document.getElementById(P_Qty_S).value=parseInt(currentVal)-1;


 var qty_id = "cart[" + Prod_id + "][qty]"; 
  

//document.getElementById(qty_id).value=qs;
//document.Cart_Edit.submit();  
//setLocation(Prod_Url);
var actionPath="http://www.hiwayhifi.co.uk/checkout/cart/updatePost/";


var dataString ='cart['+ Prod_id +'][qty]='+qs;
     
     jQuery.ajax({
            url: actionPath,
            type: 'POST',
            data: dataString ,
            dataType: 'html',
            error: function(){
                alert ("ERROR");
            },
            success: function(resp,textStatus){
            document.getElementById(ajax_id_S).style.display="none";
           // document.getElementsByTagName('html')[0].innerHTML =resp
           shop_getInfo();
         
          
                
            }
        }); 
//setQty(Prod_id, \'\');" id="add1" class="plus" />');


}

}

</script>

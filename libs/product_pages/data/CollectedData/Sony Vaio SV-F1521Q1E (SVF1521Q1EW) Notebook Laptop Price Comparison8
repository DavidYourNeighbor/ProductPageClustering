<script type="text/javascript">
    //<![CDATA[
        var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail');
    //]]>

jQuery('#newsletter')                    // get element whose id is 'myinput'
    .click(function() { 
        // bind a click handler
        
         if(document.getElementById('newsletter').value=="Your Email Address" || document.getElementById('newsletter').value=="Enter Your Email Address")
         {
        jQuery(this).val('')  ;      
        }  // clear field
               // unbind click handler to avoid field being cleared again
     })
     jQuery('#newsletter')                    // get element whose id is 'myinput'
    .blur(function() { 
       // bind a click handler
       if(document.getElementById('newsletter').value=="")
       {
        jQuery(this).val('Your Email Address')   ; 
        }      // clear field
              // unbind click handler to avoid field being cleared again
     })
     
     jQuery('#News_submit')                    // get element whose id is 'myinput'
    .click(function() { 
        // bind a click handler
if(document.getElementById('newsletter').value=="Your Email Address" || document.getElementById('newsletter').value=="Enter Your Email Address")
{
document.getElementById('newsletter').value="Enter Your Email Address";
return false;
}
else
{

var checkmail=document.getElementById('newsletter').value;
var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;  
   if(!emailReg.test(checkmail)) {  
        alert("Please enter valid email id");
        document.getElementById('newsletter').focus();
return false;
   }   

document.getElementById('news_email').value=document.getElementById('newsletter').value;


url="http://www.hiwayhifi.co.uk/capcha.php";
jQuery.post(
				url,
				function (response) {
			
			//document.getElementById('News_Captcha').innerHTML=response;
			//jQuery('#News_Captcha').html(response);
			
			}
);

			jQuery('#news_dialog').fadeIn();
jQuery('#backgroundPopup3').css({
			      'display': 'block'
		            });
			
			
		return false;	
			
			
			

}
     })
jQuery('#News_Close').click(function() {
jQuery("#backgroundPopup3").css({
			      "display": "none"
		            });
 
		            jQuery('#news_dialog').fadeOut();
		           


});

jQuery('#Newsletter_submit').click(function() {


var url="http://www.hiwayhifi.co.uk/newsletter/subscriber/new/";

var email=document.getElementById('news_email').value;

var captcha=document.getElementById('recaptcha_response_field').value;

var name=document.getElementById('news-name').value;

var telephone=document.getElementById('telephone').value;
var Re_Captcha=document.getElementById('recaptcha_challenge_field').value;

if(name=="")
{
alert("Please Enter Name");
document.getElementById('news-name').focus();
return false;
}

if(email=="")
{
alert("Please Enter Email");
document.getElementById('email').focus();
return false;
}


if(captcha=="")
{
alert("Please Enter Captacha");
document.getElementById('captcha').focus();
return false;
}
 document.getElementById('Ajax_News').style.display="block";
				data['email'] =email;
				data['recaptcha_challenge_field']=Re_Captcha;				
				data['recaptcha_response_field']=captcha;
				 
				jQuery.post(
				url,
				data,
				function (response) {
					
					response = jQuery.parseJSON(response);

					if (response.status=="success") {
					
                                           document.getElementById('User_Msg').style.display="";
					   document.getElementById('User_Msg').innerHTML=response.message;
					    document.getElementById('Ajax_News').style.display="none";
					    document.getElementById('newsletter').value="";

setTimeout(function(){
jQuery('#backgroundPopup3').css({
			      'display': 'none'
		            });
  jQuery("#news_dialog").fadeOut("slow", function () {
  
      });
 
}, 3000);
						
					} 
					
					
					
					else
					{
					
					 document.getElementById('User_Msg').style.display="";
					   document.getElementById('User_Msg').innerHTML=response.message;
					     document.getElementById('Ajax_News').style.display="none";
					
					
					
					
					}
				}
			);
			


});

function number(e)
	{
	var key;
	var keychar;

	if (window.event)
	   key = window.event.keyCode;
	else if (e)
	   key = e.which;
	else
	   return true;
	keychar = String.fromCharCode(key);
	keychar = keychar.toLowerCase();

	// control keys
	if ((key==null) || (key==0) || (key==8) || 
		(key==9) || (key==13) || (key==27) || (key==32)  )
	   return true;

	// alphas and numbers
	else if ((("1234567890.").indexOf(keychar) > -1))
	   return true;
	else
	   return false;
	}
	


    </script>

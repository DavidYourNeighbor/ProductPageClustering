function getInfoACC() {
			var form_key = "wqAQwZVWPbjB3Aof";
			var url = "http://www.hiwayhifi.co.uk/mcc17/ajaxcart/cart/getinfo/";
			mask('Updating information ...');
			
			jQuery.post(
				url,
				{
					form_key: form_key
				},
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);


					
					update(response);
				}
			)
		}
		
	
	function mask(msg) {
			jQuery('body').mask(msg);
		}
		
		function unmask() {
			jQuery('body').unmask();	
		}
		
		
		
		function update(response) {
		
			jQuery('.top-link-cart').html(response.topLink);			
			jQuery('#shopping_cont').replaceWith(response.sidebar);	                  

			
			
		}
		
		function REL_ADDCART_OPT(prid,prdsku,prdname,prdval)
	
		{
		
		var blid="REW_";
						var WarShow=blid.concat(prdsku);
						var Add_name="Adc_";
	
						var Add_user = Add_name.concat(prid);	
	
	
	
	var prdname;
	data["product"] = prid;
	data[prdname]=prdval;
	

	
	
	
	data['form_key'] = "wqAQwZVWPbjB3Aof";
						
			jQuery.post(
				url,
				data,
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);
					if (response.success) {
						getInfoACC();	
						
						document.getElementById(WarShow).style.display="none";
						
						document.getElementById(Add_user).innerHTML="Added";
						
						jQuery("#backgroundPopup4").css({
			      "display": "none"
		            });
						
						
					} else {
					
						alert(response.msg);
						document.getElementById(WarShow).style.display="none";
						jQuery("#backgroundPopup4").css({
			      "display": "none"
		            });
						
						
					}
				}
			);
		
		
		}

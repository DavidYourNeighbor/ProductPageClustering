return false;
}
	


Askprocesssub();

return false;



});
});






function Askprocesssub()
{


var data = new Object();
	url="http://www.hiwayhifi.co.uk/mcc17/securimage/example_form.ajax.php"
data['captcha'] = jQuery('#Ask_ct_captcha').val();
data['name'] =jQuery('#Ask_name').val();
data['email'] =jQuery('#Ask_email').val();

jQuery.post(
				url,
				data,
				
				
				function (response) {
					
					response = jQuery.parseJSON(response);
					
					if(response=="1")
					{
					
					
					
					
				
					
					var action = jQuery('#Ask_contactform').attr('action');

	jQuery('#Ask_submit')
		.before('<img src="'+$_SKIN_URL+'quickcontact/images/opc-ajax-loader.gif" class="loader" style="padding-right:10px;"/>') ; 
		
var mylist = document.getElementById("Ask_Customer");
    iterations = mylist.options[mylist.selectedIndex].text;

var subject="Enquiry - "+iterations+" : "+jQuery('#Ask_subject').val();
    
	jQuery.post(action, { 
		name: jQuery('#Ask_name').val(),
		email: jQuery('#Ask_email').val(),
		telephone: jQuery('#Ask_telephone').val(),
                captcha:jQuery('#Ask_ct_captcha').val(),
		comment: jQuery('#Ask_comment').val(),			
		cmprice: jQuery('#Ask_CMPRICE').val(),
                cmlink: jQuery('#Ask_CMLINK').val(),
                cmpricelbl: jQuery('#cmpricelbl').val(),
                cmlinklbl:  jQuery('#cmlinklbl').val(),
                Enquiry:jQuery('#Ask_Customer').val(),
                subject: subject
	},
		function(data){
			//jQuery('#Ask_contactform #Ask_submit').attr('disabled','');
			jQuery('.response').remove();
			jQuery('#TxtArea').after('<span class="response">'+data+'</span>');

                     if(data.indexOf('Thank') != -1){

//jQuery("#Ask_contactform").css("display", "none");
//jQuery(".Ask_cont_man").css("display", "none");
AscreloadCaptcha();
   
}
			jQuery('.response').slideDown();
			jQuery('#Ask_contactform img.loader').fadeOut(500,function(){jQuery(this).remove()});
		}
	);

	return false;
					
					
					
					
					
					
					
					
					
					}
					
					
					else
					
					{
					
					//jQuery('#Ask_contactform #submit').attr('disabled','');
			jQuery('.response').remove();
			jQuery('#TxtArea').after('<span class="response">Your Captcha is incorrect. Please try again.</span>');
AscreloadCaptcha();			 
			jQuery('.response').slideDown();
			jQuery('#Ask_contactform img.loader').fadeOut(500,function(){jQuery(this).remove()});
			
					
					}
				}
			);
			
			
			
			}


function AscreloadCaptcha()
    {


jQuery("#Ask_siimage").attr('src','http://www.hiwayhifi.co.uk/mcc17/securimage/securimage_show.php?sid=' + Math.random());
return false;
       // jQuery('#siimage').src = 'http://www.hiwayhifi.co.uk/mcc17/securimage/securimage_show.php?sid=' + Math.random();
    }

jQuery( "#Ask_Customer" ).live( "change", function() {

if(this.value=="marketing@hiwayhifi.co.uk")
{
jQuery("#Ask_CMPRICE,#Ask_CMLINK,#Ask_CMLINK_LBL,#Ask_CMLINK_PRC").css({
			      "display": ""
		            });
		            
document.getElementById('cmlinklbl').value="Competitor Link:";
document.getElementById('cmpricelbl').value="Competitor Price:";


}
else
{

jQuery("#Ask_CMPRICE,#Ask_CMLINK,#Ask_CMLINK_LBL,#Ask_CMLINK_PRC").css({
			      "display": "none"
		            });

document.getElementById('cmlinklbl').value="";
document.getElementById('cmpricelbl').value="";

}



});
			

function  ContentHide()  

{

var lvalue=document.getElementById("flix-inpage").innerHTML;
var larr=lvalue.split(" ");
var len = 0;

// For each iterates over the index of arrays
for(var i in larr) { 
     len += larr[ i ].length // Acummulate the length of all the strings
}


//alert(len);
//jQuery('#flix-inpage').innerHTML


if(len<500)
{
document.getElementById('product_tabs_description_tabbed').style.display="none";
 document.getElementById("product_tabs_Custom_Description").setAttribute("class","active"); 
document.getElementById('product_tabs_Custom_Description_contents').style.display="";
}
else
{
 document.getElementById("product_tabs_description_tabbed").setAttribute("class","active");
document.getElementById('product_tabs_description_tabbed').style.display="";
document.getElementById('product_tabs_description_contents').style.display="";


}


}

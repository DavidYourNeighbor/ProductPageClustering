<script>
 var browser = navigator.appName;
	var data = new Object();
	jQuery(function() {
		var dialog = null;


			
		function mask(msg) {
                  

if(browser!="Microsoft Internet Explorer")
{

			jQuery('body').mask(msg);
}
else
{

document.getElementById('m-wait').style.display="";


}
		}
		
		function unmask() {


if(browser!="Microsoft Internet Explorer")
{

			jQuery('body').unmask();
}
else
{

document.getElementById('m-wait').style.display="none";

}
				
		}
	
		function update(response) {
		
			jQuery('.top-link-cart').html(response.topLink);			
			jQuery('#shopping_cont').replaceWith(response.sidebar);
			jQuery('#mg-ajaxcart-dialog .product-info .msg .name').html(response.name);
			

                          jQuery("#backgroundPopup1").css({
			      "opacity": "0.3"
		            });
 jQuery("#backgroundPopup1").css({
			      "z-index": "990"
		            });
jQuery("#backgroundPopup1").css({
			      "display": "block"
		            });


			
			jQuery('#mg-ajaxcart-dialog').fadeIn();
			//dialog = setTimeout(function() { jQuery('#mg-ajaxcart-dialog').fadeOut(); }, 10000);
		}


	
		function getInfo() {
			var form_key = "wqAQwZVWPbjB3Aof";
			var url = "http://www.hiwayhifi.co.uk/ajaxcart/cart/getinfo/";
			mask('Updating Basket...');
			
			jQuery.post(
				url,
				{
					form_key: form_key
				},
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);
					
					update(response);
				}
			)
		}
		
		jQuery('.Pop_Cont,.Ack_close').click(function() {
			unmask();
			jQuery('#mg-ajaxcart-dialog').fadeOut();
			clearTimeout(dialog);
			
jQuery('#backgroundPopup1').css({
			      'display': 'none'
		            });
			
			return false;
		});
		

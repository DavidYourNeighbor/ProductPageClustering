		jQuery('.Click_Collect').click(function() {
		
			//mask('Adding Product to Cart ...');
			
			var form_key = "wqAQwZVWPbjB3Aof";
			var url = "http://www.hiwayhifi.co.uk/ajaxcart/cart/add/";
			
			var inp = jQuery('#product_addtocart_form input');
			var sel = jQuery('#product_addtocart_form select');
			
			jQuery.each(inp, function(key, value) {			
			        var type = jQuery(value).attr('type');	 
				var name = jQuery(value).attr('name');							
				var value = jQuery(value).val();
				
				if(type!="radio")
				{
				data[name] = value;
				
				
				}
				else
				{
				var radio_b=jQuery('input:radio:checked').val();
				data[name] = radio_b;
				
				}
				
				
				
				
			});
			
			jQuery.each(sel, function(key, value) {
				var name = jQuery(value).attr('name');
				var value = jQuery(value).val();
				data[name] = value;
				
			});
                     
			
	
			data['form_key'] = form_key;
						
			jQuery.post(
				url,
				data,
				function (response) {
					unmask();
					response = jQuery.parseJSON(response);
					if (response.success) {
					
					
						
						window.location.href="http://www.hiwayhifi.co.uk/checkout/cart/clickcollect/";
					} else {
					alert(response.msg);
						
					}
				}
			);
			
			return false;
		});
	
	
	
	
	
	
	
	
	

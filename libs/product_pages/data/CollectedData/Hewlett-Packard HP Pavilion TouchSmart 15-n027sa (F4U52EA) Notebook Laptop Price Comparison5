// create associative array/object of all site IDs
var $siteIds = {};
// $siteIds['site name'] = ["live site id", "staging/production site id", "extension"]
$siteIds['very'] = ["90092320", "60092320", ".co.uk"];
$siteIds['littlewoods'] = ["90092183", "60092183", ".com"];
$siteIds['kandco'] = ["90052727", "60052727", ".com"];
$siteIds['isme'] = ["90089306", "60089306", ".com"];
$siteIds['littlewoodsireland'] = ["90178928", "60178928", ".ie"];
$siteIds['woolworths'] = ["90103280", "60103280", ".co.uk"];
// get url
var $root = location.host;
// if site is live
if ($root.match('www')) {
// strip url to leave only the brand name
$root = $root.split('.')[1];
var $currentSiteId = eval('$siteIds.' + $root + '[0]');
} 
// if site is on staging or production
else {
// remove any domain/extension (.lsdg.com for example)
$root = $root.split('.')[0];
var $splitCount = $root.split('-').length -1;
// strip url to leave only the brand name
$root = $root.split('-')[eval($splitCount)];
var $currentSiteId = eval('$siteIds.' + $root + '[1]'); 
} 
// set site name as brand + extension
var $currentSiteName = $root + eval('$siteIds.' + $root + '[2]');
cmSetClientID($currentSiteId, true, $currentSiteName);
</script>

		function cmVideoTrack(eventData) {
			var throwTag = false;
			var eventDataArray = eventData.split(',');
			var eventTypes = null;

			var eventMap = new Array();
			eventMap["LOAD"]	= 0;
			eventMap["PLAY"]	= 2;
			eventMap["PAUSE"]	= 1;
			eventMap["STOP"]	= 3;

			switch (eventDataArray[0]) {
				case "LOAD" :
				case "PLAY" :
				case "PAUSE" :
				case "STOP" :
					throwTag = true;
			}

		   var timeStamp = eventDataArray[1];

		   if (eventDataArray[0] == 'LOAD') {
		    timeStamp = 0;
		   }

		   if (throwTag) {
		    var elementAttributes = "-_--_--_--_--_--_--_--_--_--_--_--_-" + eventMap[eventDataArray[0]] + "-_-" + decodeURIComponent(timeStamp) + "-_-";
		    cmCreateElementTag("Games &amp; Puzzles", 'Play Video', elementAttributes);
		   }
		}
	


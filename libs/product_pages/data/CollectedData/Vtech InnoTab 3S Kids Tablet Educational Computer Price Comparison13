<script type="text/javascript">
if(jQuery(".sizeSelector").find('li').length > 1) {
if(jQuery(".sizeSelector").find('li[class*="selected"]').length == 0) {
jQuery("#Quantity").attr('disabled', 'disabled');
}
}
jQuery("div.productQuantity select.quantity_select").change( function() {
if( jQuery('[name="product_set_type"]').length > 0 && jQuery('[name="product_set_type"]').val() == 'WSO')
{
if (jQuery(this).val() != 0)
{
jQuery("#in_stock_message").show();
jQuery("#in_stock_message").children(".stockMessage").show();
jQuery("#actionStockMessage").slideDown('600' , 'easeOutQuint');
}
else
{
if(jQuery(".sizeSelector").find('li[class*="selected"]').length != 0) {
jQuery("#in_stock_message").show();
jQuery("#in_stock_message").children(".stockMessage").show();
}
jQuery("#actionStockMessage").slideUp('600' , 'easeOutQuint');
}
calculateWSOProductsPrice();
}
});
jQuery("div.productQuantity select.quantity_select").change( function() {
var form = document.forms.cartaddproduct;
var pid = "";
if(jQuery("#hof-product-sku .hof-value").length > 0)
pid = jQuery("#hof-product-sku .hof-value").text();
else
pid = jQuery("#skuProductNumber").text();
if (pid === undefined || pid == "" || pid == null) {
pid = form.open_pid.value;
}
calculateTotalProductPrice(form.update_url_totalprice.value, pid, $(this).val());
});
 //pdict.MaxOrderQuantity > 1 is for cases where there is no inventory allocation

availabilityMessage = "in stock, only 1 left"


</script>


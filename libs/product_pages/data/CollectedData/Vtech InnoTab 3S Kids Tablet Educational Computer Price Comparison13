		function prepareSearchButton() {
						
			jQuery("#search-submit").click( function() {
				var searchString = jQuery("#q").val();
				if (searchString != "" && searchString != "Enter keyword, product code or brand...") {
					jQuery("#SimpleSearchForm").trigger('submit');
				} else {
					jQuery("#q").addClass('siteSearchError');
					return false;
				}
				
			});
		}
				
		jQuery(document).ready(function() {
			
				var searchSuggestURL = "/on/demandware.store/Sites-hof-Site/default/Page-GetSuggestions";
				app.searchsuggest.init("SimpleSearchForm", "q", "simplesearch.searchtext", "searchSuggestions", searchSuggestURL);
			
			
				//change search button's type if javascript is enabled so that empty search doesn't refresh the page
			prepareSearchButton();
		
			
			
		});

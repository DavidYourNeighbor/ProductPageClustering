<script type="text/javascript">
	var productNotAvailable = false;
	var productNotAvailableSince = null;

	function checkProductNotAvailable() {
		if (productNotAvailable) {
			var id = "message_click";
			jQuery("#" + id).remove();
			var text = 'Out of stock';
			jQuery('#SjElement3').append($('<div id="productNotAvailableMessage">'+ text +'</div>'));
			tagProductNotAvailable();
		}
	}

	function checkStock() {
		if ( jQuery('.stockMessage').text() == 'Out of stock' ) {
			tagProductNotAvailable();
		}
	}

	function tagProductNotAvailable() {
		if (isCmetricEnabled) {
			

			var cmErrorsArray = new Array();
			cmErrorsArray.push({
				'errorType' : 'Stock not available',
				'fieldName' : '',
				'errorMessage' : 'Sorry, this product is out of stock'
			});
			cmTrackCheckoutErrors(cmErrorsArray);
		}
	}

	jQuery(document).ready(function() {
		checkStock();
	});

	// rendering: productdetails
	
</script>

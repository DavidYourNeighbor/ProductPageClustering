<script>
			$(document).ready(function() {			 
				$('#pewpewpew').after('<p class="error" id="emailValidationMessage">&nbsp;</p>');
				$('#emailValidationMessage').css({opacity : 0});

				$('#pewpewpew').click(function() { 				
							 
					var hasError = false;
					var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;			 
					var emailaddressVal = $("#emailaddress").val();
					
					if(emailaddressVal == '') {
						$("#emailValidationMessage").html('Please enter your email address').animate({ opacity : 1}, function () {
							$(this).delay(2000).animate({ opacity : 0});
						});
						$('#emailaddress').addClass('error');
						hasError = true;
					}
			 
					else if(!emailReg.test(emailaddressVal)) {
						$("#emailValidationMessage").html('Please enter a valid email address').animate({ opacity : 1}, function () {
							$(this).delay(2000).animate({ opacity : 0});
						});
						$('#emailaddress').addClass('error');
						hasError = true;
					}
			 
					if(hasError == true) { return false; }
			 
				});
			});        
        </script>

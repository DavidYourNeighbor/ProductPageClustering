<script type="text/javascript">
<!--


	var brandOfProduct = null;
	
		brandOfProduct = 'Vtech';
	
	function showMsgWhenDone(mode) {
		var messageHtml = "";
		
		if (mode == 'productset') {
			jQuery('#rrContainer').empty();
			messageHtml += "<div id=\"addedItems\">You've added to your bag ";
		}
		else {
			messageHtml += "<div class=\"actionMessageAdd\"><span class=\"actionMessageHeading\">You've added to your bag</span> ";
		}
		messageHtml += '<br />';

		if (mode == 'product') {
			var productVariantID = jQuery('#' + 'hof-product-sku .hof-value').text();
			if(brandOfProduct == 'CHANEL' || brandOfProduct == 'M·A·C'){	
				productVariantID = jQuery('#' + 'skuProductNumber').text();
			}
			var addedProductQnt = jQuery('#' + 'Quantity').val();
			
			if (jQuery('#' + 'quantity_select_hidden_' + productVariantID).length > 0) {
				var productQntAlreadyAddedHidden = jQuery('#' + 'quantity_select_hidden_' + productVariantID).val();
			} else {
				var productQntAlreadyAddedHidden = 0;
			}
			
			var currentIterationQnt = parseInt(productQntAlreadyAddedHidden, 10) + parseInt(addedProductQnt, 10);
			var variantsSelectedHidden = '';
			
			var variantsAlreadySelected = jQuery('#variants_selected_hidden').html();
			var isVariantAddedAlready = variantsAlreadySelected.indexOf('productVariantID-' + productVariantID);

			// The current product variant occurs for the first time at this page load so we add its message. Otherwise, we just update its quantity.
			if (isVariantAddedAlready == -1) {
				variantsSelectedHidden += '<span id="productVariantID-' + productVariantID + '_content">';
				variantsSelectedHidden += '<span id="productVariantID-' + productVariantID + '">' + currentIterationQnt + '</span> x ';
				if(brandOfProduct != 'CHANEL' && brandOfProduct != 'M·A·C'){
					variantsSelectedHidden += '<strong>' + jQuery('h1 span.product-name').text() + '</strong>';
				} else {
					variantsSelectedHidden += '<strong>' + jQuery('h1 span.productName').text() + '</strong>';
				}	
				var colourRefinementTitle = jQuery('.colour-swatches-list .selected a');
				if(brandOfProduct == 'CHANEL' || brandOfProduct == 'M·A·C'){	
					colourRefinementTitle = jQuery('.colourSelector .selected a');
				}
				if (colourRefinementTitle.length > 0) {
				 	variantsSelectedHidden += '<strong>';
					variantsSelectedHidden += colourRefinementTitle.attr('title');
					variantsSelectedHidden += '</strong>';
				}
				
				var sizeRefinementTitle = jQuery('.size-swatches-list .selected');
				if (sizeRefinementTitle.length > 0) {
					if (colourRefinementTitle.length > 0) {
						// In case of both, Size and Colour values are set we will separate them with cart.addtocartajax.valuesSeparator.
						variantsSelectedHidden += ' / ';
					}
				 	variantsSelectedHidden += '&nbsp;<strong>Size ';
					variantsSelectedHidden += sizeRefinementTitle.text();
					variantsSelectedHidden += '</strong>';
				}
				
				jQuery('#' + 'quantity_select_hidden_container').append("<input type=\"hidden\" value=\"" + currentIterationQnt + "\" name=\"quantity_select_hidden_" + productVariantID + "\" id=\"quantity_select_hidden_" + productVariantID + "\" \/>");
				variantsSelectedHidden += '<br />';
				variantsSelectedHidden += '</span>';
				variantsSelectedHidden += jQuery('#' + 'variants_selected_hidden').html();
			} else {
				jQuery('#' + 'quantity_select_hidden_' + productVariantID).val(currentIterationQnt);
				
				jQuery('#' + 'variants_selected_hidden' + ' #' + 'productVariantID-' + productVariantID).html(currentIterationQnt);
				variantsSelectedHidden += jQuery('#' + 'variants_selected_hidden').html();
			}

			// Add new products messages or update the quantity of already used message.
			jQuery('#variants_selected_hidden').html(variantsSelectedHidden);
			messageHtml += variantsSelectedHidden;
		} else if (mode == 'productset') {
			var selectedItemQtys;
			
			var productSelectedHidden = jQuery('#' + 'products_selected_hidden').html();

			if ( jQuery('div.itemQtyWSR').length > 0) {
				 selectedItemQtys = jQuery('div.itemQtyWSR.selected');
			} else if ( jQuery('#set-products-wso').length > 0){
				selectedItemQtys = jQuery('.quantity_prod_select');
				for (var i = 0; i < selectedItemQtys.length; i++)
				{
					var sizeSelector = jQuery(this).parents('.WSOSetProductContainer').find('.wsoSizeSelector'); 
					if (jQuery(sizeSelector).length > 0 && jQuery(sizeSelector).children('li.checked').length == 0)
					{ 
						selectedItemQtys.splice(i, 1);
						i--;
					}
				}
			} else {
				var el = jQuery('div.itemQty.selected').length;
				if(el)
					selectedItemQtys = jQuery('div.itemQty.selected');
				else
					selectedItemQtys = jQuery('div.item-qty.selected');
			}
			
			var productsAlreadySelected = jQuery('#products_selected_hidden').html();
			
			var isProductAddedAlready = '';
			var productsSelectedHidden = '';

			jQuery.each(selectedItemQtys, function(index) {
				if (!jQuery(this).find('select').attr('disabled') && jQuery(this).find('select').val() > 0)
				{
					var selectedItemQty = jQuery(this).find('select').val();
					var selectedItemName = '';
					var selectedItemColor = ''; // For WSR and WSO.
					var selectedItemSize = ''; // For WSR and WSO.
					// Do not use jQuery siblings() method because of the Browsers Support.
					var productID = jQuery(this).parent().find('.productID').val();
					
					isProductAlreadySelected = jQuery('#' + 'quantity_select_hidden_' + productID).length;

					// The current product occurs for the first time at this page load so we add its message. Otherwise, we just update its quantity.
					if (isProductAlreadySelected == 0) {
						if (jQuery("[name='product_set_type']").length > 0 && jQuery("[name='product_set_type']").attr('value') == 'WSR') {
							selectedItemName = jQuery(this).parent().parent().find('h5 a').text();
							selectedItemColor = ' ' + jQuery('#selectedColourValue').text() + ' / ';
							selectedItemSize = 'Size ' + jQuery(this).parent().parent().find('.wsrSizeSelector option:selected').text();
						} else if ( jQuery("[name='product_set_type']").length > 0 && jQuery("[name='product_set_type']").attr('value') == 'WSO') {
							selectedItemName = jQuery(this).parents('.WSOSetProductContainer').find('h5 a').text();
							var colourSelector = jQuery(this).parents('.WSOSetProductContainer').find('.colourSelector');
							var sizeSelector = jQuery(this).parents('.WSOSetProductContainer').find('.wsoSizeSelector');
							
							if ( jQuery(colourSelector).length > 0)
							{
								selectedItemColor = jQuery(this).parents('.WSOSetProductContainer').find('[name="colourSwatches"] div ul li.selected a').attr('title');
							}
							else
							{
								selectedItemColor = '';
							}
							
							// Add the separator
							if (jQuery(colourSelector).length > 0 && jQuery(sizeSelector).length > 0) {
								selectedItemColor += ' / ';
							}
							
							if ( jQuery(sizeSelector).length > 0)
							{
								selectedItemSize = 'Size ' + jQuery(sizeSelector).find('li.selected a').text();
							}
							else
							{
								selectedItemSize = '';
							}
							
						} else {
							selectedItemName = jQuery(this).parent().find('h5 a').text();
							selectedItemColor = '';
							selectedItemSize = '';
						}
						
						productsSelectedHidden = '<span id="productID-' + productID + '_content">';
						productsSelectedHidden += '<span id="productID-' + productID + '">' + selectedItemQty + '</span> x <strong>' + selectedItemName + ' ' + selectedItemColor + selectedItemSize + '</strong><br />';
						productsSelectedHidden += '</span>';
						productsSelectedHidden += jQuery('#' + 'products_selected_hidden').html();
						
						jQuery('#' + 'quantity_select_hidden_container').append("<input type=\"hidden\" value=\"" + selectedItemQty + "\" name=\"quantity_select_hidden_" + productID + "\" id=\"quantity_select_hidden_" + productID + "\" \/>");
					} else {
						var currentIterationQnt = jQuery('#' + 'quantity_select_hidden_' + productID).val();
						currentIterationQnt = parseInt(currentIterationQnt) + parseInt(selectedItemQty);	
						
						jQuery('#' + 'quantity_select_hidden_' + productID).val(currentIterationQnt);
			
						jQuery('#' + 'products_selected_hidden' + ' #' + 'productID-' + productID).html(currentIterationQnt);
						productsSelectedHidden = jQuery('#' + 'products_selected_hidden').html();
					
					}
				
					jQuery('#products_selected_hidden').html(productsSelectedHidden);
					}
				});
	
				// Add new products messages or update the quantity of already used message.
				messageHtml += productsSelectedHidden;
	
				jQuery('#buttonAddToBag').blur();
				
				var productSetSelector = 'input[name="product_set_type"]';
				
				if(jQuery(productSetSelector).length > 0 && jQuery(productSetSelector).val() == 'WSR') {
					HOFSite.functions.resetProductsSelection();
				}
		}
		if(brandOfProduct != 'CHANEL' && brandOfProduct != 'M·A·C'){
			messageHtml += '<div class="spacer"></div><a href="https://www.houseoffraser.co.uk/on/demandware.store/Sites-hof-Site/default/Cart-Show" class="forwardButtonWhite hof-btn hof-btn-mini hof-btn-addtobag">Secure Checkout</a></div>';
		} else {
			messageHtml += '<div class="spacer"></div><a href="https://www.houseoffraser.co.uk/on/demandware.store/Sites-hof-Site/default/Cart-Show" class="button forwardButtonWhite">Secure Checkout<span></span></a></div>';
		}
		var rrContainerParent = null; 
		if (jQuery('#rrContainer').length == 0 ) {
			// rrContainerParent.append(jQuery('<div id="rrContainer"></div>'));
		} else {
			rrContainerParent = jQuery('#rrContainer').parent();
		}

		if (jQuery('#buttonAddToBagContainer').length > 0) {
			messageHtml += "<br />";
			var buyInfo = jQuery(messageHtml).clone();
			var rrContainer = jQuery('#rrContainer');
			buyInfo.appendTo(rrContainer);
			rrContainer.show();
		} else {
			jQuery('.rrActionMessageAdd').empty();
			jQuery('#addToBagActionMessage').find('.actionMessageAdd').remove();
			jQuery('#addToBagActionMessage').prepend(messageHtml);
			jQuery('#addToBagActionMessage').show();
			
				jQuery('#rrActionMessageShow').show();
			
		}
		
		if (mode == 'productset') {
			var clonedRecs = jQuery('#addToBagActionMessage > div').clone();
			jQuery('#rrContainer').append(clonedRecs);
		}

		jQuery('#add_to_bag_message_hidden').val('<div id="rrContainer" class="rrActionMessage" style="display: block;">' + jQuery('#rrContainer').html() + '</div>');
	}
	
	var lastHideBasketID = 0;
	
	function addProductToMiniCart(form, callback) {
		var serializedForm = jQuery(form).serialize();
		jQuery.post(
			'/on/demandware.store/Sites-hof-Site/default/Cart-MiniAddProduct',
			serializedForm,
			function(html){
				var htmlCache = html.replace(/<script.*?>[\s\S.]*?<\/script>/igm, " ");
    
				HOFSite.variables.miniBagUI.updateCache(htmlCache);
				jQuery('#basket-content').html(html);
				
				var minicartSubtotal = jQuery.trim(jQuery('#minicart-pricesubtotal').text());
				jQuery('#basket-total-sum').html(minicartSubtotal);
				
				// The following call will update the mini bag button items whithout opening the mini bag
				showMiniBag(true);
		
				showMsgWhenDone('product');
				
				if(callback)
					callback();
			}
		);
	}

	function addProductsToMiniCart(form) {
		var serializedForm = jQuery(form).serialize();
		
		jQuery.post(
			'/on/demandware.store/Sites-hof-Site/default/Cart-MiniAddProducts',
			serializedForm,
			function(html){
				var htmlCache = html.replace(/<script.*?>[\s\S.]*?<\/script>/igm, " ");
				
				HOFSite.variables.miniBagUI.updateCache(htmlCache);
				jQuery('#basket-content').html(html);
				
				var minicartSubtotal = jQuery.trim(jQuery('#minicart-pricesubtotal').text());
				jQuery('#basket-total-sum').html(minicartSubtotal);
				
				// The following call will update the mini bag button items whithout opening the mini bag
				showMiniBag(true);
	
				showMsgWhenDone('productset');
				
				HOFSite.functions.resetProductsSelection();
			}
		);
	}
	
	
	function removeProductFromMiniCart(id) {
   		jQuery.post(
   			'/on/demandware.store/Sites-hof-Site/default/Cart-MiniRemoveProduct',
   			{pid: id},
   			function(html){
   				HOFSite.variables.miniBagUI.updateCache(html);
 				jQuery('#basket-content').html(html);
 				
				var minicartSubtotal = jQuery.trim(jQuery('#minicart-pricesubtotal').text());
				if(jQuery('#minicart-pricesubtotal').text())
					jQuery('#basket-total-sum').html(minicartSubtotal);
				else
					jQuery('#basket-total-sum').html(HOFSite.variables.miniBagEmptyAmount);
 				
				showMiniBag(); 	
   				//jQuery('#basket-content').css('display', 'block');
  			}
  		);
 	}
 	
 	
 	function updateProductFromMiniCart(id, qty) {
		jQuery.post(
   			'/on/demandware.store/Sites-hof-Site/default/Cart-MiniUpdateProduct',
   			{
   				pid: id,
   				qty: qty
   			},
   			function(html){
   				HOFSite.variables.miniBagUI.updateCache(html);
   				jQuery('#basket-content').html(html);

				var minicartSubtotal = jQuery.trim(jQuery('#minicart-pricesubtotal').text());
				jQuery('#basket-total-sum').html(minicartSubtotal);

				showMiniBag(); 			
   				//jQuery('#basket-content').css('display', 'block');
  			}
  		);
 	}
 	
//-->
</script>






jQuery(document).ready(function() {
function getVideoName(video) {
return jQuery(video).find("[itemprop='name']").text();
}
if (jQuery(".hof-pdp").length == 0) {
jQuery(".videoContainer video").bind({
play : function() {
var videoName = getVideoName(this);
var duration = this.duration.toFixed(2);
cmCreateElementTag("Play Video: " + videoName, "Play Video", cmPageNameVar + "-_--_--_--_--_--_--_--_--_--_--_--_--_--_-" + duration);
},
pause : function() {
var videoName = getVideoName(this);
var currentTime = this.currentTime.toFixed(2);
var duration = this.duration.toFixed(2);
//throw pause tag only if video NOT completed( < used because of IE duration and current time mismatch)
if (Math.round(currentTime*100) < Math.round(duration*100)) {
cmCreateElementTag("Pause Video: " + videoName, "Pause Video", cmPageNameVar + "-_--_--_--_--_--_--_--_--_--_--_--_--_-" + currentTime + "-_-" + duration);
}
},
ended : function() {
var videoName = getVideoName(this);
var currentTime = this.currentTime.toFixed(2);
var duration = this.duration.toFixed(2);
//call pause because of IE/Safari wrong pause flag set and secondary play do not throw play tag
this.pause();
cmCreateElementTag("Playback Complete: " + videoName, "Playback Complete", cmPageNameVar + "-_--_--_--_--_--_--_--_--_--_--_--_--_-" + currentTime + "-_-" + duration);
}
});
//FF and Chrome fullscreen video event throwing CM tags
jQuery(document, ".videoContainer video").bind('webkitfullscreenchange mozfullscreenchange fullscreenchange', function(e) {
var state = document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen;
if (state) {
var videoElement = jQuery(".videoContainer video").get(0);
var videoName = getVideoName(videoElement);
var videoDuration = videoElement.duration.toFixed(2);
cmCreateElementTag("Fullscreen Video: " + videoName, "Fullscreen Video", cmPageNameVar + "-_--_--_--_--_--_--_--_--_--_--_--_--_--_-" + videoDuration);
}
});
//iPad fullscreen video event throwing CM tags
jQuery(".videoContainer video").bind('webkitbeginfullscreen', function(e) {
var videoName = getVideoName(this);
var videoDuration = this.duration.toFixed(2);
cmCreateElementTag("Fullscreen Video: " + videoName, "Fullscreen Video", cmPageNameVar + "-_--_--_--_--_--_--_--_--_--_--_--_--_--_-" + videoDuration);
});
}
});

</script>

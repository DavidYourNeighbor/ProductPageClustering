<script type="text/javascript">
  function bannerClicks(id_banner,block_id){
    var click_url = 'https://alegol.com/bannerads/reports/click/';    
    new Ajax.Request(click_url,{
      method: 'post',
      parameters:{banner_id: id_banner,block_id:block_id},
      onFailure: '',
      onSuccess: ''
    });
  }
</script>

        if (fn instanceof Function) {
          this.scripts.push({'s':fn,'p':pos});
        }
      },
      sort : function(){
        this.scripts.sort(function(a,b){ return a.p===undefined ? -1 : b.p===undefined ? 1 :b.p - a.p; });

<script language="javascript">
	// This function is used to setup hash content from URL
	function setFilterURLHashContent(objElement) {
		if(window.location.hash) {
			var strLocationHash = window.location.hash;
			var updateURL = $(objElement).prop('href') + strLocationHash;
			$(objElement).attr('href',updateURL);
		}
	}
	

</script><script type="text/javascript">
		$('#calImg1').live('click', function(e){

			var ua = window.navigator.userAgent;
			var msie = ua.indexOf('MSIE ');
			var trident = ua.indexOf('Trident/');

			//This Change Only For IE Browser Only
			if (msie > 0) {
				var offset_l = e.pageX;
				var offset_t = e.pageY;
				// IE 10 or older => return version number
				IEVersion = parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
				//For IE8
				if(IEVersion == 8){
					offset_l = e.clientX + document.body.scrollLeft + document.documentElement.scrollLeft;
					offset_t = parseInt(e.clientY + document.body.scrollTop + document.documentElement.scrollTop) - 178;
				}
				var left = offset_l;
				var top = offset_t;

				//Set Left And Top Style For Calendar Box
				setTimeout(function(){
					$('.calendar').css('left',left);
					$('.calendar').css('top',top);
				},100);
			}
		});
	</script><script type="text/javascript">
	$(document).ready(function() {
		setTimeout("imageResizeBrandLogoTop()",2000);
	});

	function imageResizeBrandLogoTop() {
		$("#brandImageTop img").each(function () {
			var width = $(this).width();    // Current image width
			var height = $(this).height();  // Current image height
			if($("#brandImageTop").css('width')) {
				$(this).removeAttr('height');
				$(this).removeAttr('width');

				var maxWidth = $("#brandImageTop").css('width'); // Max width for the image
				maxWidth = maxWidth.replace('px','');

				var maxHeight = $("#brandImageTop").css('height');    // Max height for the image
				maxHeight = maxHeight.replace('px','');

				maxHeightOld = maxHeight;

				if(maxHeight > height){
					maxHeight = height;    // Max height for the image
				}

				if(maxWidth > width){
					maxWidth = width;    // Max height for the image
				}

				var ratio = 0;  // Used for aspect ratio
				// Check if the current width is larger than the max
				if (width > maxWidth) {
					ratio = maxWidth / width;   // get ratio for scaling image
					$(this).css("width", maxWidth); // Set new width
					$(this).css("height", height * ratio);  // Scale height based on ratio
					height = height * ratio;    // Reset height to match scaled image
					width = width * ratio;    // Reset width to match scaled image
				}

				// Check if current height is larger than max
				if (height > maxHeight) {
					ratio = maxHeight / height; // get ratio for scaling image
					$(this).css("height", maxHeight);   // Set new height
					$(this).css("width", width * ratio);    // Scale width based on ratio
					width = width * ratio;    // Reset width to match scaled image
					height = height * ratio;    // Reset height to match scaled image
				}

				//Set Margin Top By Aspect Ration Of Height
				var currImgHeight = height;
				var ratioHeight = maxHeightOld - currImgHeight;

				if(ratioHeight > 1){
					ratioHeight = ratioHeight / 2;
				}
				$(this).css('margin-top',ratioHeight);

				// Margin Bottom
				var marginBottom = 40 - ratioHeight - height;

				// Set Margin  Bottom
				$(this).css('margin-bottom', marginBottom);

				//Set Margin left By Aspect Ration Of Width
				var currImgWidth = width;
				var ratioWidth = maxWidth - currImgWidth;
				if(ratioWidth > 1){
					ratioWidth = ratioWidth / 2;
				}
				$(this).css('margin-left',ratioWidth);
			}
		});
	}

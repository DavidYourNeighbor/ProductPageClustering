				//This JS Function Used To Validation Search Form Fields
				function submitFrmTopSearch(srcType) {
					var searchValue = '';
					if (document.getElementById('txtSearch1')) {
						searchValue = document.getElementById('txtSearch1').value.replace(/^\s+|\s+$/g,"");
						if (searchValue == "Enter Search Keyword") {
							return false;
						}
						if (searchValue == '') {
							document.getElementById('txtSearch1').value = "Enter Search Keyword";
							return false;
						}
					}
					objForm = document.frmTopSearch;
					objForm.srcType.value = srcType;
					objForm.submit();
				}

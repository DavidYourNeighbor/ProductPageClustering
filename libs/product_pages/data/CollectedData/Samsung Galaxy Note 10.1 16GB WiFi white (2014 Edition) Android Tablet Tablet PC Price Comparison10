	<script type="text/javascript">
		//This Function Will Set All Multi Buy For Fields Value For Remamber On "Add To Multibuy +", "+/-" And "Remove" Button/Link Hit Case
		function setRemeberMultibuyFields(){
			//Get And Set All Multi Buy Form Value To remember After "Add To Multi Buy" Button Hit
			objFormMltBuy = document.getElementById("frmmultiProdDetail");

			lstTitle = objFormMltBuy.lstTitle.value;
			txtTitleOther = objFormMltBuy.txtTitleOther.value;
			txtName = objFormMltBuy.txtName.value;
			txtEmail = objFormMltBuy.txtEmail.value;
			txtPostCode = objFormMltBuy.txtPostCode.value;
			txtContectNo = objFormMltBuy.txtContectNo.value;
			txtDelDate = objFormMltBuy.txtDelDate.value;
			txtPriceBeat = objFormMltBuy.txtPriceBeat.value;
			taNote = objFormMltBuy.taNote.value;

			//Set To Hidden Fields
			frmAddmulti = document.getElementById("frmAddmulti");
			frmAddmulti.remlstTitle.value = lstTitle;
			frmAddmulti.remtxtTitleOther.value = txtTitleOther;
			frmAddmulti.remtxtName.value = txtName;
			frmAddmulti.remtxtEmail.value = txtEmail;
			frmAddmulti.remtxtPostCode.value = txtPostCode;
			frmAddmulti.remtxtContectNo.value = txtContectNo;
			frmAddmulti.remtxtDelDate.value = txtDelDate;
			frmAddmulti.remtxtPriceBeat.value = txtPriceBeat;
			frmAddmulti.remtaNote.value = taNote;
		}


		// This JS Function Set Product As Multi By Product
		function jsAddMultiBuy(){

			//Set All Multi Buy Form Value To remember After "Add To Multi Buy" Button Hit
			setRemeberMultibuyFields();

			setAction = document.forms["frmAddmulti"].elements[0].value = 'addMultiBuyProd';
			document.forms["frmAddmulti"].submit();
		}

		//This JS Function Incr.. Multi Buy Product Session
		function jsIncMultiQty(spRecId){

			setAction = document.forms["frmAddmulti"].elements[0].value = 'changeMultiBuyQty';
			var qtyField = document.getElementById("addMultiQuantuty"+spRecId);
			qty = qtyField.value;
			//Number Validation
			var numbers = /^[0-9]+$/;
			if(!(qty.match(numbers))) {
				alert("Please enter numeric value only");
				qtyField.focus();
				return false;
			}

			//Validation For Positive Value Only and not greater than 999
			if(parseInt(qty) <= 0 || parseInt(qty)  >= 999){
				alert("Please enter valid Quantity");
				qtyField.focus();
				return false;
			}

			qtyField.value = parseInt(qty) + 1;
			document.getElementById("changeSpRecId").value = spRecId;

			//Set All Multi Buy Form Value To remember After "Add To Multi Buy" Button Hit
			setRemeberMultibuyFields();

			document.forms["frmAddmulti"].submit();
		}

		//This JS Function Decr.. Multi Buy Product Session
		function jsDecMultiQty(spRecId){
			setAction = document.forms["frmAddmulti"].elements[0].value = 'changeMultiBuyQty';
			var qtyField = document.getElementById("addMultiQuantuty"+spRecId);
			qty = qtyField.value;

			//Number Validation
			var numbers = /^[0-9]+$/;
			if(!(qty.match(numbers))) {
				alert("Please enter numeric value only");
				qtyField.focus();
				return false;
			}

			//Validation For Positive Value Only
			if(parseInt(qty) <= 0 ){
				alert("Please enter valide Quantity");
				qtyField.focus();
				return false;
			}
			qtyField.value = parseInt(qty) - 1;
			document.getElementById("changeSpRecId").value = spRecId;

			//Set All Multi Buy Form Value To remember After "Add To Multi Buy" Button Hit
			setRemeberMultibuyFields();

			document.forms["frmAddmulti"].submit();
		}

		//This JS Function Delete.. Multi Buy Product Session
		function jsDelMultiQty(spRecId){
			setAction = document.forms["frmAddmulti"].elements[0].value = 'deleteMultiBuyQty';
			document.getElementById("addMultiQuantuty"+spRecId).value = 0;
			document.getElementById("changeSpRecId").value = spRecId;

			//Set All Multi Buy Form Value To remember After "Add To Multi Buy" Button Hit
			setRemeberMultibuyFields();

			document.forms["frmAddmulti"].submit();
		}



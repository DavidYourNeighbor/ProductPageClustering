</div></div><script type="text/javascript" src="http://www.electricshop.com/js/common.js"></script><script type="text/javascript" src="http://www.electricshop.com/js/fieldvalidation.js"></script><script type="text/JavaScript">

	
	// Js Separator
	var separator = '^~_/';

	var xmlHttp;

	function GetXmlHttpObject() {
		try	{
			// Firefox, Opera 8.0+, Safari
			xmlHttp = new XMLHttpRequest();
		}
		catch(e) {
			// Internet Explorer
			try	{
				xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");
			}
			catch(e) {
				try {
					xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
				}
				catch(e) {
					alert("Your browser does not support AJAX!");
					return false;
				}
			}
		}
	}

	
	function stateChanged() {
		if(xmlHttp.readyState == 4 || xmlHttp.readyState == "complete") {
			strResponseText = xmlHttp.responseText;

			arrResponseText = strResponseText.split(separator);


			if(arrResponseText.length == 2) {
				//alert("Selected combination is not available. Please select different combination.");

				var objSubProdId = document.getElementById('subProId');

				var objSubProdIdStockAlert = document.getElementById('subProdStockAlert');

				var oldValue = document.getElementById('attributeValue' + arrResponseText[1]).value;

				var objAttVal = document.getElementById('lstAttrVal' + arrResponseText[1]);

				//objAttVal.value = oldValue;

				if(objSubProdId) {
					objSubProdId.value = 0;
				}

				if(objSubProdIdStockAlert) {
					objSubProdIdStockAlert.value = 0;
				}
			}
			else {

				arrResponseText = strResponseText.split(separator);
				// Variables
				var wasPrice = arrResponseText[0];
				var nowPrice = arrResponseText[1];
				var savePrice = arrResponseText[2];
				var image = arrResponseText[3];
				var stock = arrResponseText[4];
				var flagImageChange = arrResponseText[5];
				var newSubProdId = arrResponseText[6];
				var flagBuyNow = arrResponseText[7];
				var flagStockMail = arrResponseText[8];

				// Additional Information
				var objWasPrice = document.getElementById('wasPrice');
				var objNowPrice = document.getElementById('nowPrice');
				var objSavePrice = document.getElementById('savePrice');
				var objdivImg = document.getElementById('divImg');
				var objdivStock = document.getElementById('divStock');
				var objSubProdId = document.getElementById('subProId');
				var objSubProdIdStockAlert = document.getElementById('subProdStockAlert');
				var objBuyNow = document.getElementById('buwNowBnt');

				if(objWasPrice) {
					objWasPrice.innerHTML = wasPrice;
				}
				if(objNowPrice) {
					objNowPrice.innerHTML = nowPrice;
				}
				if(objSavePrice) {
					objSavePrice.innerHTML = savePrice;
				}
				if(objdivImg && flagImageChange == 'Y') {
					//objdivImg.innerHTML = image;
				}
				if(objdivStock) {
					objdivStock.innerHTML = stock;
				}
				if(objSubProdId) {
					objSubProdId.value = newSubProdId;
				}
				if(objSubProdIdStockAlert) {
					objSubProdIdStockAlert.value = newSubProdId;
				}
				objBuyNow.innerHTML = '';
				if(objBuyNow) {
					if(flagBuyNow == 'Y') {
						objBuyNow.innerHTML = "<a href=\"javascript:;\" class=\"comm-btn3 buynowbtn\" onclick=\"javascript:return submitFrmCart(1);\">Add +</a>";
					}
					else {
						if(flagStockMail == 'Y') {
							objBuyNow.innerHTML += "<a href=\"#\" title=\"Email me when in stock\" class=\"lnk-typ2 mailme stockalert-js\">Email me when in stock &raquo;</a>";
						}
						objBuyNow.innerHTML += "<a href=\"javascript:;\" class=\"comm-btn3 buynowbtn disable\" onclick=\"javascript:;\">Add +</a>";
					}

				}
			}
		}
		$(document).ready(function() {
			$(".stockalert-js").colorbox({width:"50%", inline:true, href:"#stockalert-popup"});
		});
	}

	//function to addReview
	function addReview(objForm) {
		var flReturn = true;

		// Tilte
		if(!notEmptyFront(objForm.txtTitle, "", false, true)) {
			document.getElementById('titleEr').style.display = "";
			document.getElementById('txtTitle').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('titleEr').style.display = "none";
			document.getElementById('txtTitle').className = "frm-txtbox1";
		}

		if(!notEmptyFront(objForm.taReview, "", false, true)) {
			document.getElementById('comtnEr').style.display = "";
			document.getElementById('taReview').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('comtnEr').style.display = "none";
			document.getElementById('taReview').className = "frm-txtbox1";
		}

		var str = objForm.taReview.value;
		str=str.replace(/[^A-Za-z0-9.-:/$ ]/g, "");
		str=str.replace(/\n/g, "");

		if(str == "") {
			document.getElementById('comtnEr').style.display = "";
			document.getElementById('taReview').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('comtnEr').style.display = "none";
			document.getElementById('taReview').className = "frm-txtbox1";
		}

		if(!flReturn) {
			document.getElementById('commErr').style.display = "";
			if(document.getElementById('commErrMsg')) {
				document.getElementById('commErrMsg').style.display = "none";
			}
			return flReturn;
		}
		else {
			document.getElementById('commErr').style.display = "none";
			if(document.getElementById('commErrMsg')) {
				document.getElementById('commErrMsg').style.display = "none";
			}
			objForm.submit();
		}
	}

	//function to add stockAlert
	function addstockAlert(objForm) {

		emailErrMsg = "Please specify Email Address.";
		invalidEmailMsg = "Invalid email address. Please specify email address again.";

		var flReturn = true;
		// Name
		if(!notEmptyFront(objForm.txtName, "", false, true)) {
			document.getElementById('nameEr').style.display = "";
			document.getElementById('txtName').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('nameEr').style.display = "none";
			document.getElementById('txtName').className = "frm-txtbox1";
		}

		// Email validation
		if(!validateEmailFront(objForm.txtEmail, emailErrMsg, true, invalidEmailMsg, "emailEr")) {
			document.getElementById('txtEmail').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('txtEmail').className = "frm-txtbox1";
		}

		if(!flReturn) {
			document.getElementById('commErr').style.display = "";
			if(document.getElementById('commErrMsg')) {
				document.getElementById('commErrMsg').style.display = "none";
			}
			return flReturn;
		}
		else {
			document.getElementById('commErr').style.display = "none";
			if(document.getElementById('commErrMsg')) {
				document.getElementById('commErrMsg').style.display = "none";
			}
			objForm.submit();
		}
	}

	// Function to addCart
	function submitFrmCart(count) {
		subProdId = document.frmProductDetail.subProId.value;
		if(subProdId == 0) {
			alert("Selected combination is not available. Please select different combination.");
			return false;
		}

		objForm = document.frmProductDetail;

		var regQuantity = /^([0-9]+)$/;

		if(!regQuantity.test(objForm.txtQuantity.value) || objForm.txtQuantity.value == 0) {
			alert("Please enter valid Quantity");
			objForm.txtQuantity.value = 1;
			return false;
		}

		if(count == '1') {
			objForm.setAction.value = "addCart";
		}
		else {
			objForm.setAction.value = "WishList";
		}

		objForm.submit();
	}
	
	
	//This Function Set Selected Package Sub Product Id And Unchecked All Other Package Checkboxes
	function jsSetPackageId(objForm, pckSpRecId){
		
		var tempObj;
		//Looping Until All Package Checkboxes 
		for(var i = 0; i < objForm.noOfPackages.value; i++) {
			tempObj = document.getElementsByName("relatedPck[" + i + "]");
			//Set Check Box Uncheked Other Then Current Clicked
			if(tempObj[0].value != pckSpRecId) {
				tempObj[0].checked = false;
			}
			else {
				//Case For All Unchecked
				if(tempObj[0].checked == false){
					//Set Hidden Values With No Value
					objForm.flagAddPackageInCart.value = 'N';
					objForm.addPackageInCart.value = '0';
				}
				//Case For Current Selected
				else {
					//Set Hidden Values With Selected Package Check Box Sub Product Id
					objForm.flagAddPackageInCart.value = 'Y';
					objForm.addPackageInCart.value = pckSpRecId;
				}
			}
		}
		

	}


	//This Funcion Used To Validate And Submit Multi Buy Form Data
	function addMiltuProdDetail(){
		
		setAction = document.forms["frmmultiProdDetail"].elements[0].value = 'addMultiProdDetail';
		objForm = document.getElementById("frmmultiProdDetail");
		
		//Form Validation
		emailErrMsg = "Please specify E-mail.";
		invalidEmailMsg = "Invalid E-mail. Please specify E-mail again.";
		
		//For Price Validation
		priceValidReg = /^(\d*([.,](?=\d{3}))?\d+)+((?!\2)[.,]\d\d)?$/;
		
		var flReturn = true;
		
		//For Multi By Product Session Check
		cntMulSessProd = 0;
		
		if(cntMulSessProd != '') {
				
			if(parseInt(cntMulSessProd) < parseInt(3)){
				document.getElementById('sessEr').style.display = "";
				flReturn = false;
			}
			else {
				document.getElementById('sessEr').style.display = "none";
			}
		}
		else {
			
			document.getElementById('sessEr').style.display = "";
			flReturn = false;
		}
	
		//For Title If Title Selected As Other
		if(objForm.lstTitle.value=='5'){
			if(!notEmpty(objForm.txtTitleOther, "Title ", false, true)) {
				document.getElementById('otherTtlEr').style.display = "";
				document.getElementById('txtTitleOther').className = "frm-txtbox1 error-txtbox";
				flReturn = false;
			}
			else {
				document.getElementById('otherTtlEr').style.display = "none";
				document.getElementById('txtTitleOther').className = "frm-txtbox1";
			}
		}	

		// Name
		if(!notEmptyFront(objForm.txtName, "", false, true)) {
			document.getElementById('nameEr').style.display = "";
			document.getElementById('txtName').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('nameEr').style.display = "none";
			document.getElementById('txtName').className = "frm-txtbox1";
		}

		// Email validation
		if(!validateEmailFront(objForm.txtEmail, emailErrMsg, true, invalidEmailMsg, "mailErr")) {
			document.getElementById('mailErr').style.display = "";
			document.getElementById('txtEmail').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('mailErr').style.display = "none";
			document.getElementById('txtEmail').className = "frm-txtbox1";
		}

		
		// Post Code
		if(!notEmpty(objForm.txtPostCode, "Post Code", false, true)) {
			document.getElementById('postErr').style.display = "";
			document.getElementById('txtPostCode').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('postErr').style.display = "none";
			document.getElementById('txtPostCode').className = "frm-txtbox1";
		}
		
		//	Phone No.
		if(!notEmpty(objForm.txtContectNo, "Phone No.", false, true)) {
			document.getElementById('phnNuErr').style.display = "none";
			document.getElementById('phnErr').style.display = "";
			document.getElementById('txtContectNo').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('phnErr').style.display = "none";
			if(!Number(objForm.txtContectNo.value)){
				document.getElementById('phnNuErr').style.display = "";
				document.getElementById('txtContectNo').className = "frm-txtbox1 error-txtbox";
				flReturn = false;
			}
			else {
				document.getElementById('phnNuErr').style.display = "none";
				document.getElementById('txtContectNo').className = "frm-txtbox1";
			}
		}
		
		
		//	Delivery date
		if(!notEmpty(objForm.txtDelDate, "Delivery date", false, true)) {
			document.getElementById('futureDateErr').style.display = "none";
			document.getElementById('dateErr').style.display = "";
			document.getElementById('txtDelDate').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			document.getElementById('dateErr').style.display = "none";
			//Validate For Given Date Must Future Date
			var myDate = objForm.txtDelDate.value;
			var strDateInFormate = myDate.slice(3,5) + '/' + myDate.slice(0,2) + '/' + myDate.slice(6,10);
			
			//Set After Two Month Date In String Formate
			var strStartDate = new Date(strDateInFormate);
			
			//Get Current Date
			var now = new Date();
			
			//Validate For Future Date
			if(strStartDate.getTime() <= now.getTime()){
				document.getElementById('futureDateErr').style.display = "";
				document.getElementById('txtDelDate').className = "frm-txtbox1 error-txtbox";
				flReturn = false;
			}
			else {
				document.getElementById('futureDateErr').style.display = "none";
				document.getElementById('txtDelDate').className = "frm-txtbox1";
			}
		}
		
		//Price to beat Validation	
		//if(!isNumeric(objForm.txtPriceBeat, "Price to Beat", false, true)){
		if(!notEmpty(objForm.txtPriceBeat, "Price to Beat", false, true)){
			document.getElementById('negPrice').style.display = "none";
			document.getElementById('priceErr').style.display = "";
			document.getElementById('txtPriceBeat').className = "frm-txtbox1 error-txtbox";
			flReturn = false;
		}
		else {
			
			document.getElementById('priceErr').style.display = "none";
			if(!Number(objForm.txtPriceBeat.value) || objForm.txtPriceBeat.value <= 0) {
				document.getElementById('negPrice').style.display = "";
				document.getElementById('txtPriceBeat').className = "frm-txtbox1 error-txtbox";
				flReturn = false;
			}
			else {
				document.getElementById('negPrice').style.display = "none";
				document.getElementById('txtPriceBeat').className = "frm-txtbox1";
			}
		}
		
		// Characters
		if(!notEmpty(objForm.txtCharacters, "", false, true) || (objForm.txtCharacters.value.length < 6)) {
			document.getElementById('char').style.display = "";
			document.getElementById('txtCharacter').className = "frm-txtbox1 error-txtbox";		
			flReturn = false;
		}
		else {
			document.getElementById('char').style.display = "none";
			document.getElementById('txtCharacter').className = "frm-txtbox";
		}

		if(!flReturn) {
			document.getElementById('commErr').style.display = "";
			if(document.getElementById('commErrMsg')) {
				document.getElementById('commErrMsg').style.display = "none";
			}
			return flReturn;
		}
		else {
			document.getElementById('commErr').style.display = "none";
			if(document.getElementById('commErrMsg')) {
				document.getElementById('commErrMsg').style.display = "none";
			}
			objForm.submit();
		}
	}

	//]]>
</script>

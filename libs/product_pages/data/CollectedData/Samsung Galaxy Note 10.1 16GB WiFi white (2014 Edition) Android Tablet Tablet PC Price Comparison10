/*
	This JS Function Used To Scoll Content Where Link Is Clicked
*/
function jsScrollME(){
	//For Scroll(Focus) On URL Id Content On Page Load/Re-Load

	var pathname = window.location;
	if(String(pathname).split("#").length > 1){
		$('html, body').animate({scrollTop: $("#"+decodeURIComponent(String(pathname).split("#")[1])).offset().top - 133}, 600);
	}
}




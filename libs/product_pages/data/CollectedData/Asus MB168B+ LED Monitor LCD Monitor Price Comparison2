    $(".pa-submit").click(function () {
      //validate form (check email addres + at least one checkbox is selected)

      var pa_email = $(".pa-textfield").val();
      var pa_product = $("#title h1").text();
      var pa_cb1 = $("#pa-cb1").is(':checked');
      var pa_cb2 = $("#pa-cb2").is(':checked');
      var pa_cb3 = $("#pa-cb3").is(':checked');
      var pa_cb4 = $("#pa-cb4").is(':checked');
      var pa_cb5 = $("#pa-cb5").is(':checked');

      
      if ($('.product-alert input[type=checkbox]').filter(':checked').length > 0) { //check that we have one checkbox checked (clean up)
        if(isValidEmailAddress(pa_email)) { //check email address is valid

          //submit form data via ajax?
          $.post("/alerthelper.aspx", { email: pa_email, cb1: pa_cb1, cb2: pa_cb2, cb3: pa_cb3, cb4: pa_cb4, cb5: pa_cb5, intstockcode: 257296});
          
          //success message 
          var message = "Thank you for signing up for {0} product alerts.";
          message = message.replace("{0}", "<strong>" + pa_product + "</strong>").replace("{1}", "<strong>" + pa_email + "</strong>");
          
          msgBar(message, "msgbar note", 6500);
          //reset form
          $(".pa-submit").removeClass('pa-submit-active');
          $('#product-alert-form')[0].reset();
          return false;
        } else {
					 msgBar("Please enter a valid email address.", "msgbar error", 2500);
        }
      } else {
				 msgBar("Please select at least one product alert", "msgbar error", 2500);
      }
    });

});
    
    
</script>


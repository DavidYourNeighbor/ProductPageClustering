<script type='text/javascript'>
	function openWoChat(returnWindow)
	{
		var newWindow = window.open('http://simi-valley.dedi.melbourne.co.uk/chat/chatstart.htm?domain=www.365games.co.uk', 'Chat', 'width=484,height=361');

		if (returnWindow)
		{
			return newWindow;
		}
	}

	function openWoCallback(returnWindow)
	{
		var newWindow = window.open('http://simi-valley.dedi.melbourne.co.uk/chat/callback.htm?domain=www.365games.co.uk', 'Chat', 'width=484,height=361');

		if (returnWindow)
		{
			return newWindow;
		}
	}

	function setUpInvite()
	{
		$("#woInvite").css(
		{
			position: "fixed",
			top: "auto",
			bottom: "0px",
			left: "auto",
			right: "0px"
		});
	}

	var originalWoAfterLoad = woAfterLoad;
	var originalWoMove = woMove;
	var originalGetIImgw = getIImgw;
	var originalStartInvite = startInvite;
	var originalFadeInStep = fadeInStep;
	var originalFadeOutStep = fadeOutStep;
	var originalStartChat = startChat;

	woAfterLoad = function()
	{
		originalWoAfterLoad();
		setUpInvite();
	}

	woMove = function()
	{
		originalWoMove();
		setUpInvite();
	}

	getIImgw = function()
	{
		originalGetIImgw();
		setUpInvite();
	}

	startInvite = function()
	{
		invType = 5;
		originalStartInvite();
		setUpInvite();
	}

	fadeInStep = function()
	{
		originalFadeInStep();
		setUpInvite();
	}

	fadeOutStep = function()
	{
		originalFadeOutStep();
		setUpInvite();
	}

	startChat = function(event)
	{
		var offset = $(event.currentTarget).offset();

		if (event.clientX < (offset.left + $(event.currentTarget).width() - closeX) || event.clientyclientY < (offset.top + $(event.currentTarget).height() - closeY))
		{
			this.chatWindow = openWoChat(true);
			this.chatWindow.focus();
			this.chatWindow.opener = window;
		}

		stopInvite();
	}
</script>

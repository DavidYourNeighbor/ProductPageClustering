        function displayVisiLoginSSLText()
        {
            var el = document.getElementById('visiLoginSSLText');
            el.innerHTML = '<strong>Secure log in on every page</strong><br />The security of your personal information is of utmost importance. Be assured that when you log in using our visiLogin (located in the upper right of every page on our site) your username and password are secure.<br /><br />When you click the \"Log In\" button, your user name and password are sent to our servers using a Secure Sockets Layer (SSL) connection.  This secure and encrypted connection protects the privacy of any information you use or supply to us.<br /><br />For more information on our security measures, please visit our <a href=\"http://www.uk.insight.com/en-gb/static/privacy.php\">privacy policy<\/a>.';
            document.getElementById('visiLoginSSL').style.display = 'block';
        }

        function visiLoginSSLClose()
        {
            document.getElementById('visiLoginSSL').style.display = 'none';
            document.getElementById('visiLoginSSLText').innerHTML = '&nbsp;';
        }
    

			<div>﻿<script language="javascript1.2" type="text/javascript">

	document.write ("<link rel='stylesheet' type='text/css' href='http://www.hugheselectrical.co.uk/ebay/common/css/layout.css'/>");	

	// Quite simply loop our tabs and tab headers to see if they are on or off

	function changeTab(tabId)

	{

 for(var i=0; i<	document.getElementById("tabTopContainer").getElementsByTagName('li').length; i++)
 {

 	var tabTop = document.getElementById("li_anchor2_" + (i+1));
 	var tabContent = document.getElementById("li_ul_anchor2_" + (i+1));

 	if((i+1)==tabId)

 	{

 tabTop.className = "active";

 tabContent.className = "showme";

 	}

 	else

 	{

 tabTop.className = "inactive";

 tabContent.className = "hideme";

 	}
 }
}
	function showImage(thumbElement) 
	{
	 // show relevant image
	 var ai = document.getElementById('ActiveImage');
	 ai.src = thumbElement.getElementsByTagName('img')[0].src;

	 // remove 'selected' from classes
	 var anchors = document.getElementsByTagName('a');

	 for (var i=0; i<anchors.length; i++)
	 {
 	if (anchors[i].className.indexOf('thumbImageLink') != -1) 
 anchors[i].className = removeClass(anchors[i].className, 'selected');
	 }	 

	 // add 'selected' to appropriate class
	 thumbElement.className += ' selected';
 }

</script>


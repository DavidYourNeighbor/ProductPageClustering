<script language="javascript">
$(document).ready(function() {

    $('#uname-clearLR').show();
    $('#unameLR').hide();
    $('#uname-clearLR').focus(function() {
        $('#uname-clearLR').hide();
        $('#unameLR').show();
        $('#unameLR').focus();
    });
    $('#unameLR').blur(function() {
        if($('#unameLR').val() == '') {
            $('#uname-clearLR').show();
            $('#unameLR').hide();
        }
    });

    $('#password-clearLR').show();
    $('#passLR').hide();

    $('#password-clearLR').focus(function() {
        $('#password-clearLR').hide();
        $('#passLR').show();
        $('#passLR').focus();
    });
    $('#passLR').blur(function() {
        if($('#passLR').val() == '') {
            $('#password-clearLR').show();
            $('#passLR').hide();
        }
    });
});
</script>


<script type="text/javascript" language="javascript">
    function searchBoxOnclick(control) {
       if(control.value=="Your E-mail here")
       {
           control.value = ""; 
           control.className = "text_box";
       }
    } 
    function searchBoxOnblur(control) {
        if(control.value=="")
        {
           control.value = "Your E-mail here"; 
           control.className = "text_box textInactive";
        }      
    }        
</script>
    

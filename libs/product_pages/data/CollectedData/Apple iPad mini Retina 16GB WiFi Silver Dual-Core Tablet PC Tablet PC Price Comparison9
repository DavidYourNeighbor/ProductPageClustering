                    $(window).load(function()
                    {
                         $('#ajustor_play').hide();

                         $('#ajustor_pause').click(function () {
                                $('#ajustor_pause').hide();
                                $('#ajustor_play').show();
                                $('#bc-slider-menu').data('nivoslider').stop();
                        });
                        $('#ajustor_play').click(function () {
                                $('#ajustor_pause').show();
                                $('#ajustor_play').hide();
                                $('#bc-slider-menu').data('nivoslider').start();
                        });
                        var total = $('#bc-slider-menu img').length;
                        if(total>1){
                             $('#ajustor_pause').show();                                                 
                        }
                        var rand = Math.floor(Math.random()*total);
                        $("#bc-slider-menu").nivoSlider({
                                effect:'fade',
                                startSlide:rand,
                                pauseOnHover: false,
                                animSpeed:500,   
                                pauseTime:3000  
                                ,controlNav:false // 1,2,3... navigation
                                ,directionNav:false // Next & Prev navigation
                                ,directionNavHide:true // Only show on hover


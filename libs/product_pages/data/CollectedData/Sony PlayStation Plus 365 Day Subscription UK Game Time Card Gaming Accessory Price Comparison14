		<script>
		 	$(function() {
		 		
					 argos.mobilepage.saveTrolley({quantity : 0.0});
						 		 
				argos.mobilepage.displayPersistentTrolley();
				argos.mobilepage.setSpecialOfferBackLink("specialOfferTitle","< Return to special offer","specialOfferPDPBackLink");
				$.ajax({
					url: '/webapp/wcs/stores/servlet/OneClickMobilePDPCmd?storeId=10151&langId=110&partNumber=9099005&collectableOnlyProduct=false&delivarableOnlyProduct=false&noCollectOrDeliverProduct=false&delivarable=true&collectable=true&displayLayoutB=false&catentryId=803656&productId=803656&directedFrom=&pdMaxAvailable=10&oneClickReserveFailure=',					
					success: function (html) {
						$('#stockDetailsDiv').empty().append(html);
						var isInStockField = document.getElementById("isInStock");
						
						var stock_msg = $("#avlMessaging").find("p").text();
						if (stock_msg=='Unable to check stock at this time'){
							if(s !== null){
								s.prop6 = "Unable to check stock at this time";
								s.linkTrackVars = "prop6";
								s.linkTrackEvents = "none";
								s.tl(this,'o', 'Product Detail Page|Stock Status');
							}
						}
						
						if (stock_msg.match(/Available to collect in store in/gi)){
							s.prop63='Available to collect in store in a few days';
							s.linkTrackVars = "prop63";
							s.linkTrackEvents = "none";
							s.tl(this,'o', 'Product Detail Page|Availability');
						}
						
						if (stock_msg.match(/order by 1pm to collect from 4pm today/gi)){
							s.prop63='Available to collect in store today';
							s.linkTrackVars = "prop63";
							s.linkTrackEvents = "none";
							s.tl(this,'o', 'Product Detail Page|Availability');
						}
						
						if (stock_msg.match(/order by midnight/gi)){
							s.prop63='Available to collect in store tomorrow';
							s.linkTrackVars = "prop63";
							s.linkTrackEvents = "none";
							s.tl(this,'o', 'Product Detail Page|Availability');
						}
						
						if(isInStockField != null && isInStockField.value == 'false') {
							if(s !== null){
								s.prop63 = "Currently out of stock for store collection";
								s.eVar16 = document.getElementsByName("storeNumber")[0].value;
								s.events = "event32";
								s.eVar74= s.pageName;
								s.linkTrackVars = "prop63,eVar16,events,eVar74";
								s.linkTrackEvents = "event32";
								s.tl(this,'o', 'Product Detail Page|Stock Status');
							}
						}
					},
					cache:false
				});
		 	});
		</script>

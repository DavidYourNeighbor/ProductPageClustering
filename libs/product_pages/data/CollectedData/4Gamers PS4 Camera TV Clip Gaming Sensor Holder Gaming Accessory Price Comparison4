<script type="text/javascript" language="javascript">

if (!Array.prototype.forEach)
{
Array.prototype.forEach = function(fun)
{
var len = this.length;
if (typeof fun != "function")
throw new TypeError();

var thisp = arguments[1];
for (var i = 0; i < len; i++)
{
if (i in this)
fun.call(thisp, this[i], i, this);
}
};
}
var mySmallImage=new Array("http://images.channelgrabber.com/MzM2/529f2605c006c.jpg","http://images.channelgrabber.com/MzM2/529f261190a4e.jpg","http://images.channelgrabber.com/MzM2/529f261dd5417.jpg","http://images.channelgrabber.com/MzM2/529f262a04aef.jpg","");


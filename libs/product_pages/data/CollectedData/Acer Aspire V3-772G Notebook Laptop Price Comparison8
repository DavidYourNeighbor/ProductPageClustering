function zoomswap(myElem) {
if(document.getElementById("zoomanchor")) {
document.getElementById("zoomanchor").href = myElem.href;
jQuery('.productImageLink').removeData('jqzoom');
jQuery('.productImageLink').jqzoom(zoomOptions).init();
jQuery('#productImageSmall').trigger("mouseenter");
}
}
</script>



<script type="text/javascript">
	var hPaBottomTotal = 0,
		hPaBottomCurPage = 1;

	var recommendBottomNextButtonClick = function(event){
		event.preventDefault();
		hPaBottomCurPage += 1;
		jQuery("#productSlotBottomPageSetList").animate({"left": "-=150"}, 300);
		recommendBottomButtonUpdate();
	};

	var recommendBottomPrevButtonClick = function(event){
		event.preventDefault();
		hPaBottomCurPage -= 1;
		jQuery("#productSlotBottomPageSetList").animate({"left": "+=150"}, 300);
		recommendBottomButtonUpdate();
	};

	function recommendBottomButtonUpdate(){
	
		if(hPaBottomCurPage <= 4){
			jQuery('#recommendBottomButtonPrev').removeClass('recommendArrowLeftBlack');
			jQuery('#recommendBottomButtonPrev').addClass('recommendArrowLeftGrey');
		}
		else{
			jQuery('#recommendBottomButtonPrev').removeClass('recommendArrowLeftGrey');
			jQuery('#recommendBottomButtonPrev').addClass('recommendArrowLeftBlack');
		}

		if((hPaBottomCurPage < 4) || (hPaBottomCurPage == hPaBottomTotal)){
			jQuery('#recommendBottomButtonNext').removeClass('recommendArrowRightBlack');
			jQuery('#recommendBottomButtonNext').addClass('recommendArrowRightGrey');
		}
		else{
			jQuery('#recommendBottomButtonNext').removeClass('recommendArrowRightGrey');
			jQuery('#recommendBottomButtonNext').addClass('recommendArrowRightBlack');
		}
	}

function itemsViewed(start, end, ids) {
	var viewed = [];
	for(var i = start; i < end; i++) {
		if (ids[i]) {
			viewed.push(i);
			viewed.push(ids[i]);
		}
	}
	return viewed;
}

	IRSRequire(['ASDA'], function () {
		jQuery(function() {

			var ArrPids = recentpids.split(",");
                        var pidsOrdered = ArrPids.reverse();

			recommend('PERSONA', pidsOrdered.shift() , recentcat ).done(function (modules) {
				jQuery('#productSlotBottomPageSetList').html(modules[0].html);
				
				jQuery( "#productSlotBottomPageSetList [name='pid']").each(function(){
				if((jQuery(this).val() == '003724183') || (jQuery(this).val() == '003724190')){
				  jQuery(this).closest('li').remove();
				}
				});
				
				var recommendedProductCount = jQuery('#productSlotBottomPageSetList li').length;
				if(recommendedProductCount > 0) {
				
					jQuery('#no-results').hide();
					
					jQuery('#recommendBottomButtonNext').removeClass('recommendArrowRightGrey');
					jQuery('#recommendBottomButtonNext').addClass('recommendArrowRightBlack');
				
					if (modules[0].meta != null && modules[0].meta != undefined) {
						jQuery('#recommendSlotBottomHeader').text(modules[0].meta.title);
					}
					
					hPaBottomTotal = jQuery('#productSlotBottomPageSetList > li').length;
					
					if(hPaBottomTotal > 4){
						hPaBottomCurPage = 4;
					}
					else{
						hPaBottomCurPage = hPaBottomTotal;
					}
					
					jQuery('#productSlotBottomPageSetList').css('width', (hPaBottomTotal * 150) + 'px');
					
					recommendBottomButtonUpdate();
					jQuery('#recommendBottomButtonPrev').bind('click', recommendBottomPrevButtonClick);
					jQuery('#recommendBottomButtonNext').bind('click', recommendBottomNextButtonClick);

					var ids = [];
					$items = jQuery('#productSlotBottomPageSetList > li');
					$items.each(function (index, item) {
						var $item = $items.eq(index),
						$hrefs = $item.find('a'),
						id = $item.find('form').attr('id').split('cartaddproduct')[1];
						ids.push(id);
						$hrefs.each(function (index, item) {
							var $href = $hrefs.eq(index),
							href =  $href.attr('href'),
							type = '';
							if ($href.children('.brand').length > 0) {
								type = 'title';
							} else if ($href.is('.moreinfobutton')) {
								type = 'more_info';
							} else if ($href.children('img').length > 0) {
								type = 'image';
							}
							if (href.indexOf('?') > 0) {
								href = href + '&' + modules[0].meta.clicked(id, type);
							} else {
								href = href + '?' + modules[0].meta.clicked(id, type);
							}
							$href.attr('href', href);
						});
						$item.find('.addtocartbutton').on('click keypress', function () {
							modules[0].meta.added(id);
						});
					});
					jQuery('#recommendBottomButtonPrev').on('click', function () {
						modules[0].meta.paged('left', itemsViewed(hPaBottomCurPage - 4, hPaBottomCurPage , ids));
					});
					jQuery('#recommendBottomButtonNext').on('click', function () {
						modules[0].meta.paged('right', itemsViewed(hPaBottomCurPage - 4, hPaBottomCurPage , ids));
					});
					modules[0].meta.placed(ids, itemsViewed(0, 4, ids));			
				}
			});
		});
	});
</script>

<div id="recommendBottomSlide" class="recommendSlide">
	<div id="paBottombuttonwrapper" class="pabuttonwrapper">
		<div class="recommendedProducts" id="recommendSlotBottom">
			<h3 id="recommendSlotBottomHeader">There are no recently viewed products to show</h3>
			<div id="recommendBottomPageSet" class="productPageSet">
				<ul id="productSlotBottomPageSetList"></ul>
			</div>
		</div>

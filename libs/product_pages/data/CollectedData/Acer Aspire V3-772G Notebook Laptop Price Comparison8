<script type="text/javascript">
var verticalDontForgetTotal = 0;
var verticalDontForgetCurPage = 1;

var verticalDontForgetNextButtonClick = function(){
  verticalDontForgetCurPage += 1;
  jQuery("#verticalDontForgetPageSet").animate({"left": "-=213"}, 300);
  verticalDontForgetButtonUpdate();
}

var verticalDontForgetPrevButtonClick = function(){
  verticalDontForgetCurPage -= 1;
  jQuery("#verticalDontForgetPageSet").animate({"left": "+=213"}, 300);
  verticalDontForgetButtonUpdate();
}

function verticalDontForgetButtonUpdate(){
  jQuery('#verticalDontForgetButtonPrev').unbind('click');
  jQuery('#verticalDontForgetButtonNext').unbind('click');
  if(verticalDontForgetCurPage == 1){
    jQuery('#verticalDontForgetButtonPrev').removeClass('verticalPAArrowLeftBlack');
	jQuery('#verticalDontForgetButtonPrev').addClass('verticalPAArrowLeftGrey');
  }else{
    jQuery('#verticalDontForgetButtonPrev').removeClass('verticalPAArrowLeftGrey');
	jQuery('#verticalDontForgetButtonPrev').addClass('verticalPAArrowLeftBlack');
    jQuery('#verticalDontForgetButtonPrev').bind('click', verticalDontForgetPrevButtonClick);
  }
  if(verticalDontForgetCurPage == verticalDontForgetTotal){
    jQuery('#verticalDontForgetButtonNext').removeClass('verticalPAArrowRightBlack');
	jQuery('#verticalDontForgetButtonNext').addClass('verticalPAArrowRightGrey');
  }else{
    jQuery('#verticalDontForgetButtonNext').removeClass('verticalPAArrowRightGrey');
	jQuery('#verticalDontForgetButtonNext').addClass('verticalPAArrowRightBlack');
    jQuery('#verticalDontForgetButtonNext').bind('click', verticalDontForgetNextButtonClick);
  }
}

function displayCustomerOpinions(url) {
	jQuery.getJSON(url).done(function( data ){
		var items = [];
			items.push('<li class="customerTestimonialsTitle">' + what_our_customer_saying + '</li>')
		 jQuery.each(data, function(key, val) {
			 var img_url = rating_image.replace("---NUMBER---", val.Rating);
			 items.push('<li><img src="' + img_url + '" alt="' + val.Rating + '" /> ' + val.Rating  + '/5<br />' + val.ReviewText + '</li>');
		 });
		 jQuery('<ul/>', {
			 'class': 'customerTestimonials',
			 html: items.join('')
			 }).prependTo('#indescriptionOCSWrapper');
	});
}

var indescriptionWrappers = '<div id="indescriptionWrapper"><div id="indescriptionDFWrapper"></div><div id="indescriptionOCSWrapper"></div><div id="indescriptionKFWrapper"></div></div>';
if(jQuery('#overviewContent').length > 0) 
{
  jQuery('#overviewContent').prepend(indescriptionWrappers);
}else
{
  jQuery('#descriptionsContent').prepend(indescriptionWrappers);
}

if(jQuery('#keyselling').length > 0)
{
  jQuery('#keyselling').prependTo(jQuery('#indescriptionKFWrapper'));
}

if((typeof dontforgetids !="undefined") && (dontforgetids != '')){
  jQuery.get("/on/demandware.store/Sites-ASDA-Site/default/Recommendations-Start?pids=" + dontforgetids, function(data) {
	  jQuery('#verticalDontForgetHeader').text('Don\'t forget');
	  jQuery('#verticalDontForgetPageSetList').append(data);
      verticalDontForgetTotal = jQuery('#verticalDontForgetPageSetList > li').length;
      jQuery('#verticalDontForgetPageSet').css('width', (verticalDontForgetTotal * 213) + 'px');
      verticalDontForgetButtonUpdate();
	  if(verticalDontForgetTotal > 0) {
	    jQuery('#verticalDontForgetButtonWrapper').prependTo(jQuery('#indescriptionDFWrapper'));
	    jQuery('#verticalDontForgetButtonWrapper').show();
	  }
    });
}
</script>

 
	

</div>



	 

	

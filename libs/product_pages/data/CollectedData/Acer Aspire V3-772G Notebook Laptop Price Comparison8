<script type="text/javascript">
var recentlyViewedAnimationReady = true;
function ScrollRecentUp() {
if(!recentlyViewedAnimationReady) {
return;
}
var scrollwrapper = jQuery('.lastvistedScrollWrapper');
var scrollunit = jQuery('.lastvistedScrollWrapper>.vistedScrollItem:first').outerHeight(true);
var buttonUp = jQuery(".recentUp.recentarrow");
var buttonDown = jQuery(".recentDown.recentarrow");
var toScroll = scrollwrapper.scrollTop() - scrollunit;
recentlyViewedAnimationReady = false;
scrollwrapper.stop(false).animate({ scrollTop: toScroll }, function() {
buttonDown.removeClass("hide");
recentlyViewedAnimationReady = true;
if(toScroll == 0) {
buttonUp.addClass("hide");
}
});
};
function ScrollRecentDown() {
if(!recentlyViewedAnimationReady) {
return;
}
var scrollwrapper = jQuery('.lastvistedScrollWrapper');
var scrollunit = jQuery('.lastvistedScrollWrapper>.vistedScrollItem:first').outerHeight(true);
var buttonUp = jQuery(".recentUp.recentarrow");
var buttonDown = jQuery(".recentDown.recentarrow");
var toScroll = scrollwrapper.scrollTop() + scrollunit;
recentlyViewedAnimationReady = false;
scrollwrapper.stop(false).animate({ scrollTop: toScroll }, function() {
buttonUp.removeClass("hide");
recentlyViewedAnimationReady = true;
if(0 >= scrollwrapper.children().size() * scrollunit - scrollwrapper.outerHeight() - toScroll - 4) {
buttonDown.addClass("hide");
}
});
};
function initScrollRecent() {
if(jQuery('.lastvistedScrollWrapper>.vistedScrollItem').length > 3) {
jQuery('.lastvistedScrollWrapper').before("<div class='recentUp recentarrow hide'></div>");
jQuery('.lastvistedScrollWrapper').after("<div class='recentDown recentarrow'></div>");
jQuery('.recentDown').click(ScrollRecentDown);
jQuery('.recentUp').click(ScrollRecentUp);
}
};
function ScrollBundlesLeft() {
var scrollwrapper = jQuery(this).parent().find('.bundleline');
var scrollunit = 186;
scrollwrapper.animate({ scrollLeft: scrollwrapper.scrollLeft() - scrollunit });
};
function ScrollBundlesRight() {
var scrollwrapper = jQuery(this).parent().find('.bundleline');
var scrollunit = 186;
scrollwrapper.animate({ scrollLeft: scrollwrapper.scrollLeft() + scrollunit });
};
function initScrollBundles() {
jQuery('.bundleline').each( function(){
if(jQuery(this).children().length > 7) {
jQuery(this).before("<div class='bundleleft bundlearrow'></div>");
jQuery(this).after("<div class='bundleright bundlearrow'></div>");
}
else{
jQuery(this).before("<div class='bundlearrowspacer'></div>");
jQuery(this).after("<div class='bundlearrowspacer'></div>");
}
});
jQuery('.bundleleft').click(ScrollBundlesLeft);
jQuery('.bundleright').click(ScrollBundlesRight);
};
jQuery("document").ready(initScrollRecent);
jQuery("document").ready(initScrollBundles);
</script>
</div>
</div>
</div>
</div>


	



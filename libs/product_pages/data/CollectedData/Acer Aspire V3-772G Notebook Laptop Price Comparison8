<script type="text/javascript">
var saveForLaterDisabled = false;
window.onload = function(){
var sflTopNavUpdate = function() {
var slfNumElmnt = jQuery('#userControls .sfl-counter');
var viewItemsElmnt = jQuery('.saveForLaterCont .viewSavedItems');
var sflURL = "/on/demandware.store/Sites-ASDA-Site/default/SaveForLater-List";
jQuery.ajax({
url: sflURL,
dataType: "json",
success: function(response) {
//check the length of the array in order to show the exact number of SFL items and link.
var itemsNumber = response.arrayIDs.length;
if(itemsNumber > 0 ) {
slfNumElmnt.text(itemsNumber).show();
if( viewItemsElmnt.length > 0 ) {
viewItemsElmnt.removeClass('hidden');
}
} else {
slfNumElmnt.hide();
if( viewItemsElmnt.length > 0 ) {
viewItemsElmnt.addClass('hidden');
}
}
}
});
};
//Init function that accepts SFL JSON from the minibasket AJAX request result.
var sflTopNavInit = function(sflJSON) {
// bind sfl custom event for the updating of counter.
jQuery('#userControls .sfl-counter').on('sflTopUpdateEvent', sflTopNavUpdate);
var slfNumElmnt = jQuery('#userControls .sfl-counter');
var viewItemsElmnt = jQuery('.saveForLaterCont .viewSavedItems');
//check the length of the array in order to show the exact number of SFL items and link.
var itemsNumber = sflJSON.arrayIDs.length;
if(itemsNumber > 0 ) {
var saveForLaterBTN = jQuery("#saveForLaterBtn");
slfNumElmnt.text(itemsNumber).show();
if(saveForLaterBTN.length > 0) {
viewItemsElmnt.removeClass('hidden');
saveForLaterBTN.addClass("haveSaved");
var pid = getPIDonPDP();
if(sflJSON.arrayIDs.indexOf(pid) != -1) {
saveForLaterBTN.addClass("itemSaved");
}
}
} else {
slfNumElmnt.hide();
}
};
var minicartHeaderInit = function() {
jQuery.ajax({
url: "/on/demandware.store/Sites-ASDA-Site/default/Cart-MiniCart",
type: 'get'
}).done(function(response){
if(!saveForLaterDisabled) {
var sflJSON = response.split("<!-- Start: listSFL.isml -->");
sflJSON = sflJSON[1].split("<!-- End: listSFL.isml -->");
sflJSON = jQuery.parseJSON(sflJSON[0]);
sflTopNavInit(sflJSON);
}
// replace the content of the basket.
if(true) {
jQuery('#minicart').replaceWith(response);
minicart.prepare();
} else {
//Make jQuery object from the response returned isml and hide it before replacing the old minicart.
var minicartReplace = jQuery(response);
if (false) {
minicartReplace.css('visibility', 'hidden');
}
jQuery('#minicart').replaceWith(minicartReplace);
app.minicart.init();
}
});
};
var myAccoutDropDownInit = function () {
//Show Drop Down Menu
var dropDownElement = jQuery('.topNavDropdown');
dropDownElement.removeClass("hidden");
//Triger Drop Down Menu
jQuery('.myAccount').click( function(event){
event.stopPropagation();
jQuery('.topNavDropdown').toggle();
});
jQuery(document).click( function(){
jQuery('.topNavDropdown').hide();
});
}
var getPIDonPDP = function () {
var form = jQuery("#buyOptionsContainer").closest("form");
var pid = "";
pid = form.attr("id");
pid = pid.replace("cartaddproduct", "");
return pid;
}
if (typeof Mediator != 'undefined') {
Mediator.subscribe(function(){
myAccoutDropDownInit();
minicartHeaderInit();
}, 'yepnope_allFilesLoaded');
} else {
myAccoutDropDownInit();
minicartHeaderInit();
}
};
</script>
<!-- End: header/gm_header.isml -->

<!-- End: pagecomponents/header -->



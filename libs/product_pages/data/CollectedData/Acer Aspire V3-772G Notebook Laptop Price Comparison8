<script type="text/javascript">
var tabPaBottomTotal = 0;
var tabPaBottomCurPage = 1;

var productDetailsPaBottomNextButtonClick = function(event){
	event.preventDefault();
	tabPaBottomCurPage += 1;
	jQuery("#productDetailsPaBottomPageSet").animate({"left": "-=156"}, 300);
	productDetailsPaBottomButtonUpdate();
}

var productDetailsPaBottomPrevButtonClick = function(event){
	event.preventDefault();
	tabPaBottomCurPage -= 1;
	jQuery("#productDetailsPaBottomPageSet").animate({"left": "+=156"}, 300);
	productDetailsPaBottomButtonUpdate();
}

function productDetailsPaBottomButtonUpdate(){
	if(tabPaBottomCurPage <= 6){
		jQuery('#productDetailsPaBottomButtonPrev').removeClass('productDetailsPaArrowLeftBlack');
		jQuery('#productDetailsPaBottomButtonPrev').addClass('productDetailsPaArrowLeftGrey');
	}else{
		jQuery('#productDetailsPaBottomButtonPrev').removeClass('productDetailsPaArrowLeftGrey');
		jQuery('#productDetailsPaBottomButtonPrev').addClass('productDetailsPaArrowLeftBlack');
		
	}
	if((tabPaBottomCurPage < 6) || (tabPaBottomCurPage == tabPaBottomTotal)){
		jQuery('#productDetailsPaBottomButtonNext').removeClass('productDetailsPaArrowRightBlack');
		jQuery('#productDetailsPaBottomButtonNext').addClass('productDetailsPaArrowRightGrey');
	}else{
		jQuery('#productDetailsPaBottomButtonNext').removeClass('productDetailsPaArrowRightGrey');
		jQuery('#productDetailsPaBottomButtonNext').addClass('productDetailsPaArrowRightBlack');
	}
}

function itemsViewed(start, end, ids) {
	var viewed = [];
	for(var i = start; i < end; i++) {
		if (ids[i]) {
			viewed.push(i);
			viewed.push(ids[i]);
		}
	}
	return viewed;
}

IRSRequire(['ASDA'], function () {
	jQuery(function() {
		var pid = '';
		var titleOfTab = '';
		if(universal_variable != null && universal_variable != undefined && universal_variable.Product != null) {
			pid = universal_variable.Product.id; 
		}
		recommend(['GMPPRHT','GMPPBOT'], pid, '').done(function (modules) {
			for (var whichmod = 0; whichmod < modules.length; whichmod++) {
			
				if (modules[whichmod].zone === 'GMPPRHT') {
					var themodindex = whichmod;
					jQuery('#underBuyNowRecommendBottomPageSetList').html(modules[whichmod].html);
					jQuery( "#underBuyNowRecommendBottomPageSetList [name='pid']").each(function(){
					if((jQuery(this).val() == '003724183') || (jQuery(this).val() == '003724190')){
					  jQuery(this).closest('li').remove();
					}
					});
					if (modules[whichmod].meta != null && modules[whichmod].meta != undefined) {
						jQuery('#underBuyNowRecommendBottomHeader').html(modules[whichmod].meta.title);
					}
					ultimateBoughtTotal = jQuery('#underBuyNowRecommendBottomPageSetList > li').length;
					if(ultimateBoughtTotal > 0) {
						jQuery('.underBuyNowSlide').slideDown(300);
						if(ultimateBoughtTotal > 3){
							ultimateBoughtCurPage = 3;
						}
						else{
							ultimateBoughtCurPage = ultimateBoughtTotal;
						}
						jQuery('#underBuyNowRecommendBottomPageSetList').css('width', (ultimateBoughtTotal * 172) + 'px');
						var $container = jQuery('#underBuyNowPaBottomPageSet');
						jQuery('#underBuyNowBottomSlide').hover(function() {
							$container
							.stop()
							.animate({
								height : 180
							}, 300);
						},function() {	
							$container
							.stop()
							.animate({
								height : 108
							}, 300);
						});
						underBuyNowPaBottomButtonUpdate();
						var $underBuyNowPaBottomButtonPrev = jQuery('#underBuyNowPaBottomButtonPrev');
						var $underBuyNowPaBottomButtonNext = jQuery('#underBuyNowPaBottomButtonNext');
						$underBuyNowPaBottomButtonNext.bind('click', underBuyNowPaBottomNextButtonClick);
						$underBuyNowPaBottomButtonPrev.bind('click', underBuyNowPaBottomPrevButtonClick);
						
						var ids = [];
						$items = jQuery('#underBuyNowRecommendBottomPageSetList > li');
						$items.each(function (index, item) {
							var $item = $items.eq(index),
							$hrefs = $item.find('a'),
							id = $item.find('form').attr('id').split('cartaddproduct')[1];
							ids.push(id);
							$hrefs.each(function (index, item) {
								var $href = $hrefs.eq(index),
								href =  $href.attr('href'),
								type = '';
								if ($href.children('.brand').length > 0) {
									type = 'title';
								} else if ($href.is('.moreinfobutton')) {
									type = 'more_info';
								} else if ($href.children('img').length > 0) {
									type = 'image';
								}
								if (href.indexOf('?') > 0) {
									href = href + '&' + modules[whichmod].meta.clicked(id, type);
								} else {
									href = href + '?' + modules[whichmod].meta.clicked(id, type);
								}
								$href.attr('href', href);
							});
							$item.find('.addtocartbutton').on('click keypress', function () {
								modules[whichmod].meta.added(id);
							});
						});
						
						jQuery('#underBuyNowPaBottomButtonPrev').on('click', function () {
							modules[themodindex].meta.paged('left', itemsViewed(ultimateBoughtCurPage - 3, ultimateBoughtCurPage , ids));
						});
						jQuery('#underBuyNowPaBottomButtonNext').on('click', function () {
							modules[themodindex].meta.paged('right', itemsViewed(ultimateBoughtCurPage - 3, ultimateBoughtCurPage , ids));
						});
						modules[themodindex].meta.placed(ids, itemsViewed(0, 3, ids));
						
					}
				}else if (modules[whichmod].zone === 'GMPPBOT') {
					var theothermodindex = whichmod;
					jQuery('#productDetailsTabPaSlotBottomPageSetList').html(modules[whichmod].html);
					jQuery( "#productDetailsTabPaSlotBottomPageSetList [name='pid']").each(function(){
						if((jQuery(this).val() == '003724183') || (jQuery(this).val() == '003724190')){
						  jQuery(this).closest('li').remove();
						}
					});
					tabPaBottomTotal = jQuery('#productDetailsTabPaSlotBottomPageSetList > li').length;
					if(modules[whichmod].meta != null && modules[whichmod].meta != undefined){
						titleOfTab = modules[whichmod].meta.title;
					}
					if(tabPaBottomTotal > 6){
						tabPaBottomCurPage = 6;
					}else{
						tabPaBottomCurPage = tabPaBottomTotal;
					}
					jQuery('#productDetailsPaBottomPageSet').css('width', (tabPaBottomTotal * 163) + 'px');
					productDetailsPaBottomButtonUpdate();
					jQuery('#productDetailsPaBottomButtonNext').bind('click', productDetailsPaBottomNextButtonClick);
					jQuery('#productDetailsPaBottomButtonPrev').bind('click', productDetailsPaBottomPrevButtonClick);
					
					var ids = [];
					$items = jQuery('#productDetailsTabPaSlotBottomPageSetList > li');
					$items.each(function (index, item) {
						var $item = $items.eq(index),
						$hrefs = $item.find('a'),
						id = $item.find('form').attr('id').split('cartaddproduct')[1];
						ids.push(id);
						$hrefs.each(function (index, item) {
							var $href = $hrefs.eq(index),
							href =  $href.attr('href'),
							type = '';
							if ($href.children('.brand').length > 0) {
								type = 'title';
							} else if ($href.is('.moreinfobutton')) {
								type = 'more_info';
							} else if ($href.children('img').length > 0) {
								type = 'image';
							}
							if (href.indexOf('?') > 0) {
								href = href + '&' + modules[whichmod].meta.clicked(id, type);
							} else {
								href = href + '?' + modules[whichmod].meta.clicked(id, type);
							}
							$href.attr('href', href);
						});
						$item.find('.addtocartbutton').on('click keypress', function () {
							modules[whichmod].meta.added(id);
						});
					});
					jQuery('#productDetailsPaBottomButtonPrev').on('click', function () {
						modules[theothermodindex].meta.paged('left', itemsViewed(tabPaBottomCurPage - 6, tabPaBottomCurPage , ids));
					});
					jQuery('#productDetailsPaBottomButtonNext').on('click', function () {
						modules[theothermodindex].meta.paged('right', itemsViewed(tabPaBottomCurPage - 6, tabPaBottomCurPage , ids));
					});
					modules[theothermodindex].meta.placed(ids, itemsViewed(0, 6, ids));

				}

			}			
			
			if(tabPaBottomTotal>0){
			    var bundleTabChilldren = jQuery('.bundletabs').children().length;
				var tabHeader = '';
				if(bundleTabChilldren > 0){
				  tabHeader = '<div class="tab"><a href="#" hidefocus="true" style="border: none; outline: none;"><span id="recstab">' + titleOfTab + '</span></a></div>';
				}
				else{
				  tabHeader = '<div class="tab"><a href="#" class="active" hidefocus="true" style="border: none; outline: none;"><span id="recstab">' + titleOfTab + '</span></a></div>';
				}
				jQuery('div.bundletabs').append(tabHeader);
				jQuery('.bundleholder').append(jQuery('#productDetailsBottomPaSlide'));
				//jQuery("#productDetailsBottomPaSlide").show();
				
				jQuery('.bundleholder > div').hide();
				jQuery('.bundleholder > div:first').show();
				jQuery('.bundletabs .tab:first a').addClass('active');
				
				jQuery('.bundletabs .tab a').click(function(){
				    if(!jQuery(this).hasClass('active')){
					    jQuery('.bundletabs .tab a').removeClass('active');
						jQuery(this).addClass('active');
						var currentTab = jQuery('span',this).attr('id');
						jQuery('.bundleholder > div').hide();
						jQuery('.' + currentTab).show();
					}
					return false;
				});
				jQuery('div.redesignbundletabs').show();
			}
		});
	});
});

if(!jQuery('.bundletabs').children().length > 0){
	jQuery('div.redesignbundletabs').hide();
}
	

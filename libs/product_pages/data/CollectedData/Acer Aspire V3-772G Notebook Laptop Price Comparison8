<script type="text/javascript">
jQuery(function() {
    var input = jQuery('#pemail');
    input.focus(function() {
		 var el = jQuery(this);
		 if(el.val() == el.attr('title'))
			 el.val('');
    }).blur(function() {
         var el = jQuery(this);
         if(el.val() == '')
             el.val(el.attr('title'));
    });
 });

function checkhasemailaddress(){
  if(jQuery('#pemail').val() == jQuery('#pemail').attr('title')){
     jQuery('#pemail').val('');
  }
  return true;
}

jQuery('#userControls ul.options a').each(function() {
  var glnkchange = jQuery(this);
  if(glnkchange.text() == 'ASDA Groceries') glnkchange.attr('href', 'http://groceries.asda.com');
});

if(window.location.href.indexOf("&fix=") > -1){
  jQuery('a.GreenLargeViewAllButton').each(function() {
    var linkaddr = jQuery(this).attr('href');
    jQuery(this).attr('href', linkaddr + "&fix=");
  });
}
</script>





<script type="text/javascript">
function saveForLaterItemView(){
strFormID = 'cartaddproduct000524372';
jQuery.ajax({
url: 'http://direct.asda.com/on/demandware.store/Sites-ASDA-Site/default/SaveForLater-Save?pid=' + '000524372',
type: 'get'
}).done(function(){
var saveForLaterBtn = jQuery('#saveForLaterBtn');
saveForLaterBtn.addClass('itemSaved');
saveForLaterBtn.unbind();
saveForLaterBtn.click(function(){return false;});
jQuery.ajax({
url: 'http://direct.asda.com/on/demandware.store/Sites-ASDA-Site/default/Cart-MiniCart',
type: 'get'
}).done(function(response){
var result = response.split("<!-- Start: george/cart/minicartlineitem_george -->");
result = result[1].split("<!-- End: george/cart/minicartlineitem_george -->");
jQuery('.viewSavedItems').removeClass('hidden');
saveForLaterBtn.addClass("haveSaved");
//Trigger custom event for updating top nav button.
jQuery('#userControls .sfl-counter').trigger('sflTopUpdateEvent');
// replace the content
minicart.updateBasketItems(result[0],strFormID, true);
jQuery(".viewSavedItems").removeClass('hidden');
});
});
}
function toggleViewSaved() {}
jQuery(function() {
jQuery('#saveForLaterBtn').on('click', function(e) {
e.preventDefault();
saveForLaterItemView();
});
});
</script>


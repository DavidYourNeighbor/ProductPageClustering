<script type="text/javascript">
function changeVariant(url) {
var theButton = jQuery(".addtocartbuttonbig");
theButton.addClass("loading");
theButton.attr("onclick", "return false;");
theButton.attr("onkeypress", "return false;");
jQuery('#ajaxContainer').load(url, '', ajaxSuccess);
}
function ajaxSuccess(){
var theButton = jQuery(".addtocartbuttonbig");
theButton.removeClass("loading");
var zoomOptions = {
zoomWidth: 478,
zoomHeight: 478,
xOffset: 10,
yOffset: 0,
position: "right",
title: false,
lens: true,
lensReset : true,
preloadImages : true,
showPreload:true
};
jQuery('.productImageLink').jqzoom(zoomOptions);
// Reload BV reviews when a new variation option is selected.
$BV.ui("rr", "show_reviews", {
productId: productId,
onEvent: function(json) {
if (json.attributes.numReviews > 0){
setActiveTab2();
}
}
});
jQuery(".jNice").jNice();
initialiseDeliveryToggle();
}
</script>

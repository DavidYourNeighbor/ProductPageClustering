<script type="text/javascript">
	var ultimateBoughtTotal = 0,
		ultimateBoughtCurPage = 1;

	var underBuyNowPaBottomNextButtonClick = function(event){
		event.preventDefault();
		ultimateBoughtCurPage += 1;
		jQuery("#underBuyNowRecommendBottomPageSetList").animate({"left": "-=172"}, 300);
		underBuyNowPaBottomButtonUpdate();
	};

	var underBuyNowPaBottomPrevButtonClick = function(event){
		event.preventDefault();
		ultimateBoughtCurPage -= 1;
		jQuery("#underBuyNowRecommendBottomPageSetList").animate({"left": "+=172"}, 300);
		underBuyNowPaBottomButtonUpdate();
	};

	function underBuyNowPaBottomButtonUpdate(){
	
		var $underBuyNowPaBottomButtonPrev = jQuery('#underBuyNowPaBottomButtonPrev'),
			$underBuyNowPaBottomButtonNext = jQuery('#underBuyNowPaBottomButtonNext');
	
		
		if(ultimateBoughtCurPage <= 3){
			$underBuyNowPaBottomButtonPrev.removeClass('underBuyNowPaArrowLeftBlack');
			$underBuyNowPaBottomButtonPrev.addClass('underBuyNowPaArrowLeftGrey');
		}
		else{
			$underBuyNowPaBottomButtonPrev.removeClass('underBuyNowPaArrowLeftGrey');
			$underBuyNowPaBottomButtonPrev.addClass('underBuyNowPaArrowLeftBlack');
		}

		if((ultimateBoughtCurPage < 3) || (ultimateBoughtCurPage == ultimateBoughtTotal)){
			$underBuyNowPaBottomButtonNext.removeClass('underBuyNowPaArrowRightBlack');
			$underBuyNowPaBottomButtonNext.addClass('underBuyNowPaArrowRightGrey');
		}
		else{
			$underBuyNowPaBottomButtonNext.removeClass('underBuyNowPaArrowRightGrey');
			$underBuyNowPaBottomButtonNext.addClass('underBuyNowPaArrowRightBlack');
		}
	}
</script>

<div id="underBuyNowBottomSlide" class="underBuyNowSlide">
	<div id="paBottombuttonwrapper" class="pabuttonwrapper">
		<div class="underBuyNowRecommend" id="underBuyNowRecommendBottom">
			<h3 id="underBuyNowRecommendBottomHeader"></h3>
			<div id="underBuyNowPaBottomPageSet" class="underBuyNowPageSet">
				<ul id="underBuyNowRecommendBottomPageSetList"></ul>
			</div>
		</div>
		<div id="underBuyNowPaBottomButtonPrev" class="PaginationLeftArrow underBuyNowPaArrowButton underBuyNowPaArrowLeftBlack"></div>
		<div id="underBuyNowPaBottomButtonNext" class="PaginationRightArrow underBuyNowPaArrowButton underBuyNowPaArrowRightBlack"></div>
	</div>
</div>


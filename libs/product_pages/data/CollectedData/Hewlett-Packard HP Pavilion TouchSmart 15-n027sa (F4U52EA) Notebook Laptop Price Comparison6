$sd(function () {
$sd('#keywordSearchForm').on('submit', function(e) {
var $searchbox = $sd(this).find('#searchBox'),
$tip = "Please enter a search description",
$tooltip = '<div class="toolTipWrapper"><div class="toolTipTop"></div><div class="toolTipMid">'+$tip+'</div></div>';

var $siteIds = {};
// $siteIds['site name'] = ["live site id", "staging/production site id", "extension"]
$siteIds['very'] = ["90092320", "60092320", ".co.uk"];
$siteIds['littlewoods'] = ["90092183", "60092183", ".com"];
$siteIds['kandco'] = ["90052727", "60052727", ".com"];

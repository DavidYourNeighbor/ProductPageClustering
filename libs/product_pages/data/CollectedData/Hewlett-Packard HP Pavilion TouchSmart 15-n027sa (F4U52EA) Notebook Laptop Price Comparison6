$siteIds['woolworths'] = ["90103280", "60103280", ".co.uk"];
// get url
var $root = location.host;
// if site is live
if ($root.match('www')) {
// strip url to leave only the brand name
$root = $root.split('.')[1];
var $currentSiteId = eval('$siteIds.' + $root + '[0]'),
coremetricsURL = "data.coremetrics.com";
} 
// if site is on staging or production
else {
// remove any domain/extension (.lsdg.com for example)
$root = $root.split('.')[0];

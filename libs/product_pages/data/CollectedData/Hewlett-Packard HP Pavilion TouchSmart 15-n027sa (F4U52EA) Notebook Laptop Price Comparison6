<script type="text/javascript">
var queryParams = window.location.hash;
if(!queryParams.length) {
window.SDCSAPageID = "Desktop | Desktop | PRODUCT | ENTRY";
} else { 
var isDynamicContentLink = queryParams.indexOf('Dynamic Content') > -1;
var isCrossSellLinkFromBasket = queryParams.indexOf('crossSellType') > -1;
var isCrossSellLinkFromProduct = queryParams.indexOf('parentProductId') > -1;
if (isDynamicContentLink || isCrossSellLinkFromBasket || isCrossSellLinkFromProduct) { 
var paramsMinusHash=queryParams.replace("#"," ");
var params=paramsMinusHash.replace("&","-"); 
if (isDynamicContentLink) {
window.SDCSAPageID = "Desktop | Desktop | PRODUCT | ENTRY" + params;
} else {
var paramsSplit;
var zone;
var parentProduct;
var zoneParam;
var parentProductParam;
var params;
if (isCrossSellLinkFromBasket && !isCrossSellLinkFromProduct) {
paramsSplit = queryParams.split("="); 
zone = paramsSplit[1];
params = " " + zone;
} 
if (isCrossSellLinkFromBasket && isCrossSellLinkFromProduct) {
paramsSplit = queryParams.split("&");
var zoneSection = paramsSplit[0];
var parentProductSection = paramsSplit[1];
zoneParam = zoneSection.split("="); 
parentProductParam = parentProductSection.split("=");
zone = zoneParam[1];
parentProduct = parentProductParam[1];
params = " " + zone + " - " + parentProduct;
}
window.SDCSAPageID = "Desktop | Desktop | PRODUCT | ENTRY" + params;
} 
} else {
window.SDCSAPageID = "Desktop | Desktop | PRODUCT | ENTRY";
}
}
</script>
<script type="text/javascript">
/* adding "order before x:xx" delivery condition to selected products */
if (document.URL.match(/\.end/g)) { // gallery pages only
$sd('#products li.product').each( function () {
var nextDayBrand = $sd('.productTitle h3 em', this).html();
var spanDesc = $sd('.productTitle h3 span', this).text();
// order before 2pm
if (nextDayBrand === "Apple"
&& (spanDesc.match(/iPad/gi) || spanDesc.match(/iPod/gi))
&& (spanDesc.match(/Retina/gi) || spanDesc.match(/GB/gi))
) {
if ($sd('.productDelivery dd:last', this).text().match(/Next day/gi)) {
$sd('.productDelivery dd:last', this).text('Next day delivery available if ordered before 2pm');
} 
}
});
}
</script>

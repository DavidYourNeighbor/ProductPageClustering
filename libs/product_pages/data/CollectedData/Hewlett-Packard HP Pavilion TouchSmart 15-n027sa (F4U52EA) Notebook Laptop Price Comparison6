<script type="text/javascript">
pluckAppProxy.registerActivityCallback("ReviewListRendered", function () {
// show and hide
var linksToShow = ["facebook", "linkedin", "twitter"];
$sd('.pluck-share-item').each(function () {
var $item = $sd(this);
var $linkClass = $item.children('a').attr('class');
$item.hide();
if($linkClass.match(/pluck-share-link-facebook/g) || 
$linkClass.match(/pluck-share-link-twitter/g) || 
$linkClass.match(/pluck-share-link-linkedin/g)) {
$item.show();
}
});
// add new
$sd(".pluck-share-col2").append('<span class="pluck-share-item"><a class="pluck-share-link pluck-share-link-googleplus pluck-share-link-close" href="#" target="_blank"><img src="//ssl.gstatic.com/images/icons/gplus-16.png" height="16" width="16"> Google+</a></span>');
if (!("googleplus" in pluckAppProxy.pluck_util_share_url_map)) {
pluckAppProxy.pluck_util_share_url_map['googleplus'] = { href: "https://plus.google.com/share?url=?URL?&title=?TITLE?" };
}
});

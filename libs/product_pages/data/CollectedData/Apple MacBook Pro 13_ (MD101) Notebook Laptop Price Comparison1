  function get_daily_deal_data(){
   $.ajax(
   {

    datatype:"json",
    url:"/daily_deals/daily_deals.php",
 
  
    success:function(data){
     $.each(data, function(i, item){
                 

 

   
      	  var daily_deal_table = '<ul class="thumbnails">';
      daily_deal_table  += '<li class="span4">';
      daily_deal_table  += '<div class="thumbnail" align="center" style="width:250px;height:250px;">';
      daily_deal_table  += '<table width="100%" style="height:250px;">';
      daily_deal_table  += '<tr>';
      daily_deal_table  += '<td style="background-color:#e9e9e9; width:30px;">';
      daily_deal_table  += '<img  style="width:30px; height: 80px;" src="/style/images/DailyDeals.png"/>';
      daily_deal_table  += '</td>';
      daily_deal_table  += '<td align="center">';
      daily_deal_table  += '<div>';
      daily_deal_table  += '<div style="height:20px; text-align:right;"> <b><h5 style="font-size:2.5mm; line-height: 1.1;">'+ item.title +'</h5><b>';
      daily_deal_table  += '</div>';
      daily_deal_table  += '<img style="width:100px; height:100px;" src="//slrhut.co.uk/content/products-'+ item.product_type +'/'+item.id+'_1_s.jpg"  alt=Best Price Daily Deals At SLRHUT Featuring"'+item.title+'" ><br>';
      daily_deal_table  += '<span  style="width:50px; text-align:right;" onclick="javascript:window.location.href = "/product/ID'+item.id+'C'+item.category_id+'/>' ;
      daily_deal_table  += '<b><font style="color:red; font-size:5mm;font-style:bold;">&pound;'+ item.price +'</font></b>';
      /*daily_deal_table  += '<br><b><font style="color:red; font-size: 3mm;">inc. VAT</font></b>';*/
      daily_deal_table  +=' <h4 style="color:red;">Sale Ends in:</h4>';
      daily_deal_table  +=' <div style="height:20px; width:100px; background-color:red; font-size: 3mm; color:white; border: 1px solid light-grey;" id="countdown' +item.id + item.category_id + '" ></div>';
      daily_deal_table  +=' </div>';
      daily_deal_table  +=  '</td></tr></table> </div>';
      daily_deal_table  +=' </li>';
      daily_deal_table  +=' </ul>';

              
              
      $("#deals_slideout").append(daily_deal_table);
      $("#deals_slideout").click(function(){window.location.href = "//slrhut.co.uk/product/ID" + item.id + "C" + item.category_id + "/"; }); 
      $("#deals_slideout").mouseover(function(){this.style.cursor = 'pointer'});
    
      
      var untildate = new Date(item.daily_deal_until * 1000);
      $('#countdown' + item.id + item.category_id).countdown({until:untildate });
      $('#countdown').countdown({until:untildate });            
                  
     });

    }
   }
  );
  } 
                  
                  

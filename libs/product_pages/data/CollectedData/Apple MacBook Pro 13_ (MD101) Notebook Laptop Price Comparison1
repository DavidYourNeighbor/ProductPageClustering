            <script language="javascript">
                         
                function do_timezone_popup(){
                           
                    var source = "//slrhut.co.uk/banners/";
                    var width = "";
                    var align = "center";
                    var top = 100;
                    var padding = 0;
                    var backgroundColor = "#000000";
                    var borderColor = "#000000";
                    var borderWeight = 5;
                    var borderRadius = 5;
                    var fadeOutTime = 300;
                    var disableColor = "#666666";
                    var disableOpacity = 0;
                    var loadingImage = "/includes/js/loading.gif";
                         
                    //This method initialises the modal popup
                          
                         
                    modalPopup( align,
                    top,
                    width,
                    padding,
                    disableColor,
                    disableOpacity,
                    backgroundColor,
                    borderColor,
                    borderWeight,
                    borderRadius,
                    fadeOutTime,
                    source,
                    loadingImage );
                    var $sidebar   = $('#outerModalPopupDiv'), 
                    $window    = $(window),
                    offset     = $sidebar.offset(),
                    topPadding = 15;

                    $window.scroll(function() {
                        if ($window.scrollTop() > offset.top) {
                            $sidebar.stop().animate({
                                marginTop: $window.scrollTop() - offset.top + topPadding
                            });
                        } else {
                            $sidebar.stop().animate({
                                marginTop: 0
                            });
                        }
                    });

                }
                         
            </script>



    <script type="text/javascript">

        // WHE.SEARCH.MASTER

        $(document).ready(function() {
            //Allow 50ms for the page to settle before pushing to the data layer
            setTimeout(function() {
                var httpStatus = '200';
                var visitorId = '0';
                var loginState = 'False'
                var existingCustomer = 'False';
                var openOrder = $('#div_basket .baketblk-dsbl span').text() != '0 items' ? 'True' : 'False';

                dataLayer.push(
                {
                    'httpStatus': httpStatus,
                    'visitorLoginState': loginState,
                    'visitorId': visitorId,
                    'visitorExistingCustomer': existingCustomer,
                    'openOrder': openOrder
                });
            }, 50);

            // Banner Click Fredhopper
            $('.banners a').click(function() {
                dataLayer.push({
                    'bannerClick': $(this).attr('manual_cm_re'),
                    'event': 'bannerClickFredhopper'
                });
            });

            // endswith extension method
            String.prototype.endsWith = function(pattern) {
                var d = this.length - pattern.length;
                return d >= 0 && this.lastIndexOf(pattern) === d;
            };

            // Fredhopper Banner Impression
            var fredhopperImpression = '';
            var fhIdx = 1;
            $('.banners a').each(function() {
                fredhopperImpression += fhIdx.toString() + '-' + $(this).attr('manual_cm_re').replace('?', '') + '-_-';
                fhIdx++;
            });

            if (fredhopperImpression.length > 0) {
                fredhopperImpressionEvent();

                function fredhopperImpressionEvent() {
                    if (fredhopperImpression.endsWith('-_-')) {
                        fredhopperImpression = fredhopperImpression.substring(0, fredhopperImpression.length - 3);
                    }

                    dataLayer.push({
                        'bannerImpressionFredhopper': fredhopperImpression,
                        'event': 'bannerImpressionFredhopper'
                    });
                } 
            }

            // Left Nav Review
            $('#navreviews a').click(function() {
                dataLayer.push({ 'leftNavReview': $(this).text(), 'event': 'leftNavReview' });
            })

            // Stock Label Product Detail Page
            $('#stock').live('click', function() {
                dataLayer.push({ 'stockLabel': $(this).find('span:first').find('span:first').find('span:first').text().trim(), 'event': 'stockLabel' });
            });

            // Left Nav Purple
            $('.curves.grey a').live('click', function() {
                dataLayer.push({ 'leftNavPurple': $(this).text(), 'event': 'leftNavPurple' });
            });

            // Add to Compare
            $(".AddCompare").live('click', function() {
                if ($(this).is(':checked')) {
                    dataLayer.push({ 'addToCompareTitle': $(this).parent().parent().parent().find('h2').text(), 'event': 'addToCompare' });
                }
            });

            // Search Facet
            $('.filterList a').live('click', function() {
                dataLayer.push({
                    'searchFacetCategory': $(this).parent().parent().parent('.filterList').children().html().replace('Refine By ', ''),
                    'searchFacetLabel': $(this).text(),
                    'event': 'searchFacet'
                });
            });

            // Stock Label List Pages
            $('.stock').live('click', function() {
                dataLayer.push({ 'stockLabel': $(this).find('a').text().trim(), 'event': 'stockLabel' });
            });

            // Right Banner Click
            $('#navright ul li a').live('click', function() {
                dataLayer.push({ 'bannerClick': $('img:first', this).attr('alt') });
            });

            // Livechat
            if ($('#livechat').length > 0) {
                dataLayer.push({ 'livechatDisplayed': 'True', 'event': 'livechatDisplayed' });
            }

            // Livechat
            $('#livechat').live('click', function() {
                dataLayer.push({ 'livechatClicked': 'True', 'event': 'livechatClicked' });
            });

            // Add Buying Option
            $(".bundle a[id*='AddToBasket']").live('click', function() {
                dataLayer.push({ 'addBuyingOption': true });
            });

            // Add To Basket
            //$('#acrAddToBasket').live('click', function() {
            $("a[id*='AddToBasket']").live('click', function() {
                var addedProduct = '';
                var addedFromArray = ['Missing', 'ViewProduct', 'ListProducts', 'BaseCategory', 'BargainBasement', 'BargainBasementTab', 'PerfectPartner',
                                                      'SearchResults', 'LeftAccessories', 'Basket', 'BestSellers', 'ProductAccessories', 'ProductKitsAndBundles', 'WBWPromotions'];

                var skuStart = $(this).attr('href').indexOf('sku=') + 4;
                //alert('Sku Start : ' + skuStart);
                var skuEnd = $(this).attr('href').indexOf('&', skuStart);
                //alert('Sku End : ' + skuEnd);
                var addedStart = $(this).attr('href').indexOf('addedfrom=') + 10;
                //alert('Added Start : ' + addedStart);
                var addedEnd = $(this).attr('href').toString().indexOf('&', addedStart);

                if (addedEnd == -1 || addedEnd == undefined) {
                    //addedEnd = $(this).attr('href').indexOf('addedfrom=') + 10;
                    addedEnd = $(this).attr('href').length;
                }
                //alert('Added End : ' + addedEnd);

                var sku = $(this).attr('href').substring(skuStart, skuEnd);
                //alert('SKU : ' + sku);
                var addedFrom = $(this).attr('href').substring(addedStart, addedEnd);
                //alert('AddedFrom : ' + addedFrom);

                switch (parseInt(addedFrom)) {
                    case 0:         // Missing
                        addedProduct = '';
                        break;
                    case 1:         // ViewProduct
                        addedProduct = $(this).parent().parent().parent().parent().find('h1:first').text();
                        break;
                    case 2:         // ListProducts
                        addedProduct = '';
                        break;
                    case 3:         // BaseCategory
                        addedProduct = '';
                        break;
                    case 4:         // BargainBasement
                        addedProduct = '';
                        break;
                    case 5:         // BargainBasementTab
                        addedProduct = '';
                        break;
                    case 6:         // PerfectPartner
                        addedProduct = '';
                        break;
                    case 7:         // SearchResults
                        addedProduct = $(this).parent().parent().parent().find('h2:first').text();
                        //addedProduct = $(this).parent().parent().parent().find('h2:first').text();
                        break;
                    case 8:         // LeftAccessories
                        addedProduct = '';
                        break;
                    case 9:         // Basket
                        addedProduct = '';
                        break;
                    case 10:        // BestSellers
                        addedProduct = '';
                        break;
                    case 11:        // ProductAccessories
                        addedProduct = '';
                        break;
                    case 12:        // ProductKitsAndBundles
                        addedProduct = '';
                        break;
                    case 13:        // WBWPromotions
                        addedProduct = '';
                        break;
                }

                var addedToBasketCategory = $('#navbc span a:nth(2)').text();

                if (addedToBasketCategory == '')
                    addedToBasketCategory = $('#navbc span a:nth(1)').text();

                if (addedProduct.indexOf("Used ") >= 0) {
                    dataLayer.push(
                    {
                        'usedItemAddedToBasket': addedProduct.trim(),
                        'addedToBasketProduct': addedProduct.trim(),
                        'addedToBasketSKU': sku,
                        'addedToBasketFrom': addedFromArray[parseInt(addedFrom)],
                        'addedToBasketCategory': addedToBasketCategory,
                        'event': 'addedToBasket'
                    });
                }
                else {
                    dataLayer.push(
                    {
                        'addedToBasketProduct': addedProduct.trim(),
                        'addedToBasketSKU': sku,
                        'addedToBasketFrom': addedFromArray[parseInt(addedFrom)],
                        'addedToBasketCategory': addedToBasketCategory,
                        'event': 'addedToBasket'
                    });
                }
            });

            // Navigation Click Dropdown
            $('.nav_1 dd a').live('click', function() {
                dataLayer.push({
                    'dropdownNavCategory': $(this).parent().parent().parent().parent().children('a').text(),
                    'dropdownNavLabel': $(this).parent().text(),
                    'event': 'dropdownNavClick'
                }); 
            });

            $('.nav_2 dd a').live('click', function() {
                dataLayer.push({
                    'dropdownNavCategory': $(this).parent().parent().parent().parent().children('a').text(),
                    'dropdownNavLabel': $(this).parent().text(),
                    'event': 'dropdownNavClick'
                }); 
            });

            $('.nav_3 dd a').live('click', function() {
                dataLayer.push({
                    'dropdownNavCategory': $(this).parent().parent().parent().parent().children('a').text(),
                    'dropdownNavLabel': $(this).parent().text(),
                    'event': 'dropdownNavClick'
                }); 
            });

            $('.nav_4 dd a').live('click', function() {
                dataLayer.push({
                    'dropdownNavCategory': $(this).parent().parent().parent().parent().children('a').text(),
                    'dropdownNavLabel': $(this).parent().text(),
                    'event': 'dropdownNavClick'
                }); 
            });
        });
    </script>
    
    </body>
</html>

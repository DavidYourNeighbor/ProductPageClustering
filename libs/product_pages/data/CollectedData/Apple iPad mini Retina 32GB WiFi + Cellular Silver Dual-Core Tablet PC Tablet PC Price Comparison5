    BargainText = document.getElementById("ctl00_pnlMainContent_lblBargain").innerHTML;
    if (BargainText.length > 2)
        BargainTabTitile = '<h2 class="TabTitle"> ' + _MainProductTitle + ' Bargain  </h2>';
    document.getElementById("ctl00_pnlMainContent_lblBargain").innerHTML = BargainTabTitile + BargainText;

});

//]]>
    </script>
	
	<script language="javascript">
	/*	Display Commas in Prices - x needs to be fixed to 2 decimal 
	*	places before passing to this function, and gets converted to
	*	a string so can't be used in further calculations*/
	function DCP(x) {return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");}
	 
	/* 	Finance calculates IFC finance values and outputs using the 
	*	ifc-credit div first 2 numbers and MainProductPrice hidden form field
	*	required function DCP(x) */
	function IFCfinance(){
	var fprice = parseFloat(document.getElementById('ctl00_pnlMainContent_hdnLPMainProductPrice').value).toFixed(2);
	//fprice = 4656;
	
	if(null != document.getElementById('ctl00_pnlMainContent_lbFinancePromotion'))
	{
		var fterm = parseFloat(document.getElementById('ctl00_pnlMainContent_lbFinancePromotion').innerHTML.substr(3,3)).toFixed(0);
		var fpayments1 = (fprice/fterm).toFixed(2);
		if (!(fpayments1*fterm == fprice)){
			var fpaymentfinal = parseFloat((fprice-(fpayments1*(fterm-1)))).toFixed(2);
			document.write("<p class='finance-example'><strong>Finance Example:</strong><br /> Cash Price: &pound;"+DCP(fprice)+"<br />Deposit: &pound;0.00<br />Loan Amount: &pound;"+DCP(fprice)+"<br />Monthly Instalments (x"+(fterm-1)+"): &pound;"+DCP(fpayments1)+"<br />Final Monthly Instalment: &pound;"+DCP(fpaymentfinal)+"<br /><strong>Total Amount Payable: &pound;"+DCP(fprice)+"<br />0% APR (representative)</strong></p>")
		} else {
			document.write("<p class='finance-example'><strong>Finance Example:</strong><br /> Cash Price: &pound;"+DCP(fprice)+"<br />Deposit: &pound;0.00<br />Loan Amount: &pound;"+DCP(fprice)+"<br />Monthly Instalments (x"+(fterm)+"): &pound;"+DCP(fpayments1)+"<br /><strong>Total Amount Payable: &pound;"+DCP(fprice)+"<br />0% APR (representative)</strong></p>")
		};
	
		$( "#ctl00_pnlMainContent_lbFinancePromotion" ).append( "<p class='ifc-text'>from &pound;"+DCP(fpayments1)+" per month<br /> (0% APR representative)</p>" );
		$( "#ifc-hover p:first" ).prepend( fterm+" months' " );
	}
	}
	</script>



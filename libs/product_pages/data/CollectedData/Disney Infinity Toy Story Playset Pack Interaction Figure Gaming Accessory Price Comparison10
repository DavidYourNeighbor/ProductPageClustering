<script type="text/javascript">
function toggle_mobile_menu() {
	if($js(".nav-wrapper").hasClass("shown")) {
		$js(".top-link-mobile-menu").removeClass("active");
		$js(".nav-wrapper").removeClass("shown");
	} else {
		$js(".top-link-mobile-menu").addClass("active");
		$js(".nav-wrapper").addClass("shown");
	}
	
	if($js(".search-wrapper").hasClass("shown")) {
		$js(".search-wrapper").removeClass("shown");
	} else {
		$js(".search-wrapper").addClass("shown");
	}
}
</script>	

    function allowSaveCookie() {
        Mage.Cookies.set('user_allowed_save_cookie', 'a:1:{i:1;i:1;}', new Date(new Date().getTime() + 31536000 * 1000));
        if (Mage.Cookies.get('user_allowed_save_cookie')) {

<script type="text/javascript">
  function moreImages(catalogNo) {
     window.open("/viewImages.aspx?id="+catalogNo+"&type=1&active=0&show=1","MoreImages","width=760,height=720,toolbar=0,status=0,resizeable=0,menubar=0")
  }
  var mainImageUrl = "http://static.base.com/images/main/IQAY000002.jpg";
  function setMainImage( url, setDefault  ) {
    $("#mainImage")[0].src = url;
    if ( setDefault )
    {
       mainImageUrl = url;
    }
  }
  function revertMainImage() {
    $("#mainImage")[0].src = mainImageUrl;
  }

  $(document).ready(
   function() {
      $( "#mainTabs" ).tabs();
      
                
        $('#mainImage').elevateZoom({
          zoomWindowWidth:640,
          zoomWindowHeight:424,
          zoomWindowOffetx: 20,
          borderSize: 1,           borderColor: "#B7B7B7"         });
    


      $('.open-tab').click(function (event) {
        event.preventDefault();
        var tab = $(this).attr('href');
        scrollToTab( tab );
      });


      
    });
    
    
    function scrollToTab( tab ) {
        // var tab = $(this).attr('href');
        var idx = $('#mainTabs a[href="'+tab+'"]').parent().index();
        $('#mainTabs').tabs("option", "active", idx );
        var element = $(tab).parent();
        var ot = element.offset().top;
        var h  = element.height() + 32;         var wh = $(window).height(); 
        if ( h < wh ) {
          var cp =  $('html,body').scrollTop();
          if ( ot < cp || ot + h > cp + wh ) { 
             ot = ot + h - wh;           
             $('html,body').animate({scrollTop: ot}, 500); 
          }
        } else {           
           $('html,body').animate({scrollTop: ot}, 500); 
        }    
    }    
</script>


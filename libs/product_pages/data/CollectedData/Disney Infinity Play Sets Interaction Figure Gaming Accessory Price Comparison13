<script type="text/javascript" defer="defer">function CenterOverlay(id,w,h)
{var winHeight=window.innerHeight;if(winHeight<h+32)
{$("#"+id).css("position","absolute");$(window).scrollTop(0);}}
function RemoveOverlay()
{$("#window-overlay").remove();$("#window-overlay-2").remove();}
function CreateOverlay(url,w,h)
{$("body").append("<div id='window-overlay'></div>");$("body").append("<div id='window-overlay-2'><div id='window-overlay-2-inner'><iframe style='width: 100%; height: 100%; border: none;' src='"+escape(url)+"' /></div></div>");CenterOverlay('window-overlay-2',w,h);$("#window-overlay-2").click(function(){RemoveOverlay();});}
function ShowCookiePreferences()
{$('#cookie-popup-outer').remove();CreateOverlay('/form/CookieSettings.aspx',600,400);}
function CookieDismiss()
{var o=$("#cookie-popup-outer");if(o!=null&&o.length==1)
{$("#cookie-popup-outer").remove();$.ajax({url:"/form/CookieSettings.aspx",cache:false,async:false,data:{level:"Social"}});}}
var cookieDismissTimeoutID=null;</script>



<script>
   function SetMinimumSize() {
     var winHeight = $(window).height();
     var headerHeight = $("#header_frame").height();
     var footerHeight = $("#footer_frame").height();

     var height = winHeight - headerHeight - footerHeight - 56;      if ( height < 100 ) height = 100;
      
     $("#page_frame > div").css( "min-height", height );
   }
   
   $(document).ready(function(){   
      $(window).resize(function(){SetMinimumSize();});
      SetMinimumSize();
   });

</script>                                       
        
        


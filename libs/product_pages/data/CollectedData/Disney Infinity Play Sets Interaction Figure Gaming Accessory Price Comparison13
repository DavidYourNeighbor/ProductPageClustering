<div id="nav_panel"><script language='JavaScript' type='text/javascript'>
                $(document).ready(function() {
      
      function addHoverClass(){        
        $(this).addClass("showsubmenu"); 
        }

      function removeHoverClass(){ 
        $(this).removeClass("showsubmenu");
        }

    var hoverIntentConfig = {
         interval: 100,
         sensitivity: 5,
         over: addHoverClass,
         timeout: 300,
         out: removeHoverClass
    };

    $("div.menu").hoverIntent(hoverIntentConfig)

      

function getCmsValues(){
	var cmsValues = new Array();
	cmsValues[0] = '20';
	cmsValues[1] = 'DELL1520B,SAMTV,TOSHIBA_L300';
	cmsValues[2] = '20';
	cmsValues[3] = '5';
	cmsValues[4] = '5';
	cmsValues[5] = '0';
	cmsValues[6] = '5';
	cmsValues[7] = '0';
	cmsValues[8] = '0';
	cmsValues[9] = '0';
	cmsValues[10] = '360,BS,TALK_PLAN,PREPY,INS#NON_HANDSET,INS#PAYM#BAND4';
	cmsValues[11] = 'AOLBB,VIRBB,HOMEBB,ORNBB,SIMM';
	cmsValues[12] = '5';
	return cmsValues;
}

function getCookie(cookieName)
{
	var cookie = document.cookie;
	if (cookie.length > 0)
	  {
		  var cookieStart = cookie.indexOf(cookieName + "=");
		  if (cookieStart != -1)
		    {
			    cookieStart = cookieStart + cookieName.length+1;
			    cookieEnd = cookie.indexOf(";",cookieStart);
			    if (cookieEnd == -1)
				{
					cookieEnd = cookie.length;
				}
			    return unescape(cookie.substring(cookieStart,cookieEnd));
			}
	  }

	return "";
}

function setCookie(cookieName, value, expiredays)
{
	cookieValue = value;
	var cookieVal = value.split(':');
	
	if(document.getElementById(cookieVal[1]).checked == true){
		cookieModified = 'checked';
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + expiredays);
		document.cookie = cookieName + "=" + escape(value) + ";path="+ '/' + ((expiredays == null) ? "" : ";expires="+exdate.toGMTString());
	}
	else{
		cookieModified = 'unchecked';
		var cookie_date = new Date ( );  // current date & time
		cookie_date.setTime ( cookie_date.getTime() - 1 );
		document.cookie = cookieName + "=; path="+ '/' + ";expires=" + cookie_date.toGMTString();
	}
}


//]]>
</script>




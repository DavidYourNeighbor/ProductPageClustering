	function displayStoreStock(salesProductPartId, sppCpwCode){
		if(document.getElementById("resultDiv")!=null){
			if(document.getElementById("resultDiv").style.display != 'none' || document.getElementById("notStockStatus").style.display != 'none' ||document.getElementById("noStockStatus").style.display != 'none')
			{
				getStoreStockForColours(salesProductPartId);		//Function defined in dealSummaryStoreStock.jsp
			}
			document.getElementById("selectedSppCpwCode").value = salesProductPartId;
			document.getElementById("sppCpwCode").value = sppCpwCode;
			updateStylesDO();
			if(rnc=='T'){
				updateOOSStylesRnC();
				if(payg==''){
				updateStylesRnC();
				}
	    		}
		}	
	}

	function changeSwatchDisplay(element)
	{
		jQuery('#colourScrollList li').each(function (i) {
            var colourSwatchSrc=jQuery(this).children('div.dsColourPicker').children('a').children('img.dsColourSwatchImg').attr('src');
            colourSwatchSrc=colourSwatchSrc.replace("_ON","_OFF");
            jQuery(this).children('div.dsColourPicker').children('a').children('img.dsColourSwatchImg').attr('src', colourSwatchSrc);//.attr('src',colourSwatchSrc);
          
		});
		var colourSwatchSrc=jQuery(element).children('img.dsColourSwatchImg').attr('src');
		colourSwatchSrc=colourSwatchSrc.replace("_OFF","_ON");
		jQuery(element).children('img.dsColourSwatchImg').attr('src', colourSwatchSrc);
		

	}
	
	function setSelectedClass(selectedIndex)
	{
		var items = document.getElementById("colourScrollList").getElementsByTagName("li");
		for( index = 1; index <= items.length; index++ ) {
			 if (document.getElementById("list_" + index))
			 {
				document.getElementById("list_" + index).className = "";
			 }
		}
		document.getElementById("list_" + selectedIndex).className = "selected";
	}


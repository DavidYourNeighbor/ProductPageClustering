<script type="text/javascript">
jQuery(document).ready(function() {
	var defaultStore = getCookie("defaultstore.cpw").split(':');
	var defaultStoreLen = defaultStore.length;
	if(defaultStore != '' && defaultStore != null && document.getElementById('postCode') != null){
		document.getElementById('postCode').value = defaultStore[0];
		if(defaultStoreLen > 3) {
		branchIdFromPopup = defaultStore[1];
		showLoadImg();
		if(branchIdFromPopup.length > 0){           
	    	   branchId = branchIdFromPopup;
	    	   branchIdFromPopup = '';
	    }
		storeName = defaultStore[3];
		spCpwCode = document.getElementById('spCpwCode').value;
	    storeStockDWRService.findStore(branchId, spCpwCode, getCmsValues(), answer);
		}
		else {
			doPostCodeSearch();
		}
	}
});
</script>
                                 

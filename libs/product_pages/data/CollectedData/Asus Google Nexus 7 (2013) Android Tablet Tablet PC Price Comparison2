<link type="text/css" rel="STYLESHEET" href="http://reviews.carphonewarehouse.com/static/2486-en_gb/bazaarvoice.css" ></link>
<script language="JavaScript" src="http://reviews.carphonewarehouse.com/static/2486-en_gb/bazaarvoice.js" type="text/javascript"></script>

<script type="text/javascript" language="javascript">
//<!--
    //Timeout for review load.  Consider reviews unavailable if not loaded within 15 seconds
    setTimeout("BVcheckLoadState()", 15000);
//-->

// Do not change signature of this method as this is invoked by bazaar voice
function ratingsDisplayed(totalReviewsCount, avgRating, ratingsOnlyReviewCount, buyAgainPercentage, productID) 
{
	//Bug#72338
	if (totalReviewsCount > 0) 
	{	var newPDPReviews = "";
		reviewsExists();
		//This part will fetch the id from phoneDetailsComponent.jsp
		var reviewLinkElem = document.getElementById('reviewLink');
		var innerHTML = reviewLinkElem.innerHTML; 
		if(document.getElementById('reviewsNewPDP') != null)
		{
		newPDPReviews = document.getElementById('reviewsNewPDP').value;
		}
		//Bug#81095 - new condition added to avoid duplication of the reviews.
		if(reviewLinkElem != null && innerHTML.charAt(0)!='(' && newPDPReviews != 'true' )
		{
			//append the review count
			reviewLinkElem.innerHTML = '(' + totalReviewsCount + reviewLinkElem.innerHTML; 
		}
		else if(reviewLinkElem != null && innerHTML.charAt(0)!='(' && newPDPReviews == 'true')
		{
			//append the review count
			reviewLinkElem.innerHTML = totalReviewsCount + ' reviews'; 
		}
		
	}
	else
	{
		reviewsExists();
		var reviewLinkElem = document.getElementById('reviewLink');
		if(reviewLinkElem != null )
		{
			reviewLinkElem.innerHTML ="";
			
		}
		
		
	}
}

var BVisLoaded = false;
function BVcheckLoadState() {
    if(!BVisLoaded) {
        var page = document.getElementById('BVFrame').src;
        document.getElementById('BVFrame').src='http://reviews.carphonewarehouse.com/logging?page=' + escape(page);
        document.getElementById('BVReviewsContainer').innerHTML = '<!-- Review retrieval timed out -->';
    }
}
</script>

<script type="text/javascript">
function rdShowHide(elementId, button) {
	if (document.getElementById(elementId)) {
	if (document.getElementById(elementId).style.display == 'none') {
	document.getElementById(elementId).style.display = 'block';
	if (button.src) { button.src = 'http://media.phonehouse.com/cpw-sales/static/images/phone_shop/deal_summary/rd_hide.gif'; }
	} else {
	document.getElementById(elementId).style.display = 'none'
	if (button.src) { button.src = 'http://media.phonehouse.com/cpw-sales/static/images/phone_shop/deal_summary/rd_open.gif'; }
	}
	}
	} 
</script>

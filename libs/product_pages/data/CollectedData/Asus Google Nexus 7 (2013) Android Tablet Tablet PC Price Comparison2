function setCookieForStore(cookieName, value, expiredays){
	var exdate=new Date();
    exdate.setDate(exdate.getDate() + expiredays);
    document.cookie = cookieName + "=" + escape(value) + ";path="+ '/' + ((expiredays == null) ? "" : ";expires="+exdate.toGMTString());
} 

var answer = function(data)
{	
	stockList=data;
	var storeElement = document.getElementById("storeName");
	var defStore=postCode+":"+branchId;
	while (storeElement.firstChild) {
	storeElement.removeChild(storeElement.firstChild);
	}
	storeElement.appendChild(document.createTextNode(storeName));
	storeElement.alt=storeName;
	storeElement.title=storeName;
	
	
	
	

	//Fix for issue # 76822
	
	

var statusExist = false;
for(i=0;i<data.length;i++)
{	
	var bean = data[i];
	var sppId  =  bean.salesProductPartId;
	var sppIdDiv = document.getElementById(sppId);
	var toolTipHdr = document.getElementById("tipStoreStockHdr");
	var toolTipContent = document.getElementById("tipStoreStockContent");
	var omniture_store_stock; 
	var selectedphone=sppCode;
	
	if(sppId==selectedphone){
		document.getElementById("hiddenStockStatus").value = bean.status;
		if(bean.status=='INSTOCK'){
			// INFOSYS- START FT164 -  Omniture Tracking 
			omniture_store_stock = "INSTOCK";
			// INFOSYS- END FT164 -  Omniture Tracking
			
			// INFOSYS - START - 05th April NFI 44 Omniture for RnC
			omniture_store_stock_value = "INSTOCK";
			rnc_store_stock_value = "INSTOCK";
			// INFOSYS - END - 05th April NFI 44 Omniture for RnC
			if(bean.responseFromBranch100){
				sppIdDiv.innerHTML='In Stock'+ ' ' + '*';
				toolTipHdr.innerHTML = 'In Stock'+ ' ' + '*';
			}	
			else{
				sppIdDiv.innerHTML='In Stock';
				toolTipHdr.innerHTML = 'In Stock';
			}	
			if(rnc == 'T'){
				sppIdDiv.setAttribute("className" , "RCinStockTxtRC");
				sppIdDiv.setAttribute("class" , "RCinStockTxtRC");
				document.getElementById("RnCstock").setAttribute("class" , "RCinStockTxtRC");
				document.getElementById("RnCstock").setAttribute("className" , "RCinStockTxtRC");
				//document.getElementById("RnCstock").innerHTML = 'Available';
				jQuery('div#RnCstock').html('Available');
				document.getElementById("RnCstock1").setAttribute("class" , "RCinStockTxtRC");
				document.getElementById("RnCstock1").setAttribute("className" , "RCinStockTxtRC");
				//document.getElementById("RnCstock1").innerHTML = 'Available';
				jQuery('span#RnCstock1').html('Available');
				document.getElementById("RCstore").style.width='130px';
				if(rnc=='T' && payg!='T'){
					jQuery('div#resultDiv a').each(function(){
						if(jQuery(this).attr('class')=="RCinStockTxtRC"){
							jQuery(this).css('background','url("http://media.phonehouse.com/cpw-sales/static/images/right_icon.png") no-repeat scroll 93% 46% transparent');
							jQuery(this).css('margin','18px 5px 0 0');
						}
						
					});
				}

			}
			else{
				sppIdDiv.setAttribute("className" , "RCinStockTxt");
				sppIdDiv.setAttribute("class" , "RCinStockTxt");
				document.getElementById("RnCstock").setAttribute("class" , "RCinStockTxt");
				document.getElementById("RnCstock").setAttribute("className" , "RCinStockTxt");
				//document.getElementById("RnCstock").innerHTML = 'Available';
				jQuery('div#RnCstock').html('Available');
				document.getElementById("RnCstock1").setAttribute("class" , "RCinStockTxt");
				document.getElementById("RnCstock1").setAttribute("className" , "RCinStockTxt");
				//document.getElementById("RnCstock1").innerHTML = 'Available';
				jQuery('span#RnCstock1').html('Available');
				document.getElementById("RCstore").style.width='130px';
			}
			var addToBasketBtn = document.getElementById("addToBasketDiv");
			var errMsgTxt = document.getElementById("errMsg");
			if(rnc == 'T'){
				if(addToBasketBtn.style.display=='none'){
					addToBasketBtn.style.display='block';
					resetstat='NOTAVAILABLE';
				}
				if(errMsgTxt.style.display=='block'){
					errMsgTxt.style.display='none';
				}
				document.getElementById("StoreBox").style.display='none';
			}
			sppIdDiv.title = 'In Stock';
			sppIdDiv.alt = 'In Stock';
			toolTipHdr.setAttribute("className" , "inStockHdr");
			toolTipHdr.setAttribute("class" , "inStockHdr");
			toolTipContent.innerHTML = 'Your item is in stock and available to buy online now.';
			statusExist = true;
		}
		else {
			if(bean.status=='LIMITEDSTOCK'){

				// INFOSYS- START FT164 -  Omniture Tracking 
				omniture_store_stock = "LIMITEDSTOCK";
				// INFOSYS- END FT164 -  Omniture Tracking
				
				// INFOSYS - START - 05th April NFI 44 Omniture for RnC
				omniture_store_stock_value = "LIMITEDSTOCK";
				rnc_store_stock_value = "LIMITEDSTOCK";
				// INFOSYS - END - 05th April NFI 44 Omniture for RnC 
				if(bean.responseFromBranch100){
					sppIdDiv.innerHTML='In stock'+ ' ' + '*';
					toolTipHdr.innerHTML = 'In stock'+ ' ' + '*';
				}	
				else{
					sppIdDiv.innerHTML='In stock';
					toolTipHdr.innerHTML = 'In stock';
				}
				if(rnc == 'T'){
					sppIdDiv.setAttribute("className" , "RClimitedStockTxtRC");
					sppIdDiv.setAttribute("class" , "RClimitedStockTxtRC");
					document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxtRC");
					document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxtRC");
					document.getElementById("RnCstock").style.align=='right';
					//document.getElementById("RnCstock").innerHTML = 'Out of stock';
					jQuery('div#RnCstock').html('Out of stock');
					document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxtRC");
					document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxtRC");
					//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
					jQuery('span#RnCstock1').html('Out of stock');
					document.getElementById("RCstore").style.width='154px';
					
				}else{
				sppIdDiv.setAttribute("className" , "RClimitedStockTxt");
				sppIdDiv.setAttribute("class" , "RClimitedStockTxt");
				document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxt");
				document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxt");
				document.getElementById("RnCstock").style.align=='right';
				//document.getElementById("RnCstock").innerHTML = 'Out of stock';
				jQuery('div#RnCstock').html('Out of stock');
				document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxt");
				document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxt");
				//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
				jQuery('span#RnCstock1').html('Out of stock');
				document.getElementById("RCstore").style.width='154px';
				}
				var addToBasketBtn = document.getElementById("addToBasketDiv");
				var errMsgTxt = document.getElementById("errMsg");
				if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
				    resetstat='AVAILABLE';
				}
				if(errMsgTxt.style.display=='block'){
					resetstat='NOTAVAILABLE';
				}
				if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
					resetstat='NOTAVAILABLE';
					errMsgTxt.style.display=='block';
				}
					
				sppIdDiv.title = 'In stock';
				sppIdDiv.alt = 'In stock';
				toolTipHdr.setAttribute("className" , "limitedStockHdr");
				toolTipHdr.setAttribute("class" , "limitedStockHdr");
				toolTipContent.innerHTML = 'Your item is in stock and available to buy online now.';
				statusExist = true;
			}
			else{
				if(bean.status=='OUTOFSTOCK'){
					// INFOSYS- START FT164 -  Omniture Tracking 
					omniture_store_stock = "OUTOFSTOCK";
					
					// INFOSYS- END FT164 -  Omniture Tracking 
					
					//INFOSYS - START - 05th April NFI 44 Omniture for RnC
					omniture_store_stock_value = "OUTOFSTOCK";
					rnc_store_stock_value = "OUTOFSTOCK";						
					//INFOSYS - END - 05th April NFI 44 Omniture for RnC
					
					if(bean.responseFromBranch100){
						sppIdDiv.innerHTML='Out of Stock'+ ' ' + '*';
						toolTipHdr.innerHTML = 'Out of Stock'+ ' ' + '*';
					}	
					else{
						sppIdDiv.innerHTML='Out of Stock';
						toolTipHdr.innerHTML = 'Out of Stock';
					}
					if(rnc=='T'){
						sppIdDiv.setAttribute("className" , "RCoutStockTxtRC");
						sppIdDiv.setAttribute("class" , "RCoutStockTxtRC");
						
						document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxtRC");
						document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxtRC");
						
						//document.getElementById("RnCstock").innerHTML = 'Out of stock';
						jQuery('div#RnCstock').html('Out of stock');
						
						document.getElementById("RnCstock").style.align=='right';
						
						document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxtRC");
						document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxtRC");
						//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
						jQuery('span#RnCstock1').html('Out of stock');
						
						document.getElementById("RCstore").style.width='154px';
					}else{	
					sppIdDiv.setAttribute("className" , "RCoutStockTxt");
					sppIdDiv.setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxt");
					//document.getElementById("RnCstock").innerHTML = 'Out of stock';
					jQuery('div#RnCstock').html('Out of stock');
					document.getElementById("RnCstock").style.align=='right';
					document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxt");
					//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
					jQuery('span#RnCstock1').html('Out of stock');
					document.getElementById("RCstore").style.width='154px';
					}
					
					var addToBasketBtn = document.getElementById("addToBasketDiv");
					var errMsgTxt = document.getElementById("errMsg");
					if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
					    resetstat='AVAILABLE';
					}
					if(errMsgTxt.style.display=='block'){
						resetstat='NOTAVAILABLE';
					}
					if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
						resetstat='NOTAVAILABLE';
						errMsgTxt.style.display=='block';
					}
					
					sppIdDiv.title = 'Out of Stock';
					sppIdDiv.alt = 'Out of Stock';
					toolTipHdr.setAttribute("className" , "outStockHdr");
					toolTipHdr.setAttribute("class" , "outStockHdr");
					toolTipContent.innerHTML = 'This product is currently out of stock, and not available to purchase.';
					statusExist = true;
					
				}
				else if(bean.status=="NOTFOUND")
				{

					//INFOSYS - START - 05th April NFI 44 Omniture for RnC
					omniture_store_stock_value = "NOTFOUND";
					rnc_store_stock_value = "NOTFOUND";						
					//INFOSYS - END - 05th April NFI 44 Omniture for RnC
					if(rnc=='T'){
						document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxtRC");
						document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxtRC");
						
						//document.getElementById("RnCstock").innerHTML = 'Out of stock';
						jQuery('div#RnCstock').html('Out of stock');
						document.getElementById("RnCstock").style.align=='right';
						
						document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxtRC");
						document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxtRC");
						//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
						jQuery('span#RnCstock1').html('Out of stock');
						
						document.getElementById("RCstore").style.width='154px';
					}else{
					document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxt");
					
					//document.getElementById("RnCstock").innerHTML = 'Out of stock';
					jQuery('div#RnCstock').html('Out of stock');
					document.getElementById("RnCstock").style.align=='right';
					
					document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxt");
					//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
					jQuery('span#RnCstock1').html('Out of stock');
					
					document.getElementById("RCstore").style.width='154px';
					}
					var addToBasketBtn = document.getElementById("addToBasketDiv");
					var errMsgTxt = document.getElementById("errMsg");
					if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
					    resetstat='AVAILABLE';
					}
					if(errMsgTxt.style.display=='block'){
						resetstat='NOTAVAILABLE';
					}
					if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
						resetstat='NOTAVAILABLE';
						errMsgTxt.style.display=='block';
					}

					//Fix for issue # 76822
					toolTipHdr.innerHTML = 'Out of Stock';
					toolTipHdr.setAttribute("className" , "outStockHdr");
					toolTipHdr.setAttribute("class" , "outStockHdr");
					toolTipContent.innerHTML = 'This product is currently out of stock, and not available to purchase.';
					
				}
				else{
					// INFOSYS- START FT164 -  Omniture Tracking 
					omniture_store_stock = "NoStockStatus";
					// INFOSYS- END FT164 -  Omniture Tracking 
					//INFOSYS - START - 05th April NFI 44 Omniture for RnC 
					omniture_store_stock_value = "NoStockStatus";
					rnc_store_stock_value = "NoStockStatus";						
					///INFOSYS - END - 05th April NFI 44 Omniture for RnC
					
					sppIdDiv.innerHTML = "NoStockStatus";
					storestock=bean.status;
					var addToBasketBtn = document.getElementById("addToBasketDiv");
					var errMsgTxt = document.getElementById("errMsg");
					if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
					    resetstat='AVAILABLE';
					}
					if(errMsgTxt.style.display=='block'){
						resetstat='NOTAVAILABLE';
					}
					if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
						resetstat='NOTAVAILABLE';
						errMsgTxt.style.display=='block';
					}
					//Fix for issue # 76822
					
					toolTipHdr.innerHTML = 'Out of Stock';
					toolTipHdr.setAttribute("className" , "outStockHdr");
					toolTipHdr.setAttribute("class" , "outStockHdr");
					toolTipContent.innerHTML = 'This product is currently out of stock, and not available to purchase.';
				}
			}
		}
	}
	if(bean.salesProductPartId==sppCode){
		if(bean.status != 'OUTOFSTOCK' && bean.status != 'LIMITEDSTOCK' && bean.status != 'INSTOCK'){
			hideComponent(bean.status);
			sppIdDiv.style.display = 'none';
			updateStylesDO();
			if(rnc=='T'){
				updateOOSStylesRnC();
				if(payg==''){
					updateStylesRnC();
				}
		    }
		}
		else{
			
			sppIdDiv.style.display = '';
			showResult(bean.status);	
		}
		document.getElementById("visibleColourSpan").value = sppIdDiv.id; 
	}	
	else{
		sppIdDiv.style.display = 'none';
	}	
}
if(statusExist){
	addExtraSpaceBelow();
}
// INFOSYS - START - 05th April NFI 44 Omniture for RnC
omnitureValues();
// INFOSYS - END - 05th April NFI 44 Omniture for RnC
}
function colorSwatchPopulater(data,sppIde)
{
	
	var storeElement = document.getElementById("storeName");
	var defStore=postCode+":"+branchId;
	while (storeElement.firstChild) {
	storeElement.removeChild(storeElement.firstChild);
	}
	storeElement.appendChild(document.createTextNode(storeName));
	storeElement.alt=storeName;
	storeElement.title=storeName;

	
    
	
	
	
	

	//Fix for issue # 76822
	
	
	
	var statusExist = false;
	for(i=0;i<data.length;i++)
	{	
		var bean = data[i];
		var sppId  =  bean.salesProductPartId;
		var sppIdDiv = document.getElementById(sppId);
		var toolTipHdr = document.getElementById("tipStoreStockHdr");
		var toolTipContent = document.getElementById("tipStoreStockContent");
		var omniture_store_stock; 
		var selectedphone=sppIde;
		if(sppId==selectedphone){
			document.getElementById("hiddenStockStatus").value = bean.status;
			if(bean.status=='INSTOCK'){
				// INFOSYS- START FT164 -  Omniture Tracking 
				omniture_store_stock = "INSTOCK";
				// INFOSYS- END FT164 -  Omniture Tracking 
				if(bean.responseFromBranch100){
					sppIdDiv.innerHTML='In Stock'+ ' ' + '*';
					toolTipHdr.innerHTML = 'In Stock'+ ' ' + '*';
				}	
				else{
					sppIdDiv.innerHTML='In Stock';
					toolTipHdr.innerHTML = 'In Stock';
				}
				if(rnc=='T'){
					sppIdDiv.setAttribute("className" , "RCinStockTxtRC");
					sppIdDiv.setAttribute("class" , "RCinStockTxtRC");
					document.getElementById("RnCstock").setAttribute("class" , "RCinStockTxtRC");
					document.getElementById("RnCstock").setAttribute("className" , "RCinStockTxtRC");
					//document.getElementById("RnCstock").innerHTML = 'Available';
					//jQuery('div#RnCstock').html('Available');
					jQuery('div#RnCstock').html('In stock');
					document.getElementById("RnCstock1").setAttribute("class" , "RCinStockTxtRC");
					document.getElementById("RnCstock1").setAttribute("className" , "RCinStockTxtRC");
					//document.getElementById("RnCstock1").innerHTML = 'Available';
					//jQuery('span#RnCstock1').html('Available');
					jQuery('span#RnCstock1').html('In stock');
					document.getElementById("RCstore").style.width='130px';
				}else{	
				sppIdDiv.setAttribute("className" , "RCinStockTxt");
				sppIdDiv.setAttribute("class" , "RCinStockTxt");
				document.getElementById("RnCstock").setAttribute("class" , "RCinStockTxt");
				document.getElementById("RnCstock").setAttribute("className" , "RCinStockTxt");
				//document.getElementById("RnCstock").innerHTML = 'Available';
				//jQuery('div#RnCstock').html('Available');
				jQuery('div#RnCstock').html('In stock');
				document.getElementById("RnCstock1").setAttribute("class" , "RCinStockTxt");
				document.getElementById("RnCstock1").setAttribute("className" , "RCinStockTxt");
				//document.getElementById("RnCstock1").innerHTML = 'Available';
				//jQuery('span#RnCstock1').html('Available');
				jQuery('span#RnCstock1').html('In stock');
				document.getElementById("RCstore").style.width='130px';
				}
				var addToBasketBtn = document.getElementById("addToBasketDiv");
				var errMsgTxt = document.getElementById("errMsg");
				if(rnc == 'T'){
					if(addToBasketBtn.style.display=='none'){
						addToBasketBtn.style.display='block';
						resetstat='NOTAVAILABLE';
					}
					if(errMsgTxt.style.display=='block'){
						errMsgTxt.style.display='none';
					}
				}
				if(rnc=='F'){
					//document.getElementById("resultDiv").setAttribute("class" ,"RCrightTxt_new");
					jQuery('div#resultDiv').attr('class','RCrightTxt_new');
				}
				sppIdDiv.title = 'In Stock';
				sppIdDiv.alt = 'In Stock';
				toolTipHdr.setAttribute("className" , "inStockHdr");
				toolTipHdr.setAttribute("class" , "inStockHdr");
				toolTipContent.innerHTML = 'Your item is in stock and available to buy online now.';
				statusExist = true;
			}
			else {
				if(bean.status=='LIMITEDSTOCK'){
					
					// INFOSYS- START FT164 -  Omniture Tracking 
					omniture_store_stock = "LIMITEDSTOCK";
					// INFOSYS- END FT164 -  Omniture Tracking 
					if(bean.responseFromBranch100){
						sppIdDiv.innerHTML='In stock'+ ' ' + '*';
						toolTipHdr.innerHTML = 'In stock'+ ' ' + '*';
					}	
					else{
						sppIdDiv.innerHTML='In stock';
						toolTipHdr.innerHTML = 'In stock';
					}
					if(rnc=='T'){
						sppIdDiv.setAttribute("className" , "RClimitedStockTxtRC");
						sppIdDiv.setAttribute("class" , "RClimitedStockTxtRC");
						document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxtRC");
						document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxtRC");
						document.getElementById("RnCstock").style.align=='right';
						//document.getElementById("RnCstock").innerHTML = 'Out of stock';
						jQuery('div#RnCstock').html('Out of stock');	
						document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxtRC");
						document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxtRC");
						//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
						jQuery('span#RnCstock1').html('Out of stock');	
						document.getElementById("RCstore").style.width='154px';
					}else{
					sppIdDiv.setAttribute("className" , "RClimitedStockTxt");
					sppIdDiv.setAttribute("class" , "RClimitedStockTxt");
					document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxt");
					document.getElementById("RnCstock").style.align=='right';
					//document.getElementById("RnCstock").innerHTML = 'Out of stock';
					jQuery('div#RnCstock').html('Out of stock');
					document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxt");
					document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxt");
					//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
					jQuery('span#RnCstock1').html('Out of stock');	
					document.getElementById("RCstore").style.width='154px';
					}
					var addToBasketBtn = document.getElementById("addToBasketDiv");
					var errMsgTxt = document.getElementById("errMsg");
					if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
					    resetstat='AVAILABLE';
					}
					if(errMsgTxt.style.display=='block'){
						resetstat='NOTAVAILABLE';
					}
					if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
						resetstat='NOTAVAILABLE';
						errMsgTxt.style.display=='block';
					}
						
					sppIdDiv.title = 'In stock';
					sppIdDiv.alt = 'In stock';
					if(rnc=='F'){
						//document.getElementById("resultDiv").setAttribute("class" ,"RCrightTxt_new");
						jQuery('div#resultDiv').attr('class','RCrightTxt_new');
					}
					toolTipHdr.setAttribute("className" , "limitedStockHdr");
					toolTipHdr.setAttribute("class" , "limitedStockHdr");
					toolTipContent.innerHTML = 'Your item is in stock and available to buy online now.';
					statusExist = true;
				}
				else{
					if(bean.status=='OUTOFSTOCK'){
						// INFOSYS- START FT164 -  Omniture Tracking 
						omniture_store_stock = "OUTOFSTOCK";
						
						// INFOSYS- END FT164 -  Omniture Tracking 
						if(bean.responseFromBranch100){
							sppIdDiv.innerHTML='Out of Stock'+ ' ' + '*';
							toolTipHdr.innerHTML = 'Out of Stock'+ ' ' + '*';
						}	
						else{
							sppIdDiv.innerHTML='Out of Stock';
							toolTipHdr.innerHTML = 'Out of Stock';
						}
						if(rnc=='T'){
							sppIdDiv.setAttribute("className" , "RCoutStockTxtRC");
							sppIdDiv.setAttribute("class" , "RCoutStockTxtRC");
							
							document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxtRC");
							document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxtRC");
							
							//document.getElementById("RnCstock").innerHTML = 'Out of stock';
							jQuery('div#RnCstock').html('Out of stock');
							document.getElementById("RnCstock").style.align=='right';
							
							document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxtRC");
							document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxtRC");
							//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
							jQuery('span#RnCstock1').html('Out of stock');
							
							document.getElementById("RCstore").style.width='154px';
						}else{
						sppIdDiv.setAttribute("className" , "RCoutStockTxt");
						sppIdDiv.setAttribute("class" , "RCoutStockTxt");
						
						document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxt");
						document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxt");
						
						//document.getElementById("RnCstock").innerHTML = 'Out of stock';
						jQuery('div#RnCstock').html('Out of stock');
						document.getElementById("RnCstock").style.align=='right';
						
						document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxt");
						document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxt");
						//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
						jQuery('span#RnCstock1').html('Out of stock');
						
						document.getElementById("RCstore").style.width='154px';
						}
						
						var addToBasketBtn = document.getElementById("addToBasketDiv");
						var errMsgTxt = document.getElementById("errMsg");
						if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
						    resetstat='AVAILABLE';
						}
						if(errMsgTxt.style.display=='block'){
							resetstat='NOTAVAILABLE';
						}
						if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
							resetstat='NOTAVAILABLE';
							errMsgTxt.style.display=='block';
						}
						
						sppIdDiv.title = 'Out of Stock';
						sppIdDiv.alt = 'Out of Stock';
						if(rnc=='F'){
							//document.getElementById("resultDiv").setAttribute("class" ,"RCrightTxt_new");
							jQuery('div#resultDiv').attr('class','RCrightTxt_new');
						}
						toolTipHdr.setAttribute("className" , "outStockHdr");
						toolTipHdr.setAttribute("class" , "outStockHdr");
						toolTipContent.innerHTML = 'This product is currently out of stock, and not available to purchase.';
						statusExist = true;
						
					}
					else if(bean.status=="NOTFOUND")
					{
						if(rnc=='T'){
							document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxtRC");
							document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxtRC");
							
							//document.getElementById("RnCstock").innerHTML = 'Out of stock';
							jQuery('div#RnCstock').html('Out of stock');
							document.getElementById("RnCstock").style.align=='right';
							
							document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxtRC");
							document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxtRC");
							//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
							jQuery('span#RnCstock1').html('Out of stock');
							
							document.getElementById("RCstore").style.width='154px';
						}else{
						document.getElementById("RnCstock").setAttribute("class" , "RCoutStockTxt");
						document.getElementById("RnCstock").setAttribute("className" , "RCoutStockTxt");
						
						//document.getElementById("RnCstock").innerHTML = 'Out of stock';
						jQuery('div#RnCstock').html('Out of stock');
						document.getElementById("RnCstock").style.align=='right';
						
						document.getElementById("RnCstock1").setAttribute("class" , "RCoutStockTxt");
						document.getElementById("RnCstock1").setAttribute("className" , "RCoutStockTxt");
						//document.getElementById("RnCstock1").innerHTML = 'Out of stock';
						jQuery('span#RnCstock1').html('Out of stock');
						
						document.getElementById("RCstore").style.width='154px';
						}
						var addToBasketBtn = document.getElementById("addToBasketDiv");
						var errMsgTxt = document.getElementById("errMsg");
						if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
						    resetstat='AVAILABLE';
						}
						if(errMsgTxt.style.display=='block'){
							resetstat='NOTAVAILABLE';
						}
						if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
							resetstat='NOTAVAILABLE';
							errMsgTxt.style.display=='block';
						}
						if(rnc=='T'){
							sppIdDiv.setAttribute("className" , "unknownStockTxtRC");
							sppIdDiv.setAttribute("class" , "unknownStockTxtRC");
							sppIdDiv.style.padding="3px";
						}else{
						sppIdDiv.setAttribute("className" , "unknownStockTxt");
						sppIdDiv.setAttribute("class" , "unknownStockTxt");
						sppIdDiv.style.padding="3px";
						}
						
						sppIdDiv.innerHTML = document.getElementById("notStockStatus").innerHTML;

						//Fix for issue # 76822
						sppIdDiv.style.color="#DD0000";
						toolTipHdr.innerHTML = 'Out of Stock';
						//if(rnc=='F'){
							//document.getElementById("resultDiv").setAttribute("class" ,"RCrightTxt_newSNF");
							//jQuery('div#resultDiv').attr('class','RCrightTxt_newSNF');
						//}
						toolTipHdr.setAttribute("className" , "outStockHdr");
						toolTipHdr.setAttribute("class" , "outStockHdr");
						toolTipContent.innerHTML = 'This product is currently out of stock, and not available to purchase.';
					}
					else{
						// INFOSYS- START FT164 -  Omniture Tracking 
						omniture_store_stock = "NoStockStatus";
						// INFOSYS- END FT164 -  Omniture Tracking 
						if(rnc=='T'){
							sppIdDiv.innerHTML = document.getElementById("noStockStatusRC").innerHTML;
						}
						sppIdDiv.innerHTML = document.getElementById("noStockStatus").innerHTML;
						storestock=bean.status;
						var addToBasketBtn = document.getElementById("addToBasketDiv");
						var errMsgTxt = document.getElementById("errMsg");
						if(addToBasketBtn.style.display=='block'&& errMsgTxt.style.display=='none'){
						    resetstat='AVAILABLE';
						}
						if(errMsgTxt.style.display=='block'){
							resetstat='NOTAVAILABLE';
						}
						if(addToBasketBtn.style.display=='none' && errMsgTxt.style.display=='none'){
							resetstat='NOTAVAILABLE';
							errMsgTxt.style.display=='block';
						}
						//Fix for issue # 76822
						sppIdDiv.style.color="#DD0000";
						
						
						if(rnc=='T'){
							sppIdDiv.setAttribute("className" , "unknownStockTxtRC");
							sppIdDiv.setAttribute("class" , "unknownStockTxtRC");
						}else{
						sppIdDiv.setAttribute("className" , "unknownStockTxt");
						sppIdDiv.setAttribute("class" , "unknownStockTxt");
						}
						
						
						toolTipHdr.innerHTML = 'Out of Stock';
						if(rnc=='F'){
							//document.getElementById("resultDiv").setAttribute("class" ,"RCrightTxt_newOOS");
							jQuery('div#resultDiv').attr('class','RCrightTxt_newOOS');
						}
						toolTipHdr.setAttribute("className" , "outStockHdr");
						toolTipHdr.setAttribute("class" , "outStockHdr");
						toolTipContent.innerHTML = 'This product is currently out of stock, and not available to purchase.';
					}
				}
			}
		}
		if(bean.salesProductPartId==sppIde){
			if(bean.status != 'OUTOFSTOCK' && bean.status != 'LIMITEDSTOCK' && bean.status != 'INSTOCK'){
				hideComponent(bean.status);
				sppIdDiv.style.display = 'none';
				updateStylesDO();
				if(rnc=='T'){
					jQuery('span#labelavailable').each(function(){
						if(jQuery(this).attr('class')=="RCbackOrderStockTxtRC"){
							jQuery(this).css('margin','11px 8px 0');
							jQuery(this).css('background','url("http://media.phonehouse.com/cpw-sales/static/images/right_icon.png") no-repeat scroll 95% 49% transparent');
						}
					});
					updateOOSStylesRnC();
					if(payg==''){
						updateStylesRnC();
					}
			    }
			}
			else{
				
				sppIdDiv.style.display = '';
				showResult(bean.status);	
			}
			document.getElementById("visibleColourSpan").value = sppIdDiv.id; 
		}	
		else{
			sppIdDiv.style.display = 'none';
		}	
	}
	if(statusExist){
		addExtraSpaceBelow();
	}

	// INFOSYS - START - 05th April NFI 44 Omniture for RnC>
	//omnitureValues();
	// INFOSYS - END - 05th April NFI 44 Omniture for RnC
}

function clearText(){
	var text = document.getElementById('postCode').value;
	if(text.indexOf('Postcode or area')>=0){
		 document.getElementById('postCode').value = '';
	}		
}

function showComponent(){
	if(rnc=='T'){
		document.getElementById("noStockStatusRC").style.display='none';
		document.getElementById("notStockStatusRC").style.display='none';
	}
	document.getElementById("noStockStatus").style.display='none';
	document.getElementById("notStockStatus").style.display='none';
	document.getElementById("resultDiv").style.display='';
	if(isupgrade=='false'){
		
		document.getElementById("header1").style.display='none';
		document.getElementById("header2").style.display='';
		if(rnc=='T'){
			document.getElementById("header1").style.display='';
			document.getElementById("header2").style.display='none';
			document.getElementById("title").style.display='none';
		}
		else{
			document.getElementById("header1").style.display='none';
			document.getElementById("header2").style.display='';
			document.getElementById("title_RC").style.display='none';
		}
		if(storestock==""){
			document.getElementById("info1").style.display='';
			document.getElementById("info2").style.display='none';
			document.getElementById("info3").style.display='none';
			document.getElementById("info4").style.display='none';
		}else{
			if(payg=='T'){
				document.getElementById("info3").style.display='';
				document.getElementById("info2").style.display='none';
				document.getElementById("info1").style.display='none';
				document.getElementById("info4").style.display='none';
			}else{
				document.getElementById("info3").style.display='none';
				document.getElementById("info1").style.display='none';
				document.getElementById("info2").style.display='';
				document.getElementById("info4").style.display='none';
			}
		}
	}else{
		document.getElementById("header1").style.display='';
		document.getElementById("header2").style.display='none';
	}
	
	document.getElementById("StoreBox").colSpan="3";
	document.getElementById("rncBox").colSpan="3";
	document.getElementById("d2sBox").colSpan="3";
	 
}
function hideComponent(storestock){
	
	
	//document.getElementById('viewMore').style.display = 'block';
	//document.getElementById('resetStore').style.display = 'block';
	document.getElementById("StoreBox").colSpan="3";
	document.getElementById("rncBox").colSpan="3";
	//document.getElementById("d2sBox").colSpan="3";
	document.getElementById("primaryStoreInfo").style.display='';
	document.getElementById("viewStores").style.display='';
	document.getElementById("resetStores").style.display='';
	
	document.getElementById("resultDiv").style.display='none';
	document.getElementById('goButton').style.display = 'none';
	
	document.getElementById("loadingImg").style.display='none';
	document.getElementById("postCode").style.display='none';
	document.getElementById("subjectTo").style.display='none';
	document.getElementById("searchDiv").style.display='none';
	if(rnc=='T'){
		document.getElementById("title").style.display='none';
	}
	else{
		document.getElementById("title_RC").style.display='none';
	}
	document.getElementById("locateStoreInfo").style.display='none';
	document.getElementById("info1").style.display='none';
	if(rnc=='T'){
		if(storestock=="NOTFOUND")
		{
			document.getElementById("notStockStatusRC").style.display='';
		}
		else
		{
			document.getElementById("noStockStatusRC").style.display='';
		}
	}else{
	if(storestock=="NOTFOUND")
	{
		document.getElementById("notStockStatus").style.display='';
	}
	else
	{
		document.getElementById("noStockStatus").style.display='';
	}
	}
	
	if(isupgrade=='false'){
		
		document.getElementById("header1").style.display='none';
		document.getElementById("header2").style.display='';
		if(rnc=='T'){
			document.getElementById("header1").style.display='';
			document.getElementById("header2").style.display='none';
			document.getElementById("title").style.display='none';
		}
		else{
			document.getElementById("header1").style.display='none';
			document.getElementById("header2").style.display='';
			document.getElementById("title_RC").style.display='none';
		}
		if(storestock==""){
			document.getElementById("info1").style.display='';
			document.getElementById("info2").style.display='none';
			document.getElementById("info3").style.display='none';
			document.getElementById("info4").style.display='none';
		}else{
			if(payg=='T'){
				document.getElementById("info3").style.display='';
				document.getElementById("info2").style.display='none';
				document.getElementById("info1").style.display='none';
				document.getElementById("info4").style.display='none';
			}else{
				document.getElementById("info3").style.display='none';
				document.getElementById("info1").style.display='none';
				document.getElementById("info2").style.display='';
				document.getElementById("info4").style.display='none';
			}
		}
	}else{
		document.getElementById("header1").style.display='';
		document.getElementById("header2").style.display='none';
		
	}
	
		
		
}

function displayStorePopup(storeLocator, anchorId)
{
	var sppCpwcode = (document.getElementById('sppCpwCode').value).replace(new RegExp("#","g"), "%23");
	var spCpwcode = (document.getElementById('spCpwCode').value).replace(new RegExp("#","g"), "%23");
	
	if(storeLocator != null){
		document.getElementById(anchorId).href = '/layout/cpw/store_locator/store_locator_popup.do?spCpwCode='
			+ spCpwcode +'&selectedSppCode='+ sppCpwcode;
	}
	else{
		document.getElementById(anchorId).href = '/layout/cpw/store_locator/store_locator_popup.do?spCpwCode='
			+ spCpwcode +'&selectedSppCode='+ sppCpwcode +'&dealSummaryPostCode='+postCode;
	}
}

function showPostCodeSearch()
{
	
	document.getElementById("header1").style.display='';
	document.getElementById("header2").style.display='none';
	document.getElementById("StoreBox").colSpan="1";
	document.getElementById("rncBox").colSpan="1";
	document.getElementById("d2sBox").colSpan="1";
	document.getElementById("searchDiv").style.display='block';
	if(rnc=='T'){
	document.getElementById("postCodeErrorMessage1").style.display='none';
	}
	document.getElementById("postCodeErrorMessage").style.display='none';
	document.getElementById("info1").style.display='none';
	document.getElementById("goButton").style.display='block';
	document.getElementById("postCode").style.display='block';
	document.getElementById("subjectTo").style.display='block';
	var addToBasketBtn = document.getElementById("addToBasketDiv");
	var errMsgTxt = document.getElementById("errMsg");
	if(resetstat=='NOTAVAILABLE'){
		addToBasketBtn.style.display='none';
		errMsgTxt.style.display='block';
		}else{
			addToBasketBtn.style.display='block';
			errMsgTxt.style.display='none';			
		}
	if(payg=='T'){
		document.getElementById("info3").style.display='none';
		document.getElementById("info2").style.display='none';
		document.getElementById("info1").style.display='';
		document.getElementById("info4").style.display='none';
	}else{
		document.getElementById("info3").style.display='none';
		document.getElementById("info1").style.display='';
		document.getElementById("info2").style.display='none';
		document.getElementById("info4").style.display='none';
	}
	document.getElementById("resultDiv").style.display='none';
	document.getElementById("loadingImg").style.display='none';
	if(rnc=='T'){
		document.getElementById("noStockStatusRC").style.display='none';
		document.getElementById("notStockStatusRC").style.display='none';
	}
	document.getElementById("noStockStatus").style.display='none';
	document.getElementById("notStockStatus").style.display='none';
	document.getElementById("locateStoreInfo").style.display='block';
	document.getElementById("primaryStoreInfo").style.display='none';
	document.getElementById("viewStores").style.display='none';
	document.getElementById("resetStores").style.display='none';
	
}

/**
 * Function to detect enter key on the form input field
 */
function stopEnterKey(evt)
{
	// Check the event object
	var evt = (evt) ? evt : ((event) ? event : null);
	
	if (evt)
	{
	    // Get targeted node (FireFox) or srcElement (IE)
	        var node = (evt.target) ? evt.target
	                : ((evt.srcElement) ? evt.srcElement : null);
	        // Only target the text input area
	        if (node && (evt.keyCode == 13) && (node.type == "text"))
	        {
	        	doPostCodeSearch();
	                // Enter not allowed
	            return false;
	            
	        }
	}
}
//Document key press event to filter out the enter key event on html form
document.onkeypress = stopEnterKey;

function getStoreStockForColours(sppId){
	var visibleSpan = document.getElementById("visibleColourSpan").value;
	var buttoncheck=document.getElementById("addToBasketDiv").style.display;
	if(buttoncheck==='none'){
		resetstat='NOTAVAILABLE';
		}else{
			resetstat='AVAILABLE';
		}
	colorSwatchPopulater(stockList,sppId);
	
	if(document.getElementById(sppId).lastChild.nodeValue!='NoStockStatus' || document.getElementById(sppId).lastChild.nodeValue!='NotStockStatus'){
		showComponent();
		var sppIdDiv = document.getElementById(sppId);
		sppIdDiv.style.display='';
	}
	else{
		hideComponent();
	}
	if(visibleSpan != sppId){
		document.getElementById("visibleColourSpan").value = sppId;
		document.getElementById(visibleSpan).style.display='none';
	}
		
}


function getAspectImage(url){	
	url = decodeURIComponent(url);		
	jQuery.ajax({
	async: "false",
	type: 'POST',
	dataType: "jsonp",
	cache:"false",
    url: url,
    success: function() {	
    },
    error: function() {        	
    }   	
      });
	
}
var responseData;
function s7jsonResponse(data, reqId)
{
  responseData = eval(data);      
	if(responseData.IMAGE_SET !='')
	{
		var jsonData = responseData.IMAGE_SET.split(",");
		var aspectArray =[];
		var aspectArrayTmp =[];
		for(var i=0;i<jsonData.length;i++){
					var temp = jsonData[i].toUpperCase();
					if(!(aspectArrayTmp.indexOf(temp)>-1))
					{
					aspectArrayTmp.push(temp);
					aspectArray.push(jsonData[i]);
					}
					}
		drawAspectImage(aspectArray.length,modelCode);
	}      
  if(VS_flag == false)
  {
	  getAspectImage(encodeURIComponent('http://images.carphonewarehouse.com/is/image/cpw/NEW-ASUS-NEXUS-7-WIFI-16GB__VS?req=imageset,json'));
	  VS_flag=true;
  } 
}


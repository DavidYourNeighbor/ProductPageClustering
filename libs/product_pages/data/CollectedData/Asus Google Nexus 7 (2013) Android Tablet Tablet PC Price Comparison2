jQuery(document).ready(function() {
	setInterval(function(){ 
        setFooterBG('cpwProdTopSctnBg'); 
        },500); 
	
	function setFooterBG(divId) 
	{ 
		
		var pageWid = jQuery(window).width(); 
		var leftMargin = (pageWid - 1001)/2; 
		if(pageWid  >  1001) { 
			jQuery('.'+divId).css ({'width':pageWid}); 
			jQuery('.'+divId).css ({'margin-left':-leftMargin}); 
		} 
		else{ 
			jQuery('.'+divId).css ({'width':''}); 
			jQuery('.'+divId).css ({'margin-left':''}); 
		} 
	}
	
});
</script><script type="text/javascript">

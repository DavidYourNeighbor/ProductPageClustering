function mapLoading() {
	if(sessionKey == ''){
		 mapDiv = document.getElementById('myMap');
		 map = new Microsoft.Maps.Map(mapDiv,{credentials:queryKey});
	}
		 map.getCredentials(function(credentials) {
			if(credentials != null && credentials != ''){
			sessionKey = credentials;
			queryKey = sessionKey;
			}
		});
	}
function isValidPostCode() {
	var regExp = /^[A-Z,a-z]{1,2}[0-9][0-9A-Za-z]{0,1} {0,1}[0-9][ABDEFGHJLMNPQRSTUWXYZabdefghjlmnpqrstuwxyz]{2}$/;
	if(regExp.test(document.getElementById('postCode').value)) {
		return true;
	}
	return false;
}
function displayErrMsg(message){
	if(rnc=='T'){
		var postCodeErrorMessage = document.getElementById('postCodeErrorMessage1');
	}else{
	var postCodeErrorMessage = document.getElementById('postCodeErrorMessage');
	}
	postCodeErrorMessage.style.display = 'block';
	
	document.getElementById('header1').style.display= '';
	document.getElementById('header2').style.display= 'none';
	document.getElementById("StoreBox").colSpan="1";
	//document.getElementById("rncBox").colSpan="1";
	//document.getElementById("d2sBox").colSpan="1";
	
	 while (postCodeErrorMessage.firstChild) {
		 postCodeErrorMessage.removeChild(postCodeErrorMessage.firstChild);
	    } 
	errorMsg = message;
	if(rnc=='T'){
		document.getElementById("postCodeErrorMessage1").innerHTML = errorMsg;
	}else{
	document.getElementById("postCodeErrorMessage").innerHTML = errorMsg;
	}
}

function showLoadImg(){
	if(rnc=='T'){
		var postCodeErrorMessage = document.getElementById('postCodeErrorMessage1');
	}else{
	var postCodeErrorMessage = document.getElementById('postCodeErrorMessage');
	}
	var goButton = document.getElementById('goButton');
	var loadingImg = document.getElementById('loadingImg');
	goButton.style.display = 'block';
	loadingImg.style.display = 'block';
	postCodeErrorMessage.style.display = 'none';
}

function showResult(storestock){
	
	var resultDiv = document.getElementById('resultDiv');
	resultDiv.style.display = '';
	document.getElementById('goButton').style.display = 'none';
	document.getElementById('loadingImg').style.display = 'none';
	if(rnc=='T'){
		document.getElementById("noStockStatusRC").style.display='none';
	}
	document.getElementById("noStockStatus").style.display='none';
	document.getElementById('locateStoreInfo').style.display = 'none';
	document.getElementById("searchDiv").style.display='none';
	document.getElementById("postCode").style.display='none';
	
	if(isupgrade=='false'){
		
		if(rnc=='T'){
			document.getElementById("header1").style.display='';
			document.getElementById("header2").style.display='none';
			document.getElementById("title").style.display='none';
		}
		else{
			document.getElementById("header1").style.display='none';
			document.getElementById("header2").style.display='';
			document.getElementById("title_RC").style.display='none';
		}
		if(storestock==""){
			document.getElementById("info1").style.display='';
			document.getElementById("info2").style.display='none';
			document.getElementById("info3").style.display='none';
			document.getElementById("info4").style.display='none';
		}else{
			if(payg=='T'){
				document.getElementById("info3").style.display='';
				document.getElementById("info2").style.display='none';
				document.getElementById("info1").style.display='none';
				document.getElementById("info4").style.display='none';
			}else{
				document.getElementById("info3").style.display='none';
				document.getElementById("info1").style.display='none';
				document.getElementById("info2").style.display='';
				document.getElementById("info4").style.display='none';
			}
		}
	}else{
		document.getElementById("header1").style.display='';
		document.getElementById("header2").style.display='none';
	}
	if(rnc=='T'){
	document.getElementById("postCodeErrorMessage1").style.display='none';
	}
	document.getElementById("postCodeErrorMessage").style.display='none';
	document.getElementById("StoreBox").colSpan="3";
	document.getElementById("rncBox").colSpan="3";
	document.getElementById("d2sBox").colSpan="3";
	document.getElementById("primaryStoreInfo").style.display='';
	document.getElementById("viewStores").style.display='';
	document.getElementById("resetStores").style.display='';
	
	
	
	updateStylesDO();
	if(rnc=='T'){
		updateOOSStylesRnC();
		if(payg==''){
			updateStylesRnC();
		}
    }
	  

}

function addExtraSpaceBelow(){
	var extraSpace = document.getElementById('extraSpace');		//div present in dealSummary.jsp for extra space
	if(extraSpace!=null){
		extraSpace.style.display = 'block';
	}
}
/* START Infosys - Bing stor locator changes */
function resultsLoaded (jsonResponse) {	
	
	if (typeof jsonResponse === "undefined") {		
		displayErrMsg('No results found');		
    	return;
    }
	
    
    if ((typeof jsonResponse.d === "undefined") || 
	    (typeof jsonResponse.d.results === "undefined") || 
		(typeof jsonResponse.d.results.length === "undefined") || 
		(jsonResponse.d.results.length == 0)) {		
    	displayErrMsg('No results found');		
    	return;
	}
    
    var results = jsonResponse.d.results; 	  
    if (results.length > 0) {        
       record = results[0];
       storeName = document.createTextNode(record.name).nodeValue;
       branchId = document.createTextNode(record.branch_id).nodeValue;
       postCode = document.createTextNode(record.PostalCode).nodeValue;
/* END Infosys - Bing store locator chagnes */
    	//START - INFOSYS - 12-08-2011 - NFI-67 - Reserve and Collect Changes 
		//Pre-populate store post code
       temp = document.getElementById('postCode').value;
       cookieVal = postCode+":"+branchId+":"+temp+":"+storeName;
     	//END - INFOSYS - 12-08-2011 - NFI-67 - Reserve and Collect Changes 
		
       setCookieForStore("defaultstore.cpw", cookieVal, 30);
       showLoadImg();
		
       
       if(branchIdFromPopup.length > 0){           
    	   branchId = branchIdFromPopup;
    	   branchIdFromPopup = '';
       }
       storeStockDWRService.findStore(branchId, spCpwCode, getCmsValues(), answer);
       
			
		
    } else {
    	       
        displayErrMsg('No results found');
    }
    
}

function doPostCodeSearch(){
	sppCode = document.getElementById('selectedSppCpwCode').value;
	spCpwCode = document.getElementById('spCpwCode').value;
	
	//START - INFOSYS - 16-08-2011 - NFI-67 - Reserve and Collect Changes 
	//Pre-populate store post code
	storeStockDWRService.storePostCode(document.getElementById('postCode').value,callback);
	//END - INFOSYS - 16-08-2011 - NFI-67 - Reserve and Collect Changes
	 
	if (!isValidPostCode()) {
		displayErrMsg('Please enter a valid postcode');
		return;
	}
	postCode = document.getElementById('postCode').value;
	/* START Infosys - Bing Store Locator Changes */
	getStoresForPostCode(postCode,'');
	//document.getElementById('postCode').style.display='none';
	/* END Infosys - Bing store locator chagnes */
}


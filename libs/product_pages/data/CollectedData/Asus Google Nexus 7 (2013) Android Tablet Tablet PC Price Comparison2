function submitSearchForm(inputObjId){
var searchValueInputObj = document.getElementById(inputObjId).value;
//TAC-3181 Start
omniTrackClickSearch();
//TAC-3181 End
if(searchValueInputObj != 'Postcode or area'){
window.location = "/store-locator/"+searchValueInputObj;
}else{
window.location = "/store-locator/";
}
}
//TAC-3181 Start
function omniTrackClickSearch(){
	var s=s_gi(omnitureAccountName);
	s.linkTrackVars="events";
	s.linkTrackEvents="event43";
	s.events="event43";
	s.tl(true,'o','globalnav_search');

}
//TAC-3181 End



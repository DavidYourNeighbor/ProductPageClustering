$(document).ready(function() { 
setInterval(function(){ 
       setFooterBG('cpwFooterBg'); 
       },500); 

function setFooterBG(divId) 
{ 
  
 var pageWid = $(window).width(); 
 var leftMargin = (pageWid - 1001)/2; 
 if(pageWid  >  1001) { 
  $('.'+divId).css ({'width':pageWid}); 
  $('.'+divId).css ({'margin-left':-leftMargin}); 
 } 
 else{ 
  $('.'+divId).css ({'width':''}); 
  $('.'+divId).css ({'margin-left':''}); 
 } 
} 

}); 

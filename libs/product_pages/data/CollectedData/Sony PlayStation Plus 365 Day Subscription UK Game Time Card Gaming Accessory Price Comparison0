<script type="text/javascript">

	$(function(){	
	
		$('.email_stock_alert_button').live('click',function(event){
		
			var email_address_signup = $('.email_address_signup').val();
			var security_code_text = $('.security_code_text').val();
			
			// SEND AJAX REQUEST				
			$.post('http://www.simplygames.com/ajax_new.php?page=email_stock_alert', { product_id : "20267", email_address : email_address_signup, security_code : security_code_text }, function(data){
				
				if(data == 0){
				
					$('.stock_alert_error_2').hide();
					$('.stock_alert_error_1').show();
				
				}else if(data == 1){
				
					$('.stock_alert_error_1').hide();
					$('.stock_alert_error_2').show();
				
				}else{
				
					$('.stock_alert_update').show();
					$('.stock_alert_error_1').hide();
					$('.stock_alert_error_2').hide();
					$('.subscribe_form').hide();
					
				}

			});
		
			event.preventDefault();
		
		});
	
	});

</script>

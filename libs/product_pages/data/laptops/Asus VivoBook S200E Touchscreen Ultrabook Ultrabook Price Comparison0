	<script type="text/javascript">
		isKiosk = function() {
			if (document.location.href.indexOf('kiosk') > 0) {
				if (!document.documentElement.className.match(new RegExp('(\s|^)testKiosk(\s|$)'))) {
					document.documentElement.className += ' testKiosk';					
				}
				return true;
			}
			else {
				return !!navigator.userAgent.match(/(KIOSKNAME)\S/gi) && !!navigator.userAgent.match(/(STOREID)\S/gi);
			}
		}
		
		getScreenWidth = function() {
		    var x = 0;
		    if (self.innerHeight) {
				    x = self.innerWidth;
		    }
		    else if (document.documentElement && document.documentElement.clientHeight) {
				    x = document.documentElement.clientWidth;
		    }
		    else if (document.body) {
				    x = document.body.clientWidth;
		    }
		    return x;
		} 

		addToDOM = function(sRef, bAfterLoad) {
		    if (bAfterLoad) {
				$('head').append(sRef);                                                                               
		    }
		    else {
				document.write(sRef);
		    }		
		}

		window.updateStyleSheet = function(bAfterLoad) {
		    var width = getScreenWidth();
		    if (window.isKiosk()) { 
				if (!document.getElementById('kiosk-css')) {
					window.addToDOM('<link rel="stylesheet" id="kiosk-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/all-styles-kiosk-min.css?ver=1.1.5" media="screen and (min-width: 1790px)" />', bAfterLoad);							
					window.addToDOM('<link rel="stylesheet" id="publishing-kiosk-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/publishing-kiosk.css?ver=1.1.5" media="screen and (min-width: 1790px)" />', bAfterLoad);
				}
		    }                                              
		    else if (width < 600) {
				if (!document.getElementById('mobile-css')) {
					window.addToDOM('<link rel="stylesheet" id="mobile-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/all-styles-mobile-min.css?ver=1.1.5" media="screen and (max-width: 599px)" />', bAfterLoad);
					window.addToDOM('<link rel="stylesheet" id="publishing-mobile-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/publishing-mobile.css?ver=1.1.5" media="screen and (max-width: 599px)" />', bAfterLoad);
				}
		    }
		    else if (width > 599 && width < 960) {
				if (!document.getElementById('tablet-css')) {
					window.addToDOM('<link rel="stylesheet" id="tablet-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/all-styles-tablet-min.css?ver=1.1.5" media="screen and (min-width: 600px) and (max-width: 959px)"  />', bAfterLoad);						
					window.addToDOM('<link rel="stylesheet" id="publishing-tablet-vertical-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/publishing-tablet-vertical.css?ver=1.1.5" media="screen and (min-width: 600px) and (max-width: 767px)" />', bAfterLoad);						
					window.addToDOM('<link rel="stylesheet" id="publishing-tablet-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/publishing-tablet.css?ver=1.1.5" media="screen and (min-width: 768px) and (max-width: 959px)" />', bAfterLoad);
				}
		    }
		    else if (width > 959 && width < 1200) {
				if (!document.getElementById('desktop-css')) {
					window.addToDOM('<link rel="stylesheet" id="desktop-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/all-styles-desktop-min.css?ver=1.1.5" media="screen and (min-width: 960px) and (max-width: 1199px)"  />', bAfterLoad);						
					window.addToDOM('<link rel="stylesheet" id="publishing-desktop-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/publishing-desktop.css?ver=1.1.5" media="screen and (min-width: 960px) and (max-width: 1199px)" />', bAfterLoad);
				}
		    }                                              
		    else if (width > 1199 ) {
				if (!document.getElementById('large-desktop-css')) {
					window.addToDOM('<link rel="stylesheet" id="large-desktop-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/all-styles-large-desktop-min.css?ver=1.1.5" media="screen and (min-width: 1200px)"  />', bAfterLoad);
					window.addToDOM('<link rel="stylesheet" id="publishing-large-desktop-css" href="http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/css/publishing-large-desktop.css?ver=1.1.5" media="screen and (min-width: 1200px)" />', bAfterLoad);
				}
		    }                                              
		}

		// Trigger default call
		window.updateStyleSheet(false);                                           

		if (window.isKiosk()) {
			document.documentElement.className += ' kiosk';
		}		

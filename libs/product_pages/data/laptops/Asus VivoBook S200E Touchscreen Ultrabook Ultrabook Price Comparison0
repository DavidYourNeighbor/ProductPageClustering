<script type="text/javascript">var isIE7 = navigator.appVersion.indexOf('MSIE')>0 && navigator.appVersion.indexOf('MSIE 7')>0; if(!isIE7) {mboxCreate("C_Product Details_Responsive with Warranty Widget_Accessories_secondary_Buy Box","product=ASUS S200E Pentium DualCore 4GB 500GB Dark Grey Laptop","category=All Laptops","subCategory1=Computing","department=Technology  Gaming");}</script>
</div><div id="ssb_block_11" class="ssb_block"><script>

if (!window.isKiosk()) {
/**
*  SquareTrade Warranty Resale Widget Integration
*  

*/
jQuery(function () {


/* ========================== Product Page Startup ========================== */

/**
*  This list defines the Product SKUs that should prevent the warranty widget
*  from initializing.
*  
 *  TODO [Tesco] Need to determine how these SKUs are rendered to the page and
*  ensure proper re-integration into merchant site content management system.
*  
 *  @type {Array}
*/
var blockedWarrantySkus = [
        "415-9399",
        "622-3383",
        "723-3152",
        "118-1560",
        "377-3448",
        "412-7984",
        "152-7233",
        "154-5441",
        "350-7137",
        "752-1983",
        "505-6458",
        "686-3613",
        "426-4029",
        "597-1049",
        "536-4541",
        "588-7791",
        "255-1645",
        "640-7060",
        "427-6031",
       "466-6167",
        "548-8010",
        "664-9969",
        "266-7178",
        "586-0122",
        "292-9195",
        "419-0639",
        "140-0810",
        "639-9564",
        "791-8059",
        "160-7317",
        "104-5888",
        "588-7162",
        "164-6635",
        "145-5463",
        "713-8154"
    ];

// Initialization method is registered for jQuery's `document.ready` event:
$(document).ready(function () {


    // Initialize the current product sku variable:
    initializeProductSku();

    // Only continue initializing if product sku is not blocked:
    if (!isProductSkuBlocked(productSku)) {

        // For every "buy box container" element in the page:
        $('.buy-from .seller').each(function () {

            // If Tesco product only:
            var container = $(this);
            if (isTescoProductContainer(container)) {

                initializeWidgetForContainer(container, productSku);
            }
        });
    }
});

/**
*  The `productCat` variable is declared here so it is available to any of
*  the methods defined within this closure.
*  
 *  TODO [Tesco] Need to determine how this string is rendered to the page and
*  ensure proper re-integration into merchant site content management system.
*  
 *  @sample 'cat3375702'; // e.g. for television
*  @sample 'cat3376345'; // e.g. for laptop
*  
 *  @type {String}
*/

var metaVal = $('meta[name="parent_category"]').attr("content");
var productCat = metaVal;

/**
*  The `productSku` variable is declared here so it is available to any of
*  the methods defined within this closure.
*  
 *  @type {String}
*/
var productSku = '';

/**
*  This helper function safely attempts to populate the `productSku` closure
*  variable during initialization.
*/
function initializeProductSku () {

    try {
        productSku = $.urlParam ? $.urlParam('skuId') : '';
    } catch (e) {

        if (console && console.error) {
            console.error(e);
        }
    }
}

/**
*  Simple helper to determine if the current `productSku` value is in the
*  list of blocked SKUs. (The variable is accessible to any function in this
*  closure, so it does not need to be passed in to this function.)
*  
 *  @returns {Boolean}
*/
function isProductSkuBlocked () {
    return listContainsItem(blockedWarrantySkus, productSku);
}

/**
*  (This Tesco-site-specific logic has been factored into a distinct helper
*  method for clarity.)
*  
 *  @param {Element} container
*  @returns {Boolean}
*/
function isTescoProductContainer (container) {
    return (container.find(".details .header-wrapper h2 span").text() == "Tesco");
}

/**
*  By default, the "reminder" Interstitial Overlay is presented when a
*  warranty is not selected and user adds the page's product is to their
*  basket. Returning `false` from this method suppresses the decision to
*  show an Overlay in the `onFormSubmit()` method below.
*  
 *  The logic to determine whether a product might need the in-page widget
*  rendered, but should not have an Interstitial widget presented, can be
*  isolated in this function. The retailer product details are passed to
*  this function for convenience.
*  
 *  @param {Event} event
*  @param {String} productSku
*  @param {String} productCat
*  @param {Number} productPrice
*  @returns {Boolean}
*/
function shouldDisplayInterstitialOverlay (event, productSku, productCat, productPrice) {
    return false;
}

/**
*  Renders the necessary triggering elements to create the Warranty Resale
*  widget for a given product container element.
*  
 *  @param {Element} container
*  @param {String} productSku
*/
function initializeWidgetForContainer (container, productSku) {
    // `productSku` and `productCat` variables are available to this method.
    // Get product price via utility method:
    var productPrice = getProductPriceFromContainer(container);

    // Render the triggering elements for the Warranty widget:
    var markup = '<div class="st_fn_component" ' +
                        'data-component="$st.components.ProductWidget" ' +
                        'data-component-name="warranty-for-sku-' + productSku +'">' +
            '<span class="st_resale_itemCategory" style="display: none">' + productCat + '</span>' +
            '<span class="st_resale_itemPrice" style="display: none">' + productPrice + '</span>' +
            '<span class="st_resale_itemCondition" style="display: none">New</span>' +
        '</div>';

    // Defines where to place widget:
    container.find(".details").after(markup);

    //alter the add to basket form
    container.find("#addToBasket_PDP-1 input").each(function(){
        if($(this).attr("name") == "/atg/commerce/order/purchase/CartModifierFormHandler.addItemToOrderSuccessURL"){
            $(this).val("/direct/basket-details/basket-details.page")
        }
    });

    //append form to allow for submission by jquery
    container.find('#addToBasket_PDP-1').append('<input type="hidden" name="/atg/commerce/order/purchase/CartModifierFormHandler.addItemToOrder" value="Add to basket" />');

    // Register the form submission handler to determine whether the warranty
    // overlay popup should be shown when the product is added to basket:
    container.find("form").submit(function (event) {
        return onFormSubmit(event, productSku, productCat, productPrice);
    });

}

/**
*  (This Tesco-site-specific logic has been factored into a distinct helper
*  method for clarity.)
*  
 *  @param {Element} container
*  @returns {Number}
*/
function getProductPriceFromContainer (container) {
    return +($.trim(container.find(".current-price").text()).substring(1).replace(/,/g, ''));
}

/* ==================== `Add-To-Basket` Form Submission ===================== */

/**
*  This flag is updated by the 'onDeclineCoverage' widget callback handler
*  and helps determine which action to take at time of form submission.
*  
 *  @type {Boolean}
*/
var coverageDeclined = false;

/**
*  This handler is called when the product page form submission has been
*  triggered. (Typically triggered by a click of the "Add-to-basket" button
*  in the product's "Buy Box", but the form submission could also be triggered
*  in response to an event directly within the Warranty selection overlay.)
*  
 *  @param {Event} event The event triggering the form submission (if available).
*  @returns {Boolean} Whether the form should be submitted.
*/
function onFormSubmit (event) {

    if (coverageDeclined) {
        // The interstitial warranty overlay has just been declined: allow the
        // form to submit normally.
        return true;
    }

    var warrantyInputElement = $('#warranty');
    if (warrantyInputElement.length) {
        // If a `#warranty` form field is located, a warranty has already been
        // selected: allow the form to submit normally.
        return true;
    }

    // If no `#warranty` form field was located, determine if an Interstitial
    // warranty popup should be displayed.
    if (shouldDisplayInterstitialOverlay(event)) {

        // Get a reference to the Widget instance and call for it to display
        // with an Overlay presentation.
        var widget = getWarrantyResaleWidgetForSku(productSku);
        if (widget) {
            widget.preselectQuoteType('base');
            widget.startAsOverlay();

            // Prevent the form submission action.
            return false;
        }
    }

    // Allow the form to submit normally in any other condition.
    return true;
}

/* ========================== Widget Configuration ========================== */

/**
*  The configuration storage object for the SquareTrade Product Widget.
*  
 *  @namespace
*  @type {Object}
*/
window.st_resale = window.st_resale || {};

/**
*  The instance configuration defaults for the Widget.
*  
 *  @type {Object}
*/
window.st_resale.defaults = {
    warrantySelectHandlerName:    'onWarrantySelect',
    warrantyPurchaseHandlerName:  'onWarrantyPurchase',
    declineCoverageHandlerName:   'onDeclineCoverage',
    warrantySignatureHandlerName: 'onWarrantySignature',

    configFilename: 'stwidgetconfig.js',
    templateFilename: 'sttemplates.js',
    imagesDirectoryname: 'st_images'
};

/**
*  A dictionary of display strings for Warranty products.
*  
 *  @type {Object}
*/
window.st_resale.displayTable = {
    '2A': '2-Year Protection Plan',
    '3A': '3-Year Protection Plan',
    '5A': '5-Year Protection Plan'
};

/**
*  Configuration, template, and image file locations for the widget are
*  referenced from within the base path specified here.
*  
 *  @type {String}
*/
st_resale.defaults.basePath = 'http://www.tesco.com/directuiassets/SiteAssets/NonSeasonal/en_GB/warranty_interstitial/';

/* ====================== Widget Callbacks / Handlers ======================= */

/**
*  This handler is called when the selection state has changed within the
*  widget (e.g. when a checkbox element has been checked or unchecked).
*  
 *  @param {String} productID
*  @param {Number} warrantyPrice
*  @param {String} warrantyDescription
*  @param {String} catalogRefId
*  
 *  (The return value of this callback is not used.)
*/
window.onWarrantySelect = function onWarrantySelect (productID, warrantyPrice, warrantyDescription, catalogRefId) {

    //add the catalog Ref Id to the add to basket button
    var warrantyInputElement = $('#addToBasket_PDP-1 #warranty');

    if (productID) {
        // console.log('[Tesco-Sample] onWarrantySelect', productID, warrantyPrice, warrantyDescription, catalogRefId);

        if (warrantyInputElement.length > 0) {
            warrantyInputElement.val(catalogRefId);
        } else {
            $('#addToBasket_PDP-1').prepend('<input id="warranty" type="hidden" tabindex="999" value="' + catalogRefId + '" name="/atg/commerce/order/purchase/CartModifierFormHandler.catalogRefIds">');
        }
    } else {
        // console.log('[Tesco-Sample] onWarrantySelect (No warranty selected.)');

        warrantyInputElement.remove();
    }
};

/**
*  This handler is called when the user has triggered an action that should
*  purchase a warranty.
*  
 *  For this particular integration, this handler is expected to be called
*  when the `Add To Basket` button is clicked in the warranty product
*  Interstitial (reminder) Overlay.
*  
 *  @param {String} productID
*  @param {Number} warrantyPrice
*  @param {String} warrantyDescription
*  @param {String} catalogRefId
*  
 *  (The return value of this callback is not used.)
*/
window.onWarrantyPurchase = function onWarrantyPurchase (productID, warrantyPrice, warrantyDescription, catalogRefId) {
    // console.log('[Tesco-Sample] onWarrantyPurchase', productID, warrantyPrice, warrantyDescription, catalogRefId);

    $('#addToBasket_PDP-1').submit();
};

/**
*  This handler is called when the user has triggered an action to decline
*  warranty coverage.
*  
 *  (No parameters are provided to this callback.)
*  
 *  (The return value of this callback is not used.)
*/
window.onDeclineCoverage = function onDeclineCoverage () {
    // console.log('[Tesco-Sample] onDeclineCoverage');

    coverageDeclined = true;

    $('#addToBasket_PDP-1').submit();
};

/**
*  This handler is called by the Widget to customize the parameter signature
*  used when firing callbacks related to warranty products.
*  
 *  @param {$st.components.ProductWidget.WarrantyPriceQuote} quote
*  @returns {Array} An array suitable for passing as `arguments`
*/
window.onWarrantySignature = function onWarrantySignature (quote) {
    return [quote.productID, quote.price, quote.description, quote.catalogRefId];
};

/* =========== Utility functions for SquareTrade widget components ========== */

/**
*  @param {Array} list
*  @param {Object} item
*  @returns {Boolean} Whether `item` is an element of the array `list`.
*/
function listContainsItem (list, item) {
    if (!list) {
        return false;
    }
    for (var i = 0, len = list.length; i < len; i++) {
        if (list[i] === item) {
            return true;
        }
    }
    return false;
}

/**
*  @param {String} itemSku
*  @returns {$st.components.ProductWidget}
*/
function getWarrantyResaleWidgetForSku (itemSku) {
    return getComponentByControllerName('warranty-for-sku-' + itemSku);
}

/**
*  @param {String} controllerName
*  @returns {$st.components.Component}
*/
function getComponentByControllerName (controllerName) {
    var manager = $st.components.ComponentManager.getInstance(),
        components = manager.getComponents();
    for (var key in components) {
        if (components.hasOwnProperty(key)) {
            var component = components[key];
            if (component.controller && (component.controller.name === controllerName)) {
                return component;
            }
        }
    }
}


});
}
</script>



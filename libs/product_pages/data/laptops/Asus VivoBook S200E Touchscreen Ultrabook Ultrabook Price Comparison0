<script type="text/javascript" charset="utf-8">
searchquery = getParam('searchquery')
function getParam(sname) {
  var page_url = window.location.href;
  var sval = "";
  if (page_url && page_url.indexOf("?") > 0) {
    var params = page_url.substr(page_url.indexOf("?") + 1);
    if (params) {
      params = params.split("&");
      // split param and value into individual pieces
      for (var i = 0; i < params.length; i++) {
        temp = params[i].split("=");
        if ([temp[0]] == sname) {
          sval = temp[1];
        }
      }
    }
  }
  return sval;
} 
function setAdsShown(containerId, adsLoaded) {
	clearTimeout(timer);
	if (!adsLoaded) {
                //If ad not loaded
                $('#adblock1').parent(".sponsored-link-block").hide();
                $('#adblock2').parent(".sponsored-link-block").hide();			
	}
  	// Things to do when CSA has completed regardless if ads are present
  	showSearchResults();
	require(['modules/product-description/common', 'modules/common'], function(pdp, common) {
		if (common.isPage('PDP')) {
			pdp.changeColumnLayout();
		}
	});

}
function showSearchResults() {
	$('#search-results-container').css('visibility', 'visible');
}
var pageOptions = {
'pubId' : 'tesco-com-vip',
'query' : "Asus All Laptops".replace(/&amp;/g, '&'),
'linkTarget': '_blank',
'hl': 'en',
'instantPreviews' : false,              // New Feature - remove if not required
'sellerRatings' : true,                // Recommended Feature - remove if not required
'siteLinks' : true,                    // Recommended Feature - remove if not required
'domainLinkAboveDescription' : true
};
var adblock2 = {
'container' : 'adblock2',
'number' : 4,
'longerHeadlines' : true,
'fontFamily' : 'arial',
'fontSizeTitle' : '12px',
'fontSizeDescription' : '11px',
'fontSizeDomainLink' : '11px',

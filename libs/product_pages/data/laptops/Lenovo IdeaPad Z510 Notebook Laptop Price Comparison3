zone = zoneParam[1];
parentProduct = parentProductParam[1];
params = " " + zone + " - " + parentProduct;
}
window.SDCSAPageID = "Desktop | Desktop | GALLERY | Search_Failed" + params;
} 
} else {
window.SDCSAPageID = "Desktop | Desktop | GALLERY | Search_Failed";
}
}

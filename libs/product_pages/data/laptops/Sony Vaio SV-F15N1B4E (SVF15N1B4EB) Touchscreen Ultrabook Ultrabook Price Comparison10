<script type="text/javascript">

    var fontType = function(){
        if(navigator.userAgent.toLowerCase().match(/android|vita|playstation/g) !== null){
            return "ttf";
        }
        else{
            return "woff";
        }
        
    };

    
        
           document.write(unescape("%3Clink rel='stylesheet' href='//fonts.sony.net/shf/v13/fonts-latinextended-" + fontType() + ".css'%3E"));
        
    

var gcs1 = document.createElement('script')
var gcs2 = document.createElement('script')
var gcs3 = document.createElement('script')
var p = (document.location.protocol === 'https:') ? 'https://' : 'http://'
gcs1.src = p+'www.google.com/jsapi'
gcs2.src = p+'apis.google.com/js/client.js'
gcs3.src = p+'www.gstatic.com/commercesearch/sayt/sayt.js'
gcs3.onload = function() {
google.load('search', '1', { 'callback' : setOnLoadCallback })
function setOnLoadCallback() {
var sayt_params = {
country: 'gb',
language: 'uk',
extra_params: { safe: false }
}
sayt = saytInitialize('008849798255463965697:ffe6r1t4abi', 'AIzaSyBCJcY1zCutuoPbBVizPScT8IeBCkCdKR0', sayt_params)
var optOptions = { 
saytActor: sayt.saytSubmit,
maxCompletions: 5
}
google.search.CustomSearchControl.attachAutoCompletionWithOptions(
'008849798255463965697:ffe6r1t4abi', document.getElementById('search'), 'searchForm', optOptions
)
}
}
s.parentNode.insertBefore(gcs1, s)
s.parentNode.insertBefore(gcs2, s)
s.parentNode.insertBefore(gcs3, s)
})()
</script>
